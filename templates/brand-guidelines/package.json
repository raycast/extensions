{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brand-guidelines",
  "title": "Brand Guidelines",
  "description": "Your brand's colors, logos, and other assets quickly available",
  "icon": "icon.png",
  "author": "thomas",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-brand-guidelines",
      "title": "Open Brand Guidelines",
      "description": "Show your brand's colors, logos, and other assets.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "latest",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
