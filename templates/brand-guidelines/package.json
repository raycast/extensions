{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brand-guidelines",
  "title": "Brand Guidelines",
  "description": "Your brand's colors, logos, and other assets quickly available",
  "icon": "icon.png",
  "author": "thomas",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-brand-guidelines",
      "title": "Open Brand Guidelines",
      "description": "Show your brand's colors, logos, and other assets.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "latest",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
