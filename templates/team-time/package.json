{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "team-time",
  "title": "Team Time",
  "description": "Quick overview of the team's timezones",
  "icon": "icon.png",
  "author": "thomas",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Team Time",
      "description": "Shows the team's timezones as subtitle",
      "mode": "no-view",
      "interval": "1m"
    }
  ],
  "dependencies": {
    "@raycast/api": "latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
