{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pianoman",
  "title": "Pianoman",
  "description": "Show piano chords and scales",
  "icon": "pianoman.png",
  "author": "juhamust",
  "categories": [
    "Documentation",
    "Media",
    "Other"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "license": "MIT",
  "commands": [
    {
      "name": "pianoman-chord",
      "title": "Search Piano Chord",
      "description": "Discover piano chord like C#",
      "mode": "view",
      "arguments": [
        {
          "name": "rootNote",
          "placeholder": "note",
          "type": "text"
        }
      ],
      "keywords": [
        "music",
        "piano",
        "chord"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "1.93.2",
    "@raycast/utils": "^1.19.1",
    "got": "^14.4.7",
    "js-base64": "^3.7.2",
    "react": "~19.0.0",
    "react-dom": "~19.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/react-dom": "~19.0.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.0.3",
    "prettier": "^3.5.3",
    "ts-jest": "^29.3.2",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "test": "jest src",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}