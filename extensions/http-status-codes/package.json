{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "http-status-codes",
  "title": "HTTP Status Codes",
  "description": "Quickly look up HTTP status codes",
  "icon": "command-icon.png",
  "author": "Alex_",
  "contributors": [
    "pernielsentikaer"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search HTTP Status Codes",
      "description": "Search all available HTTP status codes by code or description",
      "mode": "view"
    }
  ],
  "categories": [
    "Developer Tools",
    "Data"
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^18.8.3",
    "eslint": "^7.32.0",
    "prettier": "^3.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
