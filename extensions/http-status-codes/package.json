{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "http-status-codes",
  "title": "HTTP Status Codes",
  "description": "Quickly look up HTTP status codes",
  "icon": "command-icon.png",
  "author": "Alex_",
  "contributors": [
    "pernielsentikaer",
    "lucasmezencio"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search HTTP Status Codes",
      "description": "Search all available HTTP status codes by code or description",
      "mode": "view"
    }
  ],
  "categories": [
    "Developer Tools",
    "Data"
  ],
  "dependencies": {
    "@raycast/api": "^1.74.0",
    "@raycast/utils": "^1.15.0",
    "node-fetch": "^3.3.2",
    "tempy": "^2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.12.12",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "typeOfPerson",
      "description": "Are you a cat or a dog person?",
      "type": "dropdown",
      "required": false,
      "title": "Are you a cat or a dog person?",
      "placeholder": "placeHolder",
      "default": "none",
      "data": [
        {
          "title": "None",
          "value": "none"
        },
        {
          "title": "üêà Cat",
          "value": "cat"
        },
        {
          "title": "üêï Dog",
          "value": "dog"
        }
      ]
    }
  ]
}
