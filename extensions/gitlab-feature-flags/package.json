{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gitlab-feature-flags",
  "title": "GitLab Feature Flags",
  "description": "Searchable feature flags in GitLab",
  "icon": "command-icon.png",
  "author": "samdbeckham",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "GitLab Feature Flags",
      "description": "Show GitLab Feature Flags",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.2",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
