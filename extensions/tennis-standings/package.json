{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tennis-standings",
  "title": "Tennis Standings",
  "description": "Get the ATP and WTA rankings",
  "icon": "command-icon.png",
  "author": "Zalgo_123",
  "contributors": [
    "xilopaint"
  ],
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "atp-rankings",
      "title": "ATP Rankings",
      "description": "Get the ATP rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    },
    {
      "name": "wta-rankings",
      "title": "WTA Rankings",
      "description": "Get the WTA rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.14.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
