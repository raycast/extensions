{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tennis-standings",
  "title": "Tennis Standings",
  "description": "Get the ATP and WTA rankings",
  "icon": "command-icon.png",
  "author": "Zalgo_123",
  "contributors": [
    "xilopaint",
    "drillprop"
  ],
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "atp-live-rankings",
      "title": "ATP Live Rankings",
      "description": "Get the ATP rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    },
    {
      "name": "wta-live-rankings",
      "title": "WTA Live Rankings",
      "description": "Get the WTA rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    },
    {
      "name": "atp-race",
      "title": "ATP Race Rankings",
      "description": "Get the ATP race rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    },
    {
      "name": "wta-race",
      "title": "WTA Race Rankings",
      "description": "Get the WTA race rankings",
      "mode": "view",
      "keywords": [
        "tennis"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.73.2",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
