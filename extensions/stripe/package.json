  "name": "stripe",
  "title": "Stripe",
  "description": "This extension allows you to make an invoice, create a payment, add a new product, and start a new subscription.",
  "icon": "stripe.png",
  "author": "pradeepb28",
  "license": "MIT",
  "commands": [
    {
      "name": "create-a-new-invoice",
      "title": "Create a new invoice",
      "subtitle": "Stripe",
      "description": "",
      "mode": "view"
    },
    {
      "name": "create-a-new-payment",
      "title": "Create a new payment",
      "subtitle": "Stripe",
      "description": "",
