{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "windmill",
  "title": "Windmill",
  "description": "Windmill Extension to run workflows directly from Raycast",
  "icon": "windmill-logo.png",
  "author": "emiliobool",
  "repository": {
    "type": "git",
    "url": "https://github.com/emiliobool/windmill_raycast_extension.git"
  },
  
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "viewFlows",
      "title": "View Flows",
      "description": "List Windmill Flows for the added workspaces",
      "mode": "view"
    },
    {
      "name": "viewScripts",
      "title": "View Scripts",
      "description": "List Windmill Scripts for the added workspaces",
      "mode": "view"
    },
    {
      "name": "viewApps",
      "title": "View Apps",
      "description": "List Windmill Apps for the added workspaces",
      "mode": "view"
    },
    {
      "name": "manageWorkspaces",
      "title": "Manage Workspaces",
      "description": "Manage your windmill workspaces",
      "mode": "view"
    }

  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.9.1",
    "node-fetch": "^3.3.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/uuid": "^9.0.2",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
