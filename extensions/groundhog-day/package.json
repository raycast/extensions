{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "groundhog-day",
  "title": "Groundhog Day",
  "description": "Free, accurate, and portable data on prognosticating groundhogs and their yearly predictions",
  "icon": "groundhog.png",
  "author": "xmok",
  "categories": [
    "Data",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "groundhogs",
      "title": "Groundhogs",
      "description": "View Groundhogs and their Predictions",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.78.0",
    "@raycast/utils": "^1.16.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
