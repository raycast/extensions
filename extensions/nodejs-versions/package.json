{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nodejs-versions",
  "title": "Node Versions Releases",
  "description": "List and detail view of Node.js releases",
  "icon": "app-icon.svg",
  "version": "1.0.0",
  "author": "fil",
  "license": "MIT",
  "keywords": [
    "node",
    "lts",
    "release",
    "js"
  ],
  "commands": [
    {
      "name": "index",
      "title": "List Node Versions",
      "description": "List and detail view of Node.js releases",
      "mode": "view",
      "keywords": [
        "node",
        "lts",
        "release",
        "latest",
        "current",
        "js"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "1.67.1",
    "@raycast/utils": "1.12.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.6",
    "@types/node": "20.11.19",
    "@types/react": "18.2.56",
    "eslint": "8.51.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
