{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "deduplicator",
  "title": "Deduplicator",
  "description": "A text tool for de-duplicating text content.",
  "icon": "command-icon.png",
  "author": "HerbertLu",
  "categories": [
    "Productivity",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "deduplicator",
      "title": "De-duplicate Text",
      "description": "De-duplicate and copy text",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
