{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tynyfy",
  "title": "Tynyfy - URL Shortener",
  "description": "Shortens your boring long URLs",
  "icon": "command-icon.png",
  "author": "kusthi",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "tynyfy",
      "title": "Tynyfy",
      "description": "Shortener URL from Clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.0",
    "is-url": "^1.2.4",
    "node-fetch": "^3.3.1",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/is-url": "^1.2.30",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/react-dom": "^18.2.6",
    "eslint": "^7.32.0",
    "prettier": "2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
