{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ramda-documentation",
  "title": "Ramda Documentation",
  "description": "Offers quick and filterable Ramda documentation results.",
  "icon": "command-icon.png",
  "author": "jbroomer",
  "categories": [
    "Documentation",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Ramda Documentation",
      "description": "Shows any Ramda functions matching the input",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.1",
    "@raycast/utils": "^1.15.0",
    "node-html-parser": "^6.1.5",
    "turndown": "^7.2.0"
  },
  "devDependencies": {
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
