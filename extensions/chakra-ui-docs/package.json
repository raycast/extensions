{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chakra-ui-docs",
  "title": "Chakra UI Documentation",
  "description": "Provide quick links to the Chakra UI component documentation.",
  "icon": "chakra.png",
  "author": "pgvr",
  "contributors": [
    "Ansub",
    "stanley_ding"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "get-started",
      "title": "Get Started",
      "subtitle": "Chakra UI",
      "keywords": [
        "get started"
      ],
      "description": "Get Started with Chakra UI",
      "mode": "view"
    },
    {
      "name": "styled-system",
      "title": "Search Styled System",
      "subtitle": "Chakra UI",
      "keywords": [
        "styled system"
      ],
      "description": "Styled System of Chakra UI",
      "mode": "view"
    },
    {
      "name": "components",
      "title": "Search Components",
      "subtitle": "Chakra UI",
      "keywords": [
        "components"
      ],
      "description": "Components of Chakra UI",
      "mode": "view"
    },
    {
      "name": "hooks",
      "title": "Search Hooks",
      "subtitle": "Chakra UI",
      "keywords": [
        "hooks"
      ],
      "description": "Hooks of Chakra UI",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.77.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^18.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "axios": "^1.7.2",
    "cheerio": "^1.0.0-rc.12",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.2",
    "react-devtools": "^5.2.0",
    "typescript": "^5.5.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "doc:update-component": "npx tsx utils/updateComponentsDoc.ts && ray lint --fix"
  }
}
