{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chakra-ui-docs",
  "title": "Chakra UI Documentation",
  "description": "Provide quick links to the Chakra UI component documentation.",
  "icon": "chakra.png",
  "author": "pgvr",
  "contributors": [
    "Ansub"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "get-started",
      "title": "Get Started",
      "subtitle": "Chakra UI",
      "keywords": [
        "get started"
      ],
      "description": "Get Started with Chakra UI",
      "mode": "view"
    },
    {
      "name": "styled-system",
      "title": "Search Styled System",
      "subtitle": "Chakra UI",
      "keywords": [
        "styled system"
      ],
      "description": "Styled System of Chakra UI",
      "mode": "view"
    },
    {
      "name": "components",
      "title": "Search Components",
      "subtitle": "Chakra UI",
      "keywords": [
        "components"
      ],
      "description": "Components of Chakra UI",
      "mode": "view"
    },
    {
      "name": "hooks",
      "title": "Search Hooks",
      "subtitle": "Chakra UI",
      "keywords": [
        "hooks"
      ],
      "description": "Hooks of Chakra UI",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
