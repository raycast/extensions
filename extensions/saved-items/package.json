{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "saved-items",
  "title": "Saved Items",
  "description": "Save some items (eg. quick replies) and insert or copy them.",
  "icon": "saved-items.png",
  "author": "guoxudong",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Saved Items",
      "description": "Show a list of all saved items.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.1",
    "nanoid": "^5.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
