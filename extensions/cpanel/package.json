{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cpanel",
  "title": "cPanel",
  "description": "",
  "icon": "command-icon.png",
  "author": "xmok",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "cpanel_url",
      "title": "cPanel URL w/ Port",
      "description": "",
      "type": "textfield",
      "placeholder": "https://example.com:2083",
      "required": true
    },
    {
      "name": "cpanel_username",
      "title": "cPanel Username",
      "description": "",
      "type": "textfield",
      "placeholder": "user",
      "required": true
    },
    {
      "name": "api_token",
      "title": "cPanel API Token",
      "description": "",
      "type": "password",
      "placeholder": "U7HMR63FHY282DQZ4H5BIH16JLYSO01M",
      "required": true
    }
  ],
  "commands": [
    {
      "name": "email-accounts",
      "title": "Email Accounts",
      "description": "",
      "mode": "view"
    },
    {
      "name": "domains",
      "title": "Domains",
      "description": "",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
