{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lz-string",
  "title": "LZ-String",
  "description": "Encode and decode strings with lz-string.",
  "icon": "icon.png",
  "author": "basil",
  "categories": [
    "Data",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "decode",
      "title": "LZ-String Decode",
      "mode": "no-view",
      "description": "Decode an lz-string encoded string."
    },
    {
      "name": "encode",
      "title": "LZ-String Encode",
      "mode": "no-view",
      "description": "Encode a string with lz-string."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "lz-string": "^1.4.4"
  },
  "devDependencies": {
    "@types/lz-string": "^1.3.34",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
