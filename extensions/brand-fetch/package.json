{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brand-fetch",
  "title": "Brandfetch",
  "description": "Fetch your Brand Images using the BrandFetch API",
  "icon": "extension_icon.png",
  "author": "dilutedev",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Fetch Brands",
      "subtitle": "Brandfetch",
      "description": "Fetch Images using BrandFetch API",
      "mode": "view",
      "preferences": [
        {
          "title": "BrandFetch API Key",
          "name": "bfApiKey",
          "description": "Key used to authenticate brand fetch api",
          "type": "password",
          "required": true
        }
      ],
      "arguments": [
        {
          "name": "domain",
          "required": true,
          "placeholder": "www.brandfetch.com",
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
