{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "soundpick",
  "title": "Soundpick",
  "description": "Soundpick extension for raycast.com",
  "icon": "sound-icon.png",
  "author": "robinsalehjan",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "soundpick",
      "title": "Change Sound Output",
      "subtitle": "Soundpick",
      "description": "Change the device you are playing sound from",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.1",
    "run-applescript": "^6.0.0",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
