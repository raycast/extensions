{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "buildspace-os-raycast",
  "title": "buildspace os",
  "description": "use a mini version of the buildspace os in your menu bar",
  "icon": "command-icon.png",
  "author": "landon",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "buildspace os",
      "description": "use the buildspace os in your menu bar",
      "mode": "menu-bar",
      "interval": "1m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.2",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}