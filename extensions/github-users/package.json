{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-users",
  "title": "GitHub Users Search",
  "description": "Search GitHub users",
  "icon": "icon.png",
  "author": "luisFilipePT",
  "categories": [
    "Other",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list",
      "title": "Search Users",
      "subtitle": "GitHub",
      "description": "Search person on GitHub",
      "keywords": [
        "github",
        "portfolio"
      ],
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/rest": "^20.0.1",
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.9.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@octokit/types": "^11.1.0",
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
