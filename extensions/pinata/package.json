{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pinata",
  "title": "Pinata",
  "description": "An extension to interact with the Pianta API",
  "icon": "pinnie.png",
  "author": "stevedylandev",
  "owner": "pinata",
  "access": "public",
  "categories": [
    "Web",
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "PINATA_JWT",
      "description": "Your Pinata API key JWT which can be created at https://app.pinata.cloud/developers",
      "type": "password",
      "required": true,
      "title": "Pinata API JWT"
    },
    {
      "name": "GATEWAY",
      "description": "Paste in your Dedicated Gateway or public gateway in the following format: https://gateway.pinata.cloud",
      "type": "textfield",
      "required": true,
      "title": "IPFS Gateway",
      "placeholder": "https://gateway.pinata.cloud"
    }
  ],
  "commands": [
    {
      "name": "uploadFile",
      "title": "Upload File",
      "subtitle": "Pinata",
      "description": "Upload a file to Pinata",
      "mode": "view"
    },
    {
      "name": "uploadDirectory",
      "title": "Upload Folder",
      "subtitle": "Pinata",
      "description": "Upload a folder to Pinata",
      "mode": "view"
    },
    {
      "name": "listFiles",
      "title": "List Files",
      "subtitle": "Pinata",
      "description": "Retrieve the latest 100 files from your Pinata account",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "@raycast/utils": "^1.4.16",
    "@types/node-fetch": "^2.6.9",
    "axios": "^1.2.1",
    "base-path-converter": "^1.0.2",
    "form-data": "^4.0.0",
    "formidable": "^3.5.1",
    "fs": "^0.0.1-security",
    "got": "^12.5.3",
    "react": "^18.2.0",
    "recursive-fs": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
