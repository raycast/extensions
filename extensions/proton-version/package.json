{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "proton-version",
  "title": "Proton Version",
  "description": "Keep track of the lastest version of Proton products",
  "icon": "command_icon.png",
  "author": "flavien.bonvin",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Data",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "proton-version",
      "title": "Proton Version",
      "description": "Get Proton web products version number",
      "mode": "view"
    },
    {
      "name": "proton-version-beta",
      "title": "Proton Version Beta",
      "description": "Get beta Proton web products version number",
      "mode": "view"
    },
    {
      "name": "proton-version-bridge",
      "title": "Proton Bridge Version",
      "description": "Get Proton Bridge version number",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.4.18"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
