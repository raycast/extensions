{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "internet-radio",
  "title": "internet-radio",
  "description": "Plays internet radio",
  "icon": "command-icon.png",
  "author": "HelloImSteven",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "browse-stations",
      "title": "Browse Stations",
      "description": "Browse all currently configured radio stations",
      "mode": "view"
    },
    {
      "name": "new-station",
      "title": "New Station",
      "description": "Add a new radio station",
      "mode": "view"
    },
    {
      "name": "play-last-station",
      "title": "Play Last Station",
      "description": "Resumes playback of the last played station",
      "mode": "no-view"
    },
    {
      "name": "stop-playback",
      "title": "Stop Playback",
      "description": "Stops radio playback and removes the associated stream file from Music.app.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "@raycast/utils": "^1.4.16",
    "run-applescript": "^6.1.0",
    "@raycast/utils": "^1.4.16"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}