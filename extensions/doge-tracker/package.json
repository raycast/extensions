{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "doge-tracker",
  "title": "Department of Government Efficiency Tracker",
  "description": "View information about the work done by the Department of Government Efficiency.",
  "icon": "extension-icon.png",
  "author": "Visual-Studio-Coder",
  "contributors": [
    "Osprey"
  ],
  "categories": [
    "Finance",
    "Data",
    "Media",
    "News",
    "Web"
  ],
  "keywords": [
    "doge",
    "finance",
    "media",
    "news",
    "elon",
    "elon musk",
    "elonmusk",
    "department",
    "government",
    "Efficiency",
    "Donald J Trump"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "view-grant-savings",
      "title": "View Grant Savings",
      "description": "Get grant savings",
      "mode": "view"
    },
    {
      "name": "view-contract-savings",
      "title": "View Contract Savings",
      "description": "Get contract savings.",
      "mode": "view"
    },
    {
      "name": "view-leases",
      "title": "View Leases",
      "description": "Get lease savings",
      "mode": "view"
    },
    {
      "name": "view-payments",
      "title": "View Payments",
      "description": "Get payment line items.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.94.3",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "prettier --write 'src/**/*.{js,jsx}'",
    "lint": "prettier --check 'src/**/*.{js,jsx}'",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
