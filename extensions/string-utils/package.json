{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "string-utils",
  "title": "String Utils",
  "description": "Run various operations on Strings",
  "icon": "string_utils_icon.png",
  "author": "duranbe",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "stringLength",
      "title": "String Length",
      "description": "Get the length of a string",
      "mode": "view"
    },
    {
      "name": "stringReverse",
      "title": "Reverse String",
      "description": "Reverse a string",
      "mode": "view"
    },
    {
      "name": "stringToUpperCase",
      "title": "To Upper Case",
      "description": "Make string upper case",
      "mode": "view"
    },
    {
      "name": "stringToLowerCase",
      "title": "To Lower Case",
      "description": "Make string lower case",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}