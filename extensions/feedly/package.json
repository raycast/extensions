{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "author": "shubham",
  "contributors": [
    "pernielsentikaer"
  ],
  "commands": [
    {
      "description": "Browse Feedly articles and collections via Raycast",
      "mode": "view",
      "name": "feedly",
      "title": "Feedly"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "@raycast/utils": "^1.4.14",
    "node-fetch": "^3.3.1",
    "node-html-markdown": "^1.2.2"
  },
  "description": "Browse Feedly through Raycast",
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.8.1",
    "typescript": "^4.4.3"
  },
  "icon": "command-icon.png",
  "license": "MIT",
  "name": "feedly",
  "preferences": [
    {
      "description": "Feedly Access Token",
      "name": "feedlyAccessToken",
      "placeholder": "Find here: https://feedly.com/v3/auth/dev",
      "required": true,
      "title": "Feedly Access Token",
      "type": "password"
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "format": "prettier --write src/**/*",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "title": "Feedly"
}
