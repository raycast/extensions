{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-svg64",
  "title": "SVG64 - Convert SVGs to Base64 Strings",
  "description": "Convert SVGs to base64 strings, perfect for non-SVG enabled Markdown renderers.",
  "icon": "command-icon.png",
  "author": "EvanZhouDev",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "encode-svg-from-text",
      "title": "Encode SVG From Text",
      "description": "Converts SVG text into Base64.",
      "mode": "view"
    },
    {
      "name": "encode-svg-from-file",
      "title": "Encode SVG From File",
      "description": "Converts an SVG file into Base64.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.9.1",
    "svg64": "^2.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
