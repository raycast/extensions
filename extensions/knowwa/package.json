{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "knowwa",
  "title": "Knowwa",
  "description": "Generate and list documents in Knowwa directly from Raycast",
  "icon": "knowwa_icon.png",
  "author": "knowwa",
  "categories": [
    "Productivity",
    "Applications"
  ],
  "preferences": [
    {
      "type": "password",
      "required": true,
      "name": "AccessToken",
      "description": "Please add your Access Token here. You can obtain your Access Token in the User-Settings section.",
      "placeholder": "Access Token",
      "title": "Access Token"
    }
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "indexDocs",
      "title": "List Documents",
      "subtitle": "Knowwa",
      "description": "List your Knowwa documents",
      "mode": "view"
    },
    {
      "name": "newDoc",
      "title": "Create Document",
      "subtitle": "Knowwa",
      "description": "Create a new document from your templates",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.2",
    "@raycast/utils": "^1.4.10",
    "axios": "^1.8.4",
    "dayjs": "^1.11.6",
    "got": "^12.5.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.31.11",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
