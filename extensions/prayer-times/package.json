{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prayer-times",
  "title": "Prayer Times",
  "description": "An extension to show prayer times for muslims (Athan Times)",
  "keywords": [
    "Athan",
    "Prayer",
    "Pray",
    "Salah",
    "أذان",
    "صلاة",
    "صلوات",
    "Times",
    "Muslim",
    "Islam",
    "Prayer Times",
    "Athan Times"
  ],
  "icon": "extension-icon.png",
  "author": "Abdullah-Dev115",
  "categories": [
    "News",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "show-prayer-times",
      "title": "Show Prayer (athan) Times",
      "description": "Display the prayer times",
      "keywords": [
        "Athan",
        "Prayer",
        "Pray",
        "Salah",
        "أذان",
        "صلاة",
        "صلوات",
        "Times",
        "Muslim",
        "Islam",
        "Prayer Times",
        "Athan Times"
      ],
      "mode": "view"
    },
    {
      "name": "show-14-days-prayer-times",
      "title": "Show 14 Days Prayer Times",
      "description": "Display prayer times for the next 14 days",
      "keywords": [
        "Athan",
        "Prayer",
        "Salah",
        "Times",
        "Muslim",
        "Islam",
        "Prayer Times",
        "14 days",
        "Forecast"
      ],
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.98.5",
    "@raycast/utils": "^1.17.0",
    "got": "^14.4.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
