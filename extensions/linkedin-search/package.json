{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linkedin-search",
  "title": "Linkedin Search",
  "description": "Search LinkedIn directly from Raycast",
  "icon": "command-icon.png",
  "author": "zakirjaafar",
  "categories": [
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
