  "dependencies": {
    "@raycast/api": "^1.102.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@raycast/utils": "^1.9.3",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "eslint": "^8.57.0",
    "prettier": "^2.8.8",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
