{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sec-filings-search",
  "title": "SEC Filings Search",
  "description": "An extension to search company SEC Filings and open in browser. Currently only 8-K and 10-K forms.",
  "icon": "icon.png",
  "author": "chewhx",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search SEC Filings",
      "description": "Search and get company SEC Filings",
      "mode": "view"
    }
  ],
  "categories": [
    "Data",
    "Finance",
    "Productivity",
    "Web"
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "axios": "^1.8.4",
    "dayjs": "^1.11.10"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
