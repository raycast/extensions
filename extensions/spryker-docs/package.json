{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spryker-docs",
  "title": "Spryker Docs",
  "description": "Search Spryker Documentation directly from Raycast",
  "icon": "spryker.png",
  "author": "DinisEsteves",
  "categories": ["Documentation", "Developer Tools"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Spryker Docs",
      "description": "Search through the Spryker docs directly from Raycast.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "algoliasearch": "^4.17.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
