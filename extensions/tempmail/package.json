{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tempmail",
  "title": "TempMail",
  "description": "Temporary email mailbox using the mail.gw API",
  "icon": "command-icon.png",
  "author": "Joshlucpoll",
  "categories": [
    "Applications",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "tempmail",
      "title": "Open Mailbox",
      "description": "Open TempMail inbox to mange and view emails and addresses",
      "mode": "view"
    },
    {
      "name": "reset",
      "title": "Reset TempMail",
      "description": "Resets TempMail to default settings",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "@raycast/utils": "^1.10.1",
    "axios": "^1.8.4",
    "eml-parser": "^1.2.0",
    "moment": "^2.29.4",
    "node-html-markdown": "^1.3.0",
    "unique-names-generator": "^4.7.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.2.39",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
