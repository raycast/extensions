{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "converter",
  "title": "Converter",
  "description": "An elegant tool for converting various kinds of data.",
  "icon": "command-icon.png",
  "author": "Sunnyyoung",
  "license": "MIT",
  "commands": [
    {
      "name": "number",
      "title": "Convert Number",
      "description": "Convert number into different radix/endian.",
      "mode": "view"
    },
    {
      "name": "string",
      "title": "Convert String",
      "description": "Convert string into various encoded/hashed.",
      "mode": "view"
    },
    {
      "name": "ip",
      "title": "Convert IP",
      "description": "Convert IP between string and number.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.29.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}