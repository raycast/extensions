{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rusbase",
  "title": "Rusbase",
  "description": "Get sections, markets and companies from Rusbase directly in Raycast",
  "icon": "extension-icon.png",
  "author": "khlebobul",
  "categories": [
    "News"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Sections",
      "description": "Sections News from Rusbase",
      "mode": "view"
    },
    {
      "name": "markets",
      "title": "List Markets",
      "description": "Markets News from Rusbase",
      "mode": "view"
    },
    {
      "name": "companies",
      "title": "List Companies",
      "description": "Companies News from Rusbase",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.8.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
