{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "compressx",
  "title": "Compresto",
  "description": "Compress files easily, with up to 90% file size reduction",
  "icon": "compressx-icon.png",
  "author": "hieudinh",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "compressVideo",
      "title": "Compress Files",
      "subtitle": "Compresto",
      "description": "Compress selected files with Compresto",
      "mode": "view"
    },
    {
      "name": "importFiles",
      "title": "Import Files",
      "subtitle": "Compresto",
      "description": "Import selected files into Compresto",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.100.2",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/open": "^6.2.1",
    "@types/react": "^19.0.10",
    "eslint": "^8.51.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
