{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "compressx",
  "title": "CompressX",
  "description": "Compress the selected video in Finder with CompressX.",
  "icon": "compressx-icon.png",
  "author": "hieudinh",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "compressVideo",
      "title": "Compress Video",
      "subtitle": "CompressX",
      "description": "Compress selected video with CompressX",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/open": "^6.2.1",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.1.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
