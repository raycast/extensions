{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tarot",
  "title": "Tarot",
  "description": "Draw cards from a virtual tarot deck.",
  "icon": "command-icon.png",
  "author": "tsui_river",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "draw-a-card",
      "title": "Draw a Card",
      "description": "Draw a card from a virtual tarot deck and see it's meaning.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
