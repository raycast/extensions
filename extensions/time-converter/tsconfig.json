{
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["src/**/*"],
  "compilerOptions": {
	"strict": true,
	"jsx": "react-jsx",
	"target": "es2020",
	"lib": ["es2020"],
	"module": "commonjs",
	"moduleResolution": "node",
	"esModuleInterop": true,
	"skipLibCheck": true,
	"forceConsistentCasingInFileNames": true,
	"resolveJsonModule": true,
	"outDir": "dist"
  }
}