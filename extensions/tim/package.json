{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tim",
  "title": "Tim",
  "version": "1.0.0",
  "description": "Manage your timers for Tim from Raycast",
  "icon": "tim-icon.png",
  "author": "tm.wrnr",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggleTimer",
      "title": "Toggle Timer",
      "subtitle": "Tim",
      "description": "Toggle current active timer.",
      "mode": "no-view"
    },
    {
      "name": "newTask",
      "title": "New Task",
      "subtitle": "Tim",
      "description": "Create a new task.",
      "mode": "view"
    },
    {
      "name": "openActiveRecord",
      "title": "Open Active Record",
      "subtitle": "Tim",
      "description": "Open active record editor.",
      "mode": "no-view"
    },
    {
      "name": "export",
      "title": "Export",
      "subtitle": "Tim",
      "description": "Export all application data.",
      "mode": "view"
    },
    {
      "name": "tasks",
      "title": "Tasks",
      "subtitle": "Tim",
      "description": "Show all tasks.",
      "mode": "view"
    },
    {
      "name": "dailyOverview",
      "title": "Daily Overview",
      "subtitle": "Tim",
      "description": "Show task grouped by day",
      "mode": "view"
    },
    {
      "name": "import",
      "title": "Import",
      "subtitle": "Tim",
      "description": "Import data.",
      "mode": "no-view"
    }
  ],
