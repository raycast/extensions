{
  "root": true,
  "plugins": [
    "import"
  ],
  "extends": [
    "@raycast"
  ],
  "rules": {
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc"
        }
      }
    ],
    "@typescript-eslint/no-non-null-assertion": "off"
  }
}
