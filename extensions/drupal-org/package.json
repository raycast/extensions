{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "drupal-org",
  "title": "Drupal.org",
  "description": "Information from drupal.org",
  "icon": "drop-icon.png",
  "author": "hussainweb",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-change-records",
      "title": "Search for Drupal Change Records",
      "description": "Search for changes in Drupal versions pulled from change records",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "cheerio": "^1.0.0-rc.12",
    "got": "^12.5.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
