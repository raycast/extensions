{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "developer-excuse",
  "title": "Developer Excuses",
  "description": "Insert a random developer excuse for not doing your work.",
  "icon": "excuse-icon.png",
  "author": "niemtec",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Insert Developer Excuse",
      "description": "Inserts a randomly picked developer excuse into your current window",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
