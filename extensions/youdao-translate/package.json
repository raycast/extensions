{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "youdao-translate",
  "title": "Youdao Translate",
  "description": "Translate text using Youdao",
  "icon": "translate.png",
  "author": "tiancheng92",
  "license": "MIT",
  "commands": [
    {
      "name": "youdao_translate",
      "title": "Translate",
      "subtitle": "Youdao",
      "description": "Translate text using Youdao",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "app_key",
      "type": "textfield",
      "required": true,
      "title": "APP Key",
      "description": "You can get Youdao Translate APP code at https://ai.youdao.com/console/",
      "placeholder": "Youdao Translate APP code"
    },
    {
      "name": "app_secret",
      "type": "password",
      "required": true,
      "title": "APP Secret",
      "description": "You can get Youdao Translate APP secret at https://ai.youdao.com/console/",
      "placeholder": "Youdao Translate APP Secret"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/react": "^17.0.30",
    "@types/node-fetch": "^2.5.12",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
