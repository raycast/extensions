{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brave",
  "title": "Brave",
  "description": "Create new tabs, search open tabs and history in Brave.",
  "icon": "brave-icon.png",
  "author": "ron-myers",
  "contributors": [
    "Codely"
  ],
  "categories": [
    "Applications",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "brave-go-to-tab",
      "title": "Search Brave Tabs",
      "subtitle": "Brave",
      "description": "List all tabs and go to the selected.",
      "mode": "view"
    },
    {
      "name": "brave-search-history",
      "title": "Search Brave History",
      "subtitle": "Brave",
      "description": "Search browser history.",
      "mode": "view"
    },
    {
      "name": "brave-new-tab",
      "title": "New Brave Tab",
      "subtitle": "Brave",
      "description": "Open a new Brave tab.",
      "mode": "view"
    }
  ],
  "preferences": [],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "@raycast/utils": "^1.1.0",
    "react-async": "^10.0.1",
    "run-applescript": "^6.0.0",
    "sql.js": "^1.6.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/sql.js": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix"
  }
}
