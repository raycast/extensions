{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-photos",
  "title": "Google Photos",
  "description": "Access, view, download, and upload media to your Google Photos account.",
  "icon": "google-photos.png",
  "author": "BalliAsghar",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "google-photos",
      "title": "Photos",
      "subtitle": "Google Photos",
      "description": "View Google Photos",
      "mode": "view"
    },
    {
      "name": "google-photos-upload",
      "title": "Upload",
      "subtitle": "Google Photos",
      "description": "Upload a photo to Google Photos",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "pageSize",
      "title": "Number of photos to fetch",
      "required": false,
      "type": "dropdown",
      "default": "30",
      "description": "Number of photos to fetch",
      "data": [
        {
          "title": "10",
          "value": "10"
        },
        {
          "title": "20",
          "value": "20"
        },
        {
          "title": "30",
          "value": "30"
        },
        {
          "title": "40",
          "value": "40"
        },
        {
          "title": "50",
          "value": "50"
        },
        {
          "title": "60",
          "value": "60"
        },
        {
          "title": "70",
          "value": "70"
        },
        {
          "title": "80",
          "value": "80"
        },
        {
          "title": "90",
          "value": "90"
        },
        {
          "title": "100",
          "value": "100"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
