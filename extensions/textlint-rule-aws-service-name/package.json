{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-textlint-rule-aws-service-name",
  "include": "src/.textlintrc.yaml",
  "title": "Textlint AWS Service Name",
  "description": "Check AWS services name typo",
  "icon": "command-icon.png",
  "author": "bun913",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
