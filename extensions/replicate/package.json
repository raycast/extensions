{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "replicate",
  "title": "Replicate.com",
  "description": "View and search your Replicate.com ai generate predictions",
  "icon": "command-icon.png",
  "author": "KevinBatdorf",
  "categories": [
    "Media",
    "Design Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "replicate",
      "title": "View Predictions",
      "subtitle": "Replicate.com",
      "description": "View your Replicate.com predictoins",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "title": "API Token",
      "description": "Add your token from Replicate.com",
      "placeholder": "Token",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "@raycast/utils": "^1.4.1",
    "is-image": "^3.1.0",
    "node-fetch": "^3.2.10",
    "run-applescript": "^6.0.0",
    "sql.js": "^1.8.0",
    "tempy": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~18.7.18",
    "@types/react": "^18.0.20",
    "@types/sql.js": "^1.4.3",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
