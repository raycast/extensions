{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "translator",
  "title": "Translator", 
  "description": "translate to and polish english",
  "icon": "extension-icon.png",
  "author": "cos",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "translator",
      "title": "Translator",
      "description": "translate to and polish english",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.91.1",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10", 
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to bun.\\n\\nIf you did intend to publish it to bun, remove the \\`prepublishOnly\\` script and rerun \\`bun publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`bun run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "bunx @raycast/api@latest publish"
  }
}