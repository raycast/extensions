    "markdown-table-parser": "^1.0.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/markdown-table": "^3.0.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
