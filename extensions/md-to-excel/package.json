{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "md-to-excel",
  "title": "Markdown Table to Excel",
  "description": "Convert md format table to Excel compatible table.",
  "icon": "command-icon.png",
  "author": "wu.eth",
  "categories": [
    "Productivity"
