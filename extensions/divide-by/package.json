{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "divide-by",
  "title": "Divide By",
  "description": "Quickly divide by a selected number using shortcuts",
  "icon": "extension_icon.png",
  "author": "MykCib",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "keywords": [
    "division"
  ],
  "commands": [
    {
      "name": "numberToDivideBy",
      "title": "Set Default Divisor",
      "description": "Choose a number to quickly divide by",
      "mode": "no-view",
      "arguments": [
        {
          "name": "numberToDivideBy",
          "type": "text",
          "required": true,
          "placeholder": "8"
        }
      ]
    },
    {
      "name": "inputNumber",
      "title": "Input Dividend",
      "description": "Divide by a saved divisor quickly",
      "mode": "no-view",
      "arguments": [
        {
          "name": "numberToDivide",
          "type": "text",
          "required": true,
          "placeholder": "16"
        }
      ]
    },
    {
      "name": "copyFromClipboard",
      "title": "Paste Dividend",
      "description": "Quickly divide number from clipboard",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "version": "0.0.0"
}
