{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "json-to-type-raycast",
  "title": "Json To Type",
  "description": "Easily transform your JSON to type",
  "icon": "typescript.png",
  "author": "NicolasMontone",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "type",
      "title": "Json To Type",
      "description": "Transform your JSON to type",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.4",
    "@raycast/utils": "^1.3.1",
    "json-ts": "^1.6.4"
  },
  "devDependencies": {
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
