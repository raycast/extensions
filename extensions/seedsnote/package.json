{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "seedsnote",
  "title": "Seedsnote",
  "description": "A Seedsnote extension, capture your idea instantly.",
  "icon": "logo.png",
  "author": "Ironben",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "endpoint",
      "title": "API",
      "description": "https://seedsnote.com/help/advanced/apitoken/",
      "type": "textfield",
      "required": true,
      "placeholder": "Seedsnote API"
    }
  ],
  "commands": [
    {
      "name": "txtseed",
      "title": "Send Text",
      "description": "Send a text to Seedsnote",
      "mode": "view"
    },
    {
      "name": "imgseed",
      "title": "Send Image",
      "description": "Send an image to Seedsnote",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.3",
    "form-data": "^4.0.0",
    "got": "^13.0.0",
    "image-type": "^5.2.0",
    "read-chunk": "^4.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
