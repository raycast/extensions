{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "space-flights",
  "title": "Space Flights",
  "description": "Track and see past, present and future space flights",
  "icon": "extension-icon.png",
  "author": "ruisilva450",
  "categories": [
    "Fun",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "next-launches",
      "title": "Next Launches",
      "subtitle": "View the next rocket launches",
      "description": "Shows a list of the next rocket launches",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.2",
    "@raycast/utils": "^1.17.0",
    "@types/node-fetch": "2.6.12",
    "node-fetch": "2.7.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react-devtools": "5.2.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
