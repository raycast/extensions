{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "holopin",
  "title": "Holopin",
  "description": "Show Holopin stickers and board",
  "icon": "command-icon.png",
  "author": "timoransky",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "showStickers",
      "title": "Show Stickers",
      "subtitle": "Holopin",
      "description": "Show Holopin stickers in a grid",
      "mode": "view"
    },
    {
      "name": "showBoard",
      "title": "Show Board",
      "subtitle": "Holopin",
      "description": "Show Holopin board",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "username",
      "type": "textfield",
      "description": "Your Holopin username",
      "required": true,
      "title": "Username"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.2",
    "@raycast/utils": "^1.4.10"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
