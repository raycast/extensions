{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cursor-directory",
  "title": "Cursor Directory",
  "description": "Port functionality from cursor.directory",
  "icon": "extension-icon.png",
  "author": "escwxyz",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Prompt Search",
      "description": "Search system prompt from cursor.directory for Cursor Code Editor",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.81.2",
    "@raycast/utils": "^1.16.3"
  },
  "devDependencies": {
    "@babel/parser": "^7.25.6",
    "@babel/types": "^7.25.6",
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "@typescript-eslint/utils": "^8.3.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "update-prompts": "tsx src/updatePrompts.ts",
    "build": "tsx src/updatePrompts.ts && ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
