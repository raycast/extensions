{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "drupal-toolbox",
  "title": "Drupal Toolbox",
  "description": "Valuable tools and shortcuts for your Drupal Websites",
  "icon": "logo.png",
  "author": "emircanerkul",
  "categories": [
    "Developer Tools",
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Drupal Toolbox",
      "description": "Show the Drupal Toolbox",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "1.43.2",
    "@raycast/utils": "^1.4.11",
    "nanoid": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
