{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "readwise-to-tana",
  "title": "Readwise to Tana",
  "description": "Copy Readwise highlights to Tana",
  "icon": "extension_icon.png",
  "author": "believer",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Highlights",
      "description": "Copy Readwise highlights to Tana",
      "mode": "view",
      "preferences": [
        {
          "name": "readwiseToken",
          "title": "Readwise API token",
          "description": "Readwise API token",
          "type": "password",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.2",
    "@raycast/utils": "1.4.16",
    "handlebars": "4.7.7"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.8.4",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "typescript": "tsc --noEmit --watch"
  }
}
