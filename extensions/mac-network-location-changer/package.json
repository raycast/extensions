{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mac-network-location-changer",
  "title": "Mac Network Location Changer",
  "description": "Lists and changes current network location on Mac.",
  "icon": "extension-icon.png",
  "author": "rcouto",
  "version": "1.0.1",
  "categories": [
    "System",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "network-location",
      "title": "Network Location",
      "description": "Changes the network location",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.97.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
