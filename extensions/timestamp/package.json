{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "timestamp",
  "title": "TimeStamp",
  "description": "Get the current time stamp in a formatted string.",
  "icon": "command-icon.png",
  "author": "JannatinNaim",
  "categories": [
    "Productivity",
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Time Stamp",
      "description": "Copy current time stamp to clipboard and paste on current app.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
