{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "storytime",
  "title": "StoryTime",
  "description": "Generate a story using OpenAI and Upstash.",
  "icon": "command-icon.png",
  "author": "andreaselia",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Data",
    "Fun",
    "Applications",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Generate a Story",
      "description": "Generate a story using OpenAI.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "openai_api_key",
      "type": "password",
      "required": true,
      "title": "OpenAI API Key",
      "description": "You'll need this in order to send calls to the OpenAI."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "@raycast/utils": "^1.4.14",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
