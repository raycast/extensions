{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "simplebackups",
  "title": "SimpleBackups",
  "description": "View and manage your backups from SimpleBackups.com",
  "icon": "command-icon.png",
  "author": "islamessam",
  "categories": [
    "Developer Tools",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Manage Backups",
      "subtitle": "SimpleBackups",
      "icon": "simplebackups-ico-64.png",
      "description": "View and manage your backups",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "sb_api_token",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "Generate from your SimpleBackups account",
      "placeholder": "SimpleBackups API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.2.10",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
