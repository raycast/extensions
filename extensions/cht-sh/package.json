{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cht-sh",
  "title": "cht.sh",
  "description": "Quickly search cheatsheet by accessing cht.sh online.",
  "icon": "command-icon.png",
  "author": "BrandenXia",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Cheatsheets",
      "subtitle": "Show cheatsheets",
      "description": "Show cheatsheets",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.0",
    "@raycast/utils": "^1.7.0",
    "fuse.js": "^6.6.2",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}