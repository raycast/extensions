{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "leetcode",
  "title": "LeetCode",
  "description": "Search LeetCode problem, view LeetCode daily challenge, view LeetCode problem and more.",
  "icon": "leetcode.png",
  "author": "justin0u0",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "daily-challenge",
      "title": "View LeetCode Daily Challenge",
      "subtitle": "LeetCode",
      "description": "View the today daily-challenge problem.",
      "mode": "view"
    },
    {
      "name": "problem-search",
      "title": "Search LeetCode Problems",
      "subtitle": "LeetCode",
      "description": "Search and preview the LeetCode problems.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "@types/turndown": "^5.0.1",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
