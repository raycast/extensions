{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ideate",
  "title": "Ideate",
  "description": "Create a project folder, initialize it, and open with your IDE.",
  "version": "1.0.0",
  "icon": "extension_icon.png",
  "author": "B9O2",
  "license": "MIT",
  "categories": [
    "Developer Tools"
  ],
  "commands": [
    {
      "name": "create-project",
      "title": "Create Project",
      "icon": "create_project_icon.png",
      "description": "Choose a saved preset and create a project",
      "mode": "view"
    },
    {
      "name": "add-init-preset",
      "title": "Add Preset",
      "icon": "add_preset_icon.png",
      "description": "Add a reusable project preset",
      "mode": "view"
    },
    {
      "name": "manage-presets",
      "title": "Manage Presets",
      "icon": "manage_presets_icon.png",
      "description": "View, edit and delete your project presets",
      "mode": "view"
    }
  ],
  "preferences": [
  ],
  "dependencies": {
    "@raycast/api": "^1.100.0",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix",
    "publish": "npx @raycast/api@latest publish"
  }
}
