{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "semantic-scholar",
  "title": "Semantic Scholar",
  "description": "Search research papers on semantic scholar (semanticscholar.org)",
  "icon": "command-icon.png",
  "author": "blainerothrock",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Papers",
      "subtitle": "Semantic Scholar",
      "description": "Search Papers on Semantic Scholar",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.29.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
