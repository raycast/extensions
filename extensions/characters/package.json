{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "characters",
    "title": "Characters Collection Search",
    "description": "Manage your own collection of characters to copy/paste",
    "icon": "extension_icon.png",
    "author": "jan-dh",
    "categories": [
        "Productivity",
        "Developer Tools"
    ],
    "license": "MIT",
    "commands": [
        {
            "name": "index",
            "title": "Character List",
            "description": "Show a list of all characters",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.57.1"
    },
    "devDependencies": {
        "@raycast/eslint-config": "1.0.5",
        "@types/node": "18.8.3",
        "@types/react": "18.0.9",
        "eslint": "^7.32.0",
        "prettier": "^2.8.8",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "npx @raycast/api@latest publish"
    }
}
