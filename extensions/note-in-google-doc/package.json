{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "note-in-google-doc",
  "title": "Notes in Google Docs",
  "description": "Effortlessly capture your ideas and store them instantly in Google Docs",
  "icon": "notes-in-google-docs.png",
  "author": "mohit_tater",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Quick Note",
      "subtitle": "Notes in Google Docs",
      "description": "Capture your notes straight into the Google Docs.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "@raycast/utils": "^1.10.1",
    "@types/gapi": "^0.0.47",
    "googleapis": "^129.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
