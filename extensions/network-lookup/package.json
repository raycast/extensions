{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "network-lookup",
  "title": "ASN/IP lookup",
  "description": "A tool for network engineer to perform ASN/IP lookup",
  "icon": "command-icon.png",
  "author": "foof",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "asn",
      "title": "Query ASN",
      "description": "AS Number lookup",
      "mode": "view",
      "arguments": [
        {
          "name": "asn",
          "placeholder": "asn",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "ipinfo",
      "title": "Query IP address",
      "description": "IP address lookup via ipinfo",
      "mode": "view",
      "arguments": [
        {
          "name": "ip",
          "placeholder": "ip",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.0",
    "@raycast/utils": "^1.10.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
