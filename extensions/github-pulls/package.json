{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-pulls",
  "title": "Github Pulls",
  "description": "Github pulls extension for Raycast",
  "icon": "github.png",
  "author": "fonimus",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "github-pulls",
      "title": "Github Pull",
      "subtitle": "Github - pull requests",
      "description": "Overview of github pull requests",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "title": "Github token",
      "description": "Github personal generated token",
      "type": "password",
      "required": true
    },
    {
      "name": "repositories",
      "title": "Github repositories",
      "description": "Github repositories separated by space (format: owner/name)",
      "type": "textfield",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "1.48.5",
    "@raycast/utils": "1.5.0",
    "human-date": "1.4.0",
    "octokit": "2.0.14"
  },
  "devDependencies": {
    "@octokit/types": "^9.0.0",
    "@types/human-date": "1.4.2",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.54.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
