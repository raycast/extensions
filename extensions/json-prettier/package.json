{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "json-prettier",
  "title": "JSON Prettier",
  "description": "Convert JSON/JS Object to pretty JSON",
  "icon": "command-icon.png",
  "author": "lachero",
  "categories": [
    "Developer Tools",
    "Productivity",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "JSON Prettier",
      "description": "Convert JSON/JS Object to pretty JSON",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
