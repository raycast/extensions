{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brew-services",
  "title": "Manage Services",
  "description": "Manage homebrew services",
  "icon": "command-icon.png",
  "author": "es183923",
  "license": "MIT",
  "commands": [
    {
      "name": "manage-services",
      "title": "Manage Services",
      "subtitle": "Brew",
      "description": "Manage homebrew services",
      "mode": "view",
      "preferences": [
        {
          "name": "brewPath",
          "type": "textfield",
          "required": false,
          "title": "Brew Installation Path",
          "description": "Location to the Brew installation (Defaults to `/opt/homebrew/bin/brew` on M1 Macs, and `/usr/local/bin/brew` otherwise)"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.2",
    "execa": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.11.12",
    "@types/react": "^17.0.37",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.21.0",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
