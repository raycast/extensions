{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "growthbook",
  "title": "Growthbook",
  "description": "View Growthbook feature flags",
  "icon": "growthbook-logo.png",
  "author": "farisaziz12",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Feature Flags",
      "description": "Search Growthbook feature flags",
      "mode": "view"
    }
  ],
   "preferences": [
    {
      "name": "personalAccessToken",
      "type": "password",
      "required": true,
      "title": "Personal Access Token",
      "description": "These can be created by going to Personal Access Tokens under the your user menu.",
      "placeholder": "Enter your Growthbook personal access token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2",
    "@raycast/utils": "^1.9.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
