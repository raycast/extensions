{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "solana-explorer",
  "title": "Solana Explorer",
  "description": "Look up transactions and accounts on the various Solana clusters.",
  "icon": "command-icon.png",
  "author": "darklong",
  "license": "MIT",
  "contributors": [
    "AnishDe12020"
  ],
  "commands": [
    {
      "name": "mainnet",
      "title": "Solana Mainnet Explorer",
      "description": "Explore transactions on the mainnet",
      "mode": "view"
    },
    {
      "name": "testnet",
      "title": "Solana Testnet Explorer",
      "description": "Explore transactions on the testnet",
      "mode": "view"
    },
    {
      "name": "devnet",
      "title": "Solana Devnet Explorer",
      "description": "Explore transactions on the devnet",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "explorer",
      "description": "Your preferred Solana explorer",
      "type": "dropdown",
      "required": false,
      "title": "Preferred explorer",
      "default": "SOLSCAN",
      "data": [
        {
          "title": "Solscan",
          "value": "SOLSCAN"
        },
        {
          "title": "Solana Explorer",
          "value": "SOLANA_EXPLORER"
        },
        {
          "title": "Solana FM",
          "value": "SOLANA_FM"
        },
        {
          "title": "Solana Beach",
          "value": "SOLANA_BEACH"
        }
      ]
    },
    {
      "name": "maxStorage",
      "description": "Maximum last values to store",
      "type": "dropdown",
      "required": false,
      "title": "Maximum last values to store",
      "default": "20",
      "data": [
        {
          "title": "10",
          "value": "10"
        },
        {
          "title": "20",
          "value": "20"
        },
        {
          "title": "50",
          "value": "50"
        },
        {
          "title": "100",
          "value": "100"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.1",
    "@raycast/utils": "^1.4.16",
    "@solana/spl-token-registry": "^0.2.4574",
    "@solana/web3.js": "^1.73.0",
    "axios": "^1.2.6",
    "use-debounce": "^9.0.3"
  },
  "devDependencies": {
    "@types/node": "~18.11.18",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "react-devtools": "^4.27.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
