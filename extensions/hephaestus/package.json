{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hephaestus",
  "title": "Hephaestus - JSON Tools",
  "description": "A collection of useful developer tools and utilities, including JSON to Swift, Objective-C, TypeScript, and more.",
  "icon": "icon.png",
  "author": "sharker_alice_sharker",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "json2Swift",
      "title": "json2Swift",
      "description": "Convert JSON to Swift",
      "mode": "view"
    },
    {
      "name": "json2ObjectiveC",
      "title": "json2ObjectiveC",
      "description": "Convert JSON to Objective-C",
      "mode": "view"
    },
    {
      "name": "jsonFormat",
      "title": "jsonFormat",
      "description": "Format and beautify JSON content",
      "mode": "view"
    },
    {
      "name": "json2Typescript",
      "title": "json2Typescript",
      "description": "Convert JSON to Typescript",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.86.0",
    "@raycast/utils": "^1.17.0",
    "js-beautify": "^1.15.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "quicktype-core": "^23.0.170",
    "react-devtools": "^5.2.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
