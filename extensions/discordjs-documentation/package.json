{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "discordjs-documentation",
  "title": "Discord.js Documentation",
  "description": "Quickly search through discord.js documentation.",
  "icon": "command-icon.png",
  "author": "mah51",
  "categories": [
    "Documentation",
