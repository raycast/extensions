{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "static-marks",
  "title": "Static Marks - Bookmark Search",
  "description": "Search and launch websites from your Static Marks bookmark YAML file.",
  "icon": "command-icon.png",
  "author": "aerobless",
  "contributors": [
    "rude"
  ],
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Bookmarks",
      "subtitle": "Static Marks",
      "description": "Search your Static Marks bookmarks.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "static-mark-yaml-url",
      "description": "The absolute path or public url where your Static Marks YAML file is located",
      "type": "textfield",
      "placeholder": "/Users/[user folder]/bookmarks.yml",
      "required": true,
      "title": "Static Marks YAML File"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.43.2",
    "@raycast/utils": "^1.4.11",
    "cross-fetch": "^3.1.5",
    "yaml": "^2.1.3"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.25",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
