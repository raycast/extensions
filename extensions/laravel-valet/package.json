{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "laravel-valet",
  "title": "Laravel Valet",
  "description": "Search and open local Valet sites",
  "icon": "laravel-valet.png",
  "author": "denniseilander",
  "contributors": [
    "KevinBatdorf"
  ],
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Sites",
      "description": "Search and open Laravel Valet sites",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.4",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "20.1.0",
    "@types/react": "18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
