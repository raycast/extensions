{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wikipedia",
  "title": "Wikipedia",
  "description": "Search Wikipedia articles and view them",
  "icon": "wikipedia.png",
  "author": "vimtor",
  "license": "MIT",
  "commands": [
    {
      "name": "search-page",
      "title": "Search Page",
      "subtitle": "Wikipedia",
      "description": "Search Wikipedia pages by title",
      "mode": "view"
    },
    {
      "name": "random-page",
      "title": "Random Page",
      "subtitle": "Wikipedia",
      "description": "Open a random Wikipedia page",
      "mode": "view"
    },
    {
      "name": "featured-page",
      "title": "Featured Page",
      "subtitle": "Wikipedia",
      "description": "Open today's featured Wikipedia page",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.7",
    "swr": "^1.0.1",
    "got": "^11.8.3",
    "turndown": "^7.1.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3",
    "@types/turndown": "^5.0.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}