{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "host-switch",
  "title": "Host Switch",
  "description": "Quickly switch between hosts while preserving the url path",
  "icon": "icon.png",
  "author": "ewgenius",
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "switch",
      "title": "Host Switch",
      "description": "Quickly switch between hosts",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.5",
    "@raycast/utils": "^1.5.2",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
