{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "new-york-times",
  "title": "New York Times",
  "description": "Checkout all NYT posts",
  "icon": "icon.png",
  "author": "Antonwy",
  "categories": [
    "News"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "nyt-popular-posts",
      "title": "View Popular Posts",
      "subtitle": "New York Times",
      "description": "Checkout the most popular NYT posts",
      "mode": "view",
      "icon": "icon.png"
    },
    {
      "name": "nyt-search-posts",
      "title": "Search Posts",
      "subtitle": "New York Times",
      "description": "Search all NYT posts",
      "mode": "view",
      "icon": "icon.png"
    },
    {
      "name": "nyt-top-posts",
      "title": "View Top Posts",
      "subtitle": "New York Times",
      "description": "Checkout the current top NYT posts",
      "mode": "view",
      "icon": "icon.png"
    }
  ],
  "preferences": [
    {
      "name": "api_key",
      "title": "NYT API Key",
      "description": "Your NYT API Key",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.2",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
