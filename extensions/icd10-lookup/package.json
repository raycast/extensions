{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "icd10-lookup",
  "title": "ICD10 Lookup",
  "description": "Shows matching ICD10 codes based on disease name input. Uses NLM's ICD10 API.",
  "icon": "command-icon.png",
  "author": "ubergraham",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "lookup-icd10-code",
      "title": "Lookup ICD10 Code",
      "description": "Queries ICD10 API",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.1",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
