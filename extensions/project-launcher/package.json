{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "project-launcher",
  "title": "Project Launcher",
  "description": "Search and open projects in Cursor and iTerm2",
  "icon": "folder-icon.png",
  "author": "mohammad_orabi",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-projects",
      "title": "Open Projects",
      "description": "Search and open projects in Cursor and iTerm2",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.6",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^8.19.0",
    "prettier": "^2.5.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
