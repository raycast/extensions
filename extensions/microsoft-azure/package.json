{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "microsoft-azure",
  "title": "Microsoft Azure",
  "description": "Quick access to Microsoft Azure resources",
  "icon": "azure-logo.png",
  "author": "crisboarna",
  "categories": [
    "Applications",
    "Developer Tools",
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "console",
      "title": "Console",
      "description": "Get a direct link to a service in the Azure Portal",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "@raycast/utils": "^1.4.13"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
