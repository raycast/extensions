{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hypersonic",
  "title": "Hypersonic",
  "description": "Fastest Notion to-do app ever",
  "icon": "icon-purple.png",
  "author": "reboot.studio",
  "owner": "reboot",
  "contributors": [
    "thomasjost"
  ],
  "access": "public",
  "version": "2.0.4",
  "categories": [
    "Developer Tools",
    "Design Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "My Tasks",
      "subtitle": "Hypersonic",
      "description": "Fastest Notion task manager",
      "mode": "view"
    },
    {
      "name": "active-todos",
      "title": "Active Tasks",
      "subtitle": "Hypersonic",
      "description": "See your active tasks",
      "mode": "menu-bar",
      "interval": "5m"
    }
  ],
  "preferences": [],
  "dependencies": {
    "@notionhq/client": "2.2.0",
    "@raycast/api": "1.48.5",
    "@raycast/utils": "1.5.2",
    "chrono-node": "2.4.1",
    "date-fns": "2.29.3",
    "fuse.js": "6.6.2",
    "lodash": "4.17.21",
    "node-fetch": "3.2.4"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
