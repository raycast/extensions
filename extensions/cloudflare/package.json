{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cloudflare",
  "title": "Cloudflare",
  "description": "Search and view your Cloudflare sites, pages, domains, and more",
  "icon": "icon.png",
  "author": "destiner",
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "contributors": [
    "niklaswa",
    "teziovsky"
  ],
  "commands": [
    {
      "name": "view-sites",
      "title": "View Sites",
      "subtitle": "Cloudflare",
      "description": "Lists your sites",
      "mode": "view"
    },
    {
      "name": "view-pages",
      "title": "View Pages",
      "subtitle": "Cloudflare",
      "description": "Lists your page projects",
      "mode": "view"
    },
    {
      "name": "view-members",
      "title": "View Members",
      "subtitle": "Cloudflare",
      "description": "Lists your team members",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "API Token",
      "description": "API token",
      "placeholder": "Your token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "axios": "^1.6.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "~20.11.22",
    "@types/react": "^18.2.60",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "typecheck": "tsc --noEmit"
  }
}
