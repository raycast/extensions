{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mozilla-vpn",
  "title": "Mozilla VPN Connect",
  "description": "Interact with the Mozilla VPN from Raycast.",
  "icon": "MozillaVPN.png",
  "author": "natew",
  "categories": [
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "VPN Connect",
      "subtitle": "Mozilla VPN",
      "description": "Activate or deactivate the Mozilla VPN.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.71.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "prettier:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md}'",
    "prettier:format": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'"
  }
}
