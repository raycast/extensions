{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "whois",
  "title": "Whois",
  "description": "Retrieve whois data.",
  "icon": "extension_icon.png",
  "author": "zavbala",
  "categories": [
    "Web",
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "whois",
      "title": "Whois",
      "description": "Retrieve whois data",
      "mode": "view",
      "arguments": [
        {
          "name": "domain",
          "title": "Domain",
          "description": "Domain to lookup",
          "placeholder": "raycast.com",
          "required": true,
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.0",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
