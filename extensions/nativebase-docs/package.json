{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "native-base-docs",
  "title": "NativeBase Documention",
  "description": "Search Nativebase documentation",
  "icon": "nativebase-logo.png",
  "author": "shubham",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "NativeBase Documention",
      "description": "Search Nativebase documentation",
      "mode": "view",
      "icon": "nativebase-logo.png"
    }
  ],
  "preferences": [
    {
      "name": "gh_pat",
      "type": "password",
      "title": "GitHub Personal Access Token",
      "description": "GitHub Personal Access Token",
      "label": "GitHub PAT",
      "required": false
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "format": "prettier --write ./src/**/*.{ts,tsx}"
  }
}
