{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "envato",
  "title": "Envato Sales",
  "version": "1.1.0",
  "description": "Tracking New Sales For Envato Author",
  "icon": "icon.png",
  "author": "astroon",
  "license": "MIT",
  "commands": [
    {
      "name": "envato",
      "title": "Track Sales",
      "subtitle": "Envato",
      "description": "Tracking New Sales For Envato Author",
      "mode": "view"
    },
    {
      "name": "accountEnvato",
      "title": "Portfolio",
      "subtitle": "Envato",
      "description": "My Protfolio For Envato",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "required": true,
      "title": "Envato Token",
      "description": "You need to provide an API token to access the API. The token must be a 32 character long alphanumeric string. See https://build.envato.com/api/#token"
    }
  ],
  "dependencies": {
    "@raycast/utils": "^1.5.2",
    "@raycast/api": "^1.49.3",
    "dateformat": "^5.0.3",
    "envato": "^3.0.8",
    "node-html-markdown": "^1.3.0",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/dateformat": "^3.0.1",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.58",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.8.0",
    "react-devtools": "^4.27.6",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
