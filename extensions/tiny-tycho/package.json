{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tiny-tycho",
  "title": "Tiny Tycho",
  "description": "Cute and Goofy Tycho Stickers for Mac",
  "icon": "tiny-tycho.png",
  "author": "orangebyte",
  "categories": [
    "Fun",
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Stickers",
      "description": "List of all Tiny Tycho stickers",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.81.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
