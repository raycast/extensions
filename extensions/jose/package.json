{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jose",
  "title": "Jose",
  "description": "Private chat interaction",
  "icon": "icon.png",
  "author": "tkowalski29",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "assistant",
      "title": "Assistant",
      "subtitle": "Assistants collection",
      "description": "Collection of your private assistants",
      "mode": "view"
    },
    {
      "name": "chat",
      "title": "Chat",
      "subtitle": "Chat view",
      "description": "Chat view for starting conversation",
      "mode": "view",
      "arguments": [
        {
          "name": "ask",
          "type": "text",
          "placeholder": "Ask",
          "required": false
        }
      ]
    },
    {
      "name": "conversation",
      "title": "Chats",
      "subtitle": "Conversations collection",
      "description": "Collection of your recent conversation",
      "mode": "view"
    },
    {
      "name": "snippet",
      "title": "Snippet",
      "subtitle": "Snippets collection",
      "description": "Collection of your private snippets",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "apiOpenAiKey",
      "title": "Open AI API Key",
      "description": "Enter your personal Open AI API Key",
      "type": "password",
      "required": false
    },
    {
      "name": "apiOllamaHost",
      "title": "Ollama API Host",
      "description": "Enter your host to Ollama API",
      "type": "textfield",
      "required": false
    },
    {
      "name": "apiLangSmithKey",
      "title": "Lang Smith API Key",
      "description": "Enter your personal Lang Smith API Key for project `Raycast Jose`",
      "type": "password",
      "required": false
    },
    {
      "name": "apiLunaryKey",
      "title": "Lunary API Key",
      "description": "Enter your personal Lunary API Key for project `Raycast Jose`",
      "type": "password",
      "required": false
    },
    {
      "name": "apiLangFusePublicKey",
      "title": "Lang Fuse public API Key",
      "description": "Enter your personal Lang Fuse public API Key for project `Raycast Jose`",
      "type": "password",
      "required": false
    },
    {
      "name": "apiLangFuseSecretKey",
      "title": "Lang Fuse secret API Key",
      "description": "Enter your personal Lang Fuse secret API Key for project `Raycast Jose`",
      "type": "password",
      "required": false
    },
    {
      "name": "apiEndpoint",
      "title": "API Endpoint",
      "description": "API endpoint for communication",
      "type": "textfield",
      "required": false
    },
    {
      "name": "apiBinnary",
      "title": "Patch to binnary script",
      "description": "Binnary endpoint for communication",
      "type": "file",
      "required": false
    },
    {
      "name": "apiEndpointData",
      "title": "API Endpoint Data",
      "description": "API endpoint for download assistants and snippets",
      "type": "textfield",
      "required": false
    }
  ],
  "tools": [],
  "dependencies": {
    "@langchain/community": "^0.2.9",
    "@langchain/openai": "^0.1.2",
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.5.2",
    "@types/react": "^18.3.3",
    "@types/uuid": "^9.0.0",
    "langchain": "^0.2.8",
    "langfuse-langchain": "^3.14.0",
    "langsmith": "^0.1.36",
    "lunary": "^0.6.16",
    "node-fetch": "^2.7.0",
    "ollama": "^0.5.2",
    "openai": "^4.52.3",
    "say": "^0.16.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "18.8.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint-fix": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
