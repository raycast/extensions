{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "safari-tab-copy",
  "title": "Safari Tab Copier",
  "description": "Copies either the current, or all, Safari tab to various formats",
  "icon": "copy-safari-icon.png",
  "author": "rknightuk",
  "categories": [
    "Productivity",
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "copy-safari-tabs",
      "title": "Copy All Safari Tabs",
      "description": "Copy all Safari tabs",
      "mode": "view"
    },
    {
      "name": "copy-safari-tab",
      "title": "Copy Current Safari Tab",
      "description": "Copy the current Safari tab",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
