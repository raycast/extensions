{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hyper-focus",
  "title": "Hyper Focus",
  "description": "Controls the hyper-focus command line daemon which blocks distracting websites and apps.",
  "icon": "command-icon.png",
  "author": "iloveitaly",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Hyper Focus",
      "description": "View currently applied schedule, pause focus, and schedule overrides",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "@raycast/utils": "^1.4.14"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
