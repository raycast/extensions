{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hyper-focus",
  "title": "Hyper Focus",
  "description": "Controls the hyper-focus command line daemon which blocks distracting websites and apps.",
  "icon": "command-icon.png",
  "author": "iloveitaly",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Hyper Focus",
      "description": "View currently applied schedule, pause focus, and schedule overrides",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "18.0.37",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
