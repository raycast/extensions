{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "scaleway",
  "title": "Scaleway",
  "description": "Quick access to your Scaleway services",
  "icon": "scaleway.png",
  "author": "philibea",
  "contributors": [
    "flozdra"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "packageManager": "npm@9.2.0",
  "dependencies": {
    "@raycast/api": "1.63.0",
    "@scaleway/configuration-loader": "1.0.0",
    "@scaleway/sdk": "2.3.0",
    "@scaleway/use-dataloader": "4.0.0",
    "base64-js": "1.5.1",
    "cross-fetch": "4.0.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@scaleway/eslint-config-react": "3.16.3",
    "@scaleway/tsconfig": "1.1.1",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@typescript-eslint/eslint-plugin": "6.15.0",
    "@typescript-eslint/parser": "6.15.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  },
  "commands": [
    {
      "name": "appleSilicon",
      "title": "Apple Silicon",
      "icon": "appleSilicon.webp",
      "description": "Apple Silicon",
      "mode": "view"
    },
    {
      "name": "blockStorage",
      "title": "Block Storage",
      "icon": "blockStorage.webp",
      "description": "Block Storage",
      "mode": "view"
    },
    {
      "name": "containers",
      "title": "Containers",
      "description": "Get information about containers",
      "mode": "view",
      "icon": "containers.webp"
    },
    {
      "name": "instances",
      "title": "Instances",
      "description": "Get information about instances",
      "mode": "view",
      "icon": "instances.webp"
    },
    {
      "name": "iam",
      "title": "IAM",
      "description": "Get information about iam",
      "mode": "view",
      "icon": "iam.webp"
    },
    {
      "name": "databases",
      "title": "Databases",
      "description": "Get information about databases",
      "mode": "view",
      "icon": "database.webp"
    },
    {
      "name": "domains",
      "title": "Domains",
      "description": "Get information about domains",
      "mode": "view",
      "icon": "domains.webp"
    },
    {
      "name": "elasticMetal",
      "title": "Elastic Metal",
      "icon": "elasticMetal.webp",
      "description": "Elastic Metal Servers",
      "mode": "view"
    },
    {
      "name": "iot",
      "title": "IoT",
      "icon": "iot.webp",
      "description": "IOT hub management",
      "mode": "view"
    },
    {
      "name": "loadBalancer",
      "title": "Load Balancer",
      "icon": "loadBalancer.webp",
      "description": "Load Balancer management",
      "mode": "view"
    },
    {
      "name": "redis",
      "title": "Redis",
      "description": "Get information about redis clusters",
      "mode": "view",
      "icon": "redis.webp"
    },
    {
      "name": "publicGateways",
      "title": "Public Gateways",
      "description": "Get information about Public Gateways",
      "mode": "view",
      "icon": "publicGateways.webp"
    },
    {
      "name": "k8s",
      "title": "Kubernetes",
      "description": "Get information about Kubernetes",
      "mode": "view",
      "icon": "k8s.webp"
    },
    {
      "name": "mnq",
      "title": "Messaging & Queuing",
      "description": "Get information about Messaging & Queuing",
      "mode": "view",
      "icon": "mnq.webp"
    },
    {
      "name": "privateNetworks",
      "title": "Private Networks",
      "description": "Get information about Private Networks",
      "mode": "view",
      "icon": "privateNetworks.webp"
    },
    {
      "name": "functions",
      "title": "Serverless Functions",
      "description": "Get information about Serverless Functions",
      "mode": "view",
      "icon": "functions.webp"
    },
    {
      "name": "registry",
      "title": "Registry",
      "description": "Get information about Registry",
      "mode": "view",
      "icon": "registry.webp"
    },
    {
      "name": "transactionalEmail",
      "title": "Transactional Email",
      "description": "Get information about transactionalEmail domains",
      "mode": "view",
