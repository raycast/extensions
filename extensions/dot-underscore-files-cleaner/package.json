{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dot-underscore-files-cleaner",
  "title": "Dot Underscore Files Cleaner",
  "description": "Delete ._ files in the selected folder and its subfolders.",
  "icon": "extension_icon.png",
  "author": "danulqua",
  "categories": [
    "System",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Delete Dot Underscore Files",
      "description": "Delete ._ files in the selected folder and its subfolders.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "pull": "npx @raycast/api@latest pull-contributions"
  }
}
