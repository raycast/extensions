{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rlcs-scores",
  "title": "RLCS Scores",
  "description": "View live scores of RLCS games",
  "icon": "rlcs-icon.png",
  "author": "nicolasthy",
  "categories": [
    "Fun",
    "News"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Current tournament",
      "description": "Show scores of current RLCS tournament",
      "mode": "view",
      "interval": "10m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4",
    "dayjs": "^1.11.10",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
