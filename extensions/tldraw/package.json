{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tldraw",
  "title": "tldraw",
  "description": "Creates and persists tldraw projects",
  "icon": "tldraw-icon.png",
  "author": "zephraph",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-project",
      "title": "Create New Project",
      "subtitle": "tldraw",
      "description": "Creates a new tldraw project",
      "mode": "no-view",
      "arguments": [
        {
          "name": "name",
          "type": "text",
          "required": true,
          "placeholder": "name..."
        },
        {
          "name": "description",
          "type": "text",
          "required": false,
          "placeholder": "description..."
        }
      ]
    },
    {
      "name": "add-existing-project",
      "title": "Add Existing Project",
      "subtitle": "tldraw",
      "description": "Adds an existing tldraw project to your list of saved projects",
      "mode": "no-view",
      "arguments": [
        {
          "name": "url",
          "type": "text",
          "required": true,
          "placeholder": "https://www.tldraw.com/..."
        },
        {
          "name": "name",
          "type": "text",
          "required": true,
          "placeholder": "name..."
        },
        {
          "name": "description",
          "type": "text",
          "required": false,
          "placeholder": "description..."
        }
      ]
    },
    {
      "name": "list-projects",
      "title": "List Projects",
      "subtitle": "tldraw",
      "description": "List all saved tldraw projects",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.3",
    "@raycast/utils": "^1.10.1",
    "nanoid": "^5.0.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
