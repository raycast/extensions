{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "myip",
  "title": "MyIP",
  "description": "My IP information",
  "icon": "icon.png",
  "author": "Kang",
  "contributors": [
    "iwyvi",
    "j3lte",
    "hsuq",
    "pernielsentikaer",
    "ViGeng",
    "sebdanielsson"
  ],
  "categories": [
    "System",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show IP Address",
      "description": "My IP information",
      "keywords": [
        "internet"
      ],
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.102.7",
    "@raycast/utils": "^2.2.1",
    "cheerio": "1.1.2",
    "ip": "^2.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/ip": "^1.1.3",
    "@types/node": "^22",
    "eslint": "^9",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint:fix": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
