  "devDependencies": {
    "@types/ip": "^1.1.0",
    "@types/node": "~17.0.38",
    "@types/react": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "react-devtools": "^4.24.7",
    "typescript": "^4.7.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
