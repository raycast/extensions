{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "potter-db",
  "title": "Potter DB",
  "description": "Dive into the magical world of Harry Potter via Potter DB.",
  "keywords": [
    "Wizarding World",
    "Harry Potter"
  ],
  "icon": "command-icon.png",
  "author": "xmok",
  "categories": [
    "Data",
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "books",
      "title": "Search Books",
      "subtitle": "Potter DB",
      "description": "Retrieve books from the \"Wizarding World\"",
      "mode": "view"
    },
    {
      "name": "characters",
      "title": "Search Characters",
      "subtitle": "Potter DB",
      "description": "Retrieve characters from the \"Wizarding World\"",
      "mode": "view"
    },
    {
      "name": "movies",
      "title": "Search Movies",
      "subtitle": "Potter DB",
      "description": "Retrieve movies from the \"Wizarding World\"",
      "mode": "view"
    },
    {
      "name": "potions",
      "title": "Search Potions",
      "subtitle": "Potter DB",
      "description": "Retrieve potions from the \"Wizarding World\"",
      "mode": "view"
    },
    {
      "name": "spells",
      "title": "Search Spells",
      "subtitle": "Potter DB",
      "description": "Retrieve spells from the \"Wizarding World\"",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.16.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
