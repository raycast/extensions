{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "arc",
  "title": "Arc",
  "description": "Search and quickly navigate Arc's history and open tabs.",
  "icon": "icon-dark.png",
  "author": "thomas",
  "owner": "the-browser-company",
  "access": "public",
  "categories": [
    "Web",
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "contributors": [
    "zach",
    "ankitchouhan1020"
  ],
  "commands": [
    {
      "name": "search-history",
      "title": "Search History",
      "subtitle": "Arc",
      "description": "Search your browsing history.",
      "mode": "view"
    },
    {
      "name": "new-window",
      "title": "Open New Window",
      "description": "Create and open a new window",
      "mode": "no-view",
      "subtitle": "Arc"
    },
    {
      "name": "search-spaces",
      "title": "Search Spaces",
      "subtitle": "Arc",
      "description": "Search and select your spaces.",
      "mode": "view"
    },
    {
      "name": "search-tabs",
      "title": "Search Tabs",
      "subtitle": "Arc",
      "description": "Search and select your tabs.",
      "mode": "view"
    },
    {
      "name": "new-incognito-window",
      "title": "Open New Incognito Window",
      "subtitle": "Arc",
      "description": "Create and open a new incognito window",
      "mode": "no-view"
    },
    {
      "name": "new-tab",
      "title": "Open New Tab",
      "subtitle": "Arc",
      "description": "Create and open a new tab in the current window",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.0",
    "@raycast/utils": "^1.4.0",
    "lodash": "^4.17.21",
    "run-applescript": "^6.1.0",
    "run-jxa": "^3.0.0",
    "semver": "^7.3.8",
    "sql.js": "^1.8.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.17",
    "@types/semver": "^7.3.13",
    "@types/sql.js": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
