{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linguee",
  "title": "Linguee",
  "description": "Search Linguee translations for English, Chinese, German, Italian, Spanish, French.",
  "icon": "command-icon-linguee.png",
  "author": "geekdada",
  "license": "MIT",
  "commands": [
    {
      "name": "english-chinese",
      "title": "English ↔️ Chinese",
      "subtitle": "Translate English and Chinese",
      "description": "Translate English and Chinese",
      "mode": "view"
    },
    {
      "name": "english-german",
      "title": "English ↔️ German",
      "subtitle": "Translate English and German",
      "description": "Translate English and German",
      "mode": "view"
    },
    {
      "name": "english-italian",
      "title": "English ↔️ Italian",
      "subtitle": "Translate English and Italian",
      "description": "Translate English and Italian",
      "mode": "view"
    },
    {
      "name": "english-spanish",
      "title": "English ↔️ Spanish",
      "subtitle": "Translate English and Spanish",
      "description": "Translate English and Spanish",
      "mode": "view"
    },
    {
      "name": "english-french",
      "title": "English ↔️ French",
      "subtitle": "Translate English and French",
      "description": "Translate English and French",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.0",
    "axios": "^0.27.2",
    "axios-hooks": "^3.0.0",
    "lru-cache": "^7.10.1",
    "node-html-parser": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^18.0.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.7.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  },
  "engines": {
    "node": ">=16.10.0"
  }
}
