{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "1password8",
  "title": "1Password 8",
  "description": "Search, open, or edit your 1Password 8 items from Raycast.",
  "icon": "1password-icon.png",
  "author": "dteare",
  "contributors": [
    "khasbilegt"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list",
      "title": "1Password",
      "subtitle": "Open or edit items",
      "description": "List of all your 1Password 8 items in Raycast",
      "mode": "view",
      "keywords": [
        "1p",
        "1password",
        "password",
        "login"
      ]
    }
  ],
  "categories": [
    "Applications",
    "Productivity",
    "Security"
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "open": "^8.3.0",
    "sha.js": "^2.4.11"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/sha.js": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
