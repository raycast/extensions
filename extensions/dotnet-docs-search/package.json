{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dotnet-docs-search",
  "title": ".NET Documentation Search",
  "description": "Search .NET API documentation.",
  "icon": "dotnet-icon.png",
  "author": "huseyint",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": ".NET",
      "description": "Search in .NET Documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "axios": "^0.23.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/url-join": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "2.8.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
