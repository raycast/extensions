{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dub-link-shortener",
  "title": "Dub Link Shortener",
  "description": "Dub.co is the open-source link management infrastructure. This extension allows you to shorten and retrieve your Dub links directly inside Raycast.",
  "icon": "command-icon.png",
  "author": "quuu",
  "categories": [
    "Productivity",
    "Developer Tools",
    "Data",
    "Applications",
    "Web"
  ],
  "contributors": [
    "stelo"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "shorten-link",
      "title": "Shorten Link",
      "description": "Shorten links from arguments, clipboard or selection with dub.co using workspaces, custom domains, tags etc.",
      "mode": "view",
      "arguments": [
        {
          "name": "url",
          "placeholder": "URL",
          "type": "text"
        },
        {
          "name": "key",
          "placeholder": "Key",
          "type": "text"
        }
      ]
    },
    {
      "name": "search-links",
      "title": "Search Links",
      "description": "Search shortened links with dub.co in different workspaces",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.2",
    "@raycast/utils": "^1.16.0",
    "axios": "^1.6.2"
  },
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "required": true,
      "title": "API Key",
      "description": "Dub.co API key."
    }
  ],
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
