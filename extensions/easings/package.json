{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "easings",
  "title": "Easings",
  "description": "Quickly find and grab your favorite easings.",
  "icon": "command-icon.png",
  "author": "madebyankur",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Easings",
      "subtitle": "Easings",
      "description": "Search common easings to copy",
      "mode": "view"
    },
    {
      "name": "add",
      "title": "Add Custom Easing",
      "subtitle": "Easings",
      "description": "Add your own easing(s)",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4",
    "nanoid": "^5.0.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
