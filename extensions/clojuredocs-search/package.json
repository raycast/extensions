{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-clojuredocs",
  "title": "Search ClojureDocs Documentation",
  "description": "Search and browse clojuredocs.org site.",
  "icon": "clojure.png",
  "author": "danblancas",
  "categories": [
    "Productivity",
    "Documentation",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "ClojureDocs",
      "description": "Search clojuredocs.org by var, namespace or documentation string.",
      "mode": "view",
      "icon": "clojure.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "edn-data": "^1.0.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
