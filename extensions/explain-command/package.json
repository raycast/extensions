{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "explain-command",
  "title": "Explain Command",
  "description": "Let ChatGPT explain a command for you",
  "icon": "command-icon.png",
  "author": "maantje",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "explain-command",
      "title": "Explain Command",
      "subtitle": "openai.com",
      "description": "Let ChatGPT explain a command for you",
      "mode": "view"
    }
  ],
  "preferences": [
    {
        "name": "token",
        "type": "password",
        "required": true,
        "description": "OpenAI API key",
        "title": "OpenAI API key"
    }
],

  "dependencies": {
    "@raycast/api": "^1.56.0",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
