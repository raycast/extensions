{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "render",
  "title": "Render",
  "description": "View your Render services, deploys, and custom domains",
  "icon": "icon.png",
  "author": "destiner",
  "license": "MIT",
  "commands": [
    {
      "name": "view-services",
      "title": "View Services",
      "subtitle": "Render",
      "description": "View your services",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "key",
      "type": "password",
      "required": true,
      "title": "API Keys",
      "description": "Account API key",
      "placeholder": "Your key"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.0",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
