{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "render",
  "title": "Render",
  "description": "View your Render services, deploys, and custom domains",
  "icon": "icon.png",
  "author": "destiner",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "view-services",
      "title": "View Services",
      "subtitle": "Render",
      "description": "View your services",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "key",
      "type": "password",
      "required": true,
      "title": "API Key",
      "description": "Account API key",
      "placeholder": "rnd_xxxx..xxx"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.16.3",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
