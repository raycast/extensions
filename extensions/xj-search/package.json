{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "xj-search",
    "title": "xj_search",
    "description": "",
    "icon": "üîç",
    "author": "xiangjun_ma",
    "license": "MIT",
    "commands": [
        {
            "name": "index",
            "title": "xj bytedance search",
            "description": "quick lookup bytedance resource and open url",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.62.2",
        "js-yaml": "^4.1.0",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@raycast/eslint-config": "^1.0.6",
        "@types/js-yaml": "^4.0.9",
        "@types/node": "20.8.10",
        "@types/react": "18.2.27",
        "eslint": "^8.51.0",
        "prettier": "^3.0.3",
        "typescript": "^5.2.2"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint",
        "publish": "npx @raycast/api@latest publish"
    },
    "preferences": [
        {
            "name": "yamlFolderPath",
            "required": true,
            "description": "YAML File Paths",
            "type": "directory",
            "key": "yamlFolderPath",
            "title": "YAML Folder Path",
            "placeholder": "Enter the path to the folder containing YAML files"
        }
    ]
