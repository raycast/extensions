{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "force-quit",
  "title": "Force Quit",
  "description": "Force quit an open application from Raycast.",
  "icon": "command-icon.png",
  "author": "dannymout",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "force-quit",
      "title": "Force Quit",
      "description": "Force quits the selected application",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
