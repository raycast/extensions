{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "git-branch-name-generator",
  "title": "Git Branch Name Generator",
  "description": "Convert a string into a branch name",
  "icon": "command-icon.png",
  "author": "mbonner",
  "owner": "bootpack-digital",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Generate Git Branch Name",
      "subtitle": "From type and string inputs",
      "description": "Generate a git branch name using an opinionated structure",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
