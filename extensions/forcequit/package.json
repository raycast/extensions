{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "forcequit",
  "title": "ForceQuit",
  "description": "This extension allows you to force quit running applications from Raycast",
  "icon": "command-icon.png",
  "author": "boxpositron",
  "categories": [
    "System"
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Force Quit",
      "subtitle": "Force quit application",
      "description": "Force quit running / stalled applications",
      "mode": "view"
    }
  ],
  "keywords": [],
  "dependencies": {
    "@raycast/api": "^1.58.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  }
}
