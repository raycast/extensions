{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ai-humanizer",
  "title": "Humanize AI Text",
  "description": "Humanize AI Text",
  "icon": "command-icon.png",
  "author": "frolik",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "AI Humanizer",
      "description": "Humanize AI Text",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.85.1",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "title": "API Key",
      "description": "Enter your Rephrasy.ai API key",
      "required": true
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
