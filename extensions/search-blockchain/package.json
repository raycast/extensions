  "name": "search-blockchain",
  "title": "Search Blockchain",
  "description": "Search Bitcoin (BTC), Cardano (ADA), EOS, Ethereum (ETH), Monero (XMR), NEO, Ontology (ONT), Polygon (DOT), Solana (SOL), Stellar (XLM), Tron (TRX), XRP and more cryptocurrency blockchain explorer",
  "icon": "command-icon.png",
  "author": "gutenye",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "bitcoin-command",
      "title": "Search Bitcoin (BTC) Blockchain Explorer",
      "description": "Search Blockchain Explorer",
      "mode": "no-view",
      "arguments": [
        {
          "name": "query",
          "placeholder": "Query",
          "type": "text",
          "required": true
