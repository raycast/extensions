{
  "name": "xpf-converter",
  "title": "Xpf to Eur Converter",
  "description": "Convert Franc Pacifique (XPF) to Euros (EUR) with instant clipboard copy",
  "icon": "icon.png",
  "author": "vincent_perrot",
  "categories": ["Finance"],
  "license": "MIT",
  "commands": [
    {
      "name": "convert",
      "title": "Convert Xpf to Eur",
      "description": "Convert Franc Pacifique to Euros",
      "mode": "no-view",
      "arguments": [
        {
          "name": "amount",
          "placeholder": "Amount in XPF",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^8.51.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}