# Raycast Extension Environment Variables
# Copy this file to .env and fill in your actual values
# This file is safe to commit to the repository

# Author information (will be used in package.json)
RAYCAST_AUTHOR=your_github_username

# Repository URL (will be used in package.json)
RAYCAST_REPOSITORY_URL=https://github.com/your_username/isthereanydeal.git

# Extension metadata (optional, will fall back to package.json values)
RAYCAST_EXTENSION_NAME=isthereanydeal
RAYCAST_EXTENSION_TITLE=IsThereAnyDeal 