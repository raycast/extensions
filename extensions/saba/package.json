{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "saba",
  "title": "Saba",
  "description": "Watch live TV channels from around the world.",
  "categories": [
    "Applications"
  ],
  "icon": "extension-icon.png",
  "author": "dream_machine",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Saba",
      "description": "Watch live TV",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "key",
      "title": "Access Key",
      "description": "Your access key",
      "type": "password",
      "placeholder": "xxxxxx",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2",
    "snake-case": "^3.0.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
