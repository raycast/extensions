{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nowplaying-cli",
  "title": "Nowplaying-Cli",
  "description": "It's using nowplaying-cli for macos in order to control playing video or music. It's have pause next and previous features",
  "icon": "play_pause.png",
  "author": "yusuf_akdeniz",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "toggle-play-pause",
      "title": "Toggle Play/Pause",
      "description": "Toggles down and up music",
      "mode": "no-view",
      "icon": "play_pause.png"
    },
    {
      "name": "go-next",
      "title": "Go to Next One",
      "description": "Goes up to the next music",
      "mode": "no-view",
      "icon": "next.png"
    },
    {
      "name": "go-prev",
      "title": "Go to Previous One",
      "description": "Goes up to the previous music",
      "mode": "no-view",
      "icon": "previous.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "@raycast/utils": "^1.16.0",
    "@types/shelljs": "^0.8.15"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
