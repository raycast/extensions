{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "svgr",
  "title": "SVGR",
  "description": "Paste in SVG code. Get a react component. A raycast plugin for SVGR.",
  "icon": "command-icon.png",
  "author": "rickMcGavin",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "svgr",
      "description": "Opens SVGR form",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.33.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@svgr/core": "^6.2.1",
    "@svgr/plugin-jsx": "^6.2.1",
    "@svgr/plugin-svgo": "^6.2.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}