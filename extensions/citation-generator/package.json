{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "citation-generator",
  "title": "Citation Generator",
  "description": "Generates MLA style citation provided by formatically.com",
  "icon": "command-icon.png",
  "author": "AbhinavPalacharla",
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "generate-citation",
      "title": "Generate Citation",
      "description": "Generates MLA style citations",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.34.1",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
