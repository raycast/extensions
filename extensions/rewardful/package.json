{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rewardful",
  "title": "Rewardful",
  "description": "Unofficial Rewardful extension",
  "icon": "rewardful-icon.png",
  "author": "alexs",
  "license": "MIT",
  "commands": [
    {
      "name": "campaigns",
      "title": "View Campaigns",
      "subtitle": "Rewardful",
      "description": "View campaigns",
      "mode": "view"
    },
    {
      "name": "affiliates",
      "title": "View Affiliates",
      "subtitle": "Rewardful",
      "description": "View and update affiliates",
      "mode": "view"
    },
    {
      "name": "commission",
      "title": "View Commission",
      "subtitle": "Rewardful",
      "description": "View and update commission",
      "mode": "view"
    },
    {
      "name": "referrals",
      "title": "View Referrals",
      "subtitle": "Rewardful",
      "description": "View referrals",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "apiKey",
      "title": "API Key",
      "description": "Key for the API",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.14.1",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
