{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "london-live-arrivals",
  "title": "London Live Arrivals",
  "description": "This extension showing arrivals board for selection TFL stantions",
  "icon": "command-icon.png",
  "author": "ichikmarev",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "live-near-location",
      "title": "Arriving",
      "subtitle": "Select your stations",
      "description": "Shows the live timetable near location",
      "mode": "view",
      "interval": "3m"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
