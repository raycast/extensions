{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "brandfetch",
  "title": "Brandfetch",
  "description": "Quickly search and fetch icons from brandfetch",
  "icon": "command-icon.png",
  "author": "yonbergman",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Brandfetch",
      "subtitle": "Enter brand name or url",
      "description": "Search for brand icons on Brandfetch",
      "mode": "view",
      "icon": "/command-icon.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.3",
    "@raycast/utils": "^1.13.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}