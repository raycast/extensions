{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "include": ["src/**/*"],
  "compilerOptions": {
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "@commands": ["Commands/index"],
      "@constants": ["Constants/index"],
      "@components": ["Components/index"],
      "@managers": ["Managers/index"],
      "@hooks": ["Hooks/index"],
      "@models": ["Models/index"],
      "@network": ["Network/API/index"],
      "@providers": ["Providers/index"],
      "@stores": ["Stores/index"],
      "@types": ["Types/index"],
      "@urls": ["Network/URL/index"]
    }
  }
}
