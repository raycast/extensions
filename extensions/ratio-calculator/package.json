{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ratio-calculator",
  "title": "Ratio Calculator",
  "description": "A calculator for quickly calculating ratios/proportions. Replace multiple values on one side with variables such as x or y. Example syntax: \"1:2 = x:4\" outputs \"x = 2\"",
  "icon": "icon.png",
  "author": "justiniscoding",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "calculate-ratio",
      "title": "Calculate Ratio",
      "description": "Calculates equivalent ratios",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
