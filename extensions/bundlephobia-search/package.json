{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bundlephobia-search",
  "title": "Bundlephobia Search",
  "description": "Find package and check its size on Bundlephobia",
  "icon": "icon.png",
  "author": "vette",
  "contributors": [
    "rushelex"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Bundlephobia",
      "description": "Find package and check its size on Bundlephobia",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.82.5",
    "axios": "^0.26.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^5.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
