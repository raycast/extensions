{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "manotori",
  "title": "Manotori",
  "description": "Manage Domains in Manotori",
  "icon": "manotori.png",
  "author": "xmok",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "api_token",
      "title": "API Token",
      "type": "password",
      "required": true,
      "placeholder": "api-xxx...xxx",
      "description": "API Token from Manotori Dashboard"
    }
  ],
  "commands": [
    {
      "name": "view-domains",
      "title": "View Domains",
      "subtitle": "Manotori",
      "description": "View Domains",
      "mode": "view"
    },
    {
      "name": "view-dns-zones",
      "title": "View DNS Zones",
      "subtitle": "Manotori",
      "description": "View DNS Zones",
      "mode": "view"
    },
    {
      "name": "view-nameserver-sets",
      "title": "View Nameserver Sets",
      "subtitle": "Manotori",
      "description": "View Nameserver Sets",
      "mode": "view"
    }
  ],
  "platforms": [
    "macOS",
    "Windows"
  ],
  "dependencies": {
    "@raycast/api": "^1.102.6",
    "@raycast/utils": "^2.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
