{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lucky-surf",
  "title": "Lucky Surf",
  "description": "Say goodbye to scrolling through endless search results â€“ simply trust your intuition and let serendipity guide you to the information you need, swiftly and efficiently",
  "icon": "extension-icon.png",
  "author": "popalay",
  "contributors": [],
  "categories": [
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
