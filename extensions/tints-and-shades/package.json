{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tints-and-shades",
  "title": "Tints and Shades",
  "description": "Generates tints and shades from a hex color value",
  "icon": "command-icon.png",
  "author": "anu",
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Tints & Shades",
      "description": "Tints and Shades generator",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "got": "^13.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
