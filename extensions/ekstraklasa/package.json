{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ekstraklasa",
  "title": "Ekstraklasa",
  "description": "Stay up to date with the polish football league",
  "icon": "ekstraklasa-logo.png",
  "author": "szarbartosz",
  "categories": [
    "Fun",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "table",
      "title": "Table",
      "description": "View the latest Ekstraklasa standings",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
