{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "include": [
    "src/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "ESNext"
    ],
    "module": "ESNext",
    "target": "ESNext",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "moduleResolution": "Node",
    "resolveJsonModule": true
  }
}
