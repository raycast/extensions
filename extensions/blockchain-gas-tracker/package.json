{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "blockchain-gas-tracker",
  "title": "Blockchain Gas Tracker",
  "description": "Track and caculate blockchain gas fees right in your Raycast!",
  "icon": "gas_icon.png",
  "author": "Vonernue",
  "categories": [
    "Productivity",
    "Finance",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "currentGas",
      "title": "Show Current Gas",
      "description": "Show current blochchain gas fee",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "@raycast/utils": "^1.5.2",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/node-fetch": "^2.6.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
