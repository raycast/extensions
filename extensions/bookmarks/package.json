{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bookmarks",
  "title": "Craftled Bookmarks Raycast Extension",
  "description": "A Raycast extension for managing bookmarks",
  "icon": "icon.png",
  "author": "thomas",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-bookmark",
      "title": "Create Bookmark",
      "description": "Create a new bookmark",
      "mode": "view"
    },
    {
      "name": "search-bookmarks",
      "title": "Search Bookmarks",
      "description": "Search for bookmarks",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "latest",
    "@raycast/utils": "^1.4.7",
    "email-validator": "^2.0.4",
    "nanoid": "^5",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
