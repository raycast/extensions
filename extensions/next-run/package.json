{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "next-run",
  "title": "Next Run",
  "description": "Spin up a Next.js project from Raycast",
  "icon": "extension_icon.png",
  "author": "jonathangiardino",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create New Next.js Project",
      "description": "Create New Next.js Project From Raycast",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "githubToken",
      "title": "Github Token",
      "description": "Create a Github Personal Access Token and paste it here.",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.16",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "platforms": [
    "macOS"
  ]
}
