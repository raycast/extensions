{
	"name": "requesty-chat",
	"title": "Requesty Chat",
	"description": "Chat with AI models through Requesty.ai",
	"icon": "icon.png",
	"author": "thibault_jaigu",
	"categories": [
		"Developer Tools",
		"Productivity"
	],
	"license": "MIT",
	"commands": [
		{
			"name": "index",
			"title": "Requesty Chat",
			"description": "Chat with AI models through Requesty.ai",
			"mode": "view"
		}
	],
	"dependencies": {
		"@raycast/api": "^1.55.2",
		"node-fetch": "^2.7.0",
		"react-router-dom": "^7.1.1",
		"sharp": "^0.33.5"
	},
	"devDependencies": {
		"@raycast/eslint-config": "^1.0.11",
		"@raycast/utils": "^1.18.1",
		"@types/node": "~20.8.0",
		"@types/node-fetch": "^2.6.12",
		"@types/react": "^18.2.21",
		"@typescript-eslint/eslint-plugin": "^6.7.0",
		"@typescript-eslint/parser": "^6.7.0",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"prettier": "^3.4.2",
		"typescript": "^5.2.2"
	},
	"scripts": {
		"build": "ray build -e dist",
		"dev": "ray develop",
		"fix-lint": "ray lint --fix",
		"lint": "ray lint",
		"publish": "npx @raycast/api@latest publish",
		"format": "prettier --write 'src/**/*.{ts,tsx}'"
	},
	"preferences": [
		{
			"name": "apiKey",
			"title": "Requesty API Key",
			"description": "Your Requesty Router API key",
			"type": "password",
			"required": true
		}
	]
}
