{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "useless-facts",
  "title": "Useless Facts",
  "description": "Get useless, but true facts",
  "icon": "useless-facts.png",
  "author": "xmok",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "random-useless-fact",
      "title": "Random Useless Fact",
      "description": "Get a Random Useless Fact",
      "mode": "view"
    },
    {
      "name": "today-s-useless-fact",
      "title": "Today's Useless Fact",
      "description": "Get Today's Useless Fact",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
