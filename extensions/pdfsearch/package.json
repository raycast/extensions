{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pdfsearch",
  "title": "PDFSearch",
  "description": "Search across collections of PDF documents.",
  "icon": "command-icon.png",
  "author": "kevin-pek",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "collections",
      "title": "Collections",
      "description": "View list of document collections.",
      "mode": "view"
    },
    {
      "name": "open",
      "title": "View Outline",
      "description": "View outline for a single PDF file.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.1",
    "@raycast/utils": "^1.10.1",
    "execa": "^8.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist -o dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
