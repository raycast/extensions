{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "diskutil-mac",
  "title": "View your disks and volumes",
  "description": "An implementation of the macOS 'diskutil' command with quicker access to features, providing a better overview and a much more userfriendly interface",
  "icon": "extension-icon.png",
  "author": "Globolo",
  "categories": [
    "Developer Tools",
    "Productivity",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "diskutil-mac",
      "description": "View diskutil command in raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.0",
    "sudo-prompt": "^9.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "chai": "^4.3.7",
    "eslint": "^7.32.0",
    "mocha": "^10.2.0",
    "prettier": "^2.5.1",
    "ts-mocha": "^10.0.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "test": "mocha",
    "publish": "npx @raycast/api@latest publish"
  }
}
