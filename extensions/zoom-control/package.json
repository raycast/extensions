{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "zoom-meeting-control",
  "title": "Zoom Meeting Control",
  "description": "Control current Zoom meeting",
  "icon": "zoom-control.png",
  "author": "prashantv",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "icon": "zoom-mute.png",
      "name": "mute",
      "title": "Mute Meeting",
      "subtitle": "Zoom",
      "description": "Mutes current Zoom meeting",
      "mode": "no-view"
    },
    {
      "icon": "zoom-unmute.png",
      "name": "unmute",
      "title": "Unmute Meeting",
      "subtitle": "Zoom",
      "description": "Unmutes current Zoom meeting",
      "mode": "no-view"
    },
    {
      "icon": "zoom-video-on.png",
      "name": "video-on",
      "title": "Start Video",
      "subtitle": "Zoom",
      "description": "Starts video in current Zoom meeting",
      "mode": "no-view"
    },
    {
      "icon": "zoom-video-off.png",
      "name": "video-off",
      "title": "Stop Video",
      "subtitle": "Zoom",
      "description": "Stops video in current Zoom meeting",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.10.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^3.0.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
