  "devDependencies": {
    "@raycast/eslint-config": "1.0.8",
    "@types/isomorphic-fetch": "^0.0.39",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "20.13.0",
    "@types/ramda": "^0.30.0",
    "@types/react": "18.3.3",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.3.0",
    "ts-jest": "^29.1.4",
