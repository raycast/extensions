{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "surfs-up",
  "title": "Surfs Up",
  "description": "Search the weather and the ocean conditions in your favorite surfing spots",
  "icon": "logo.png",
  "author": "comoser",
  "keywords": ["surf", "weather", "ocean", "skim", "sea", "forecast"],
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Spot Forecast",
      "description": "Search the weather and the ocean conditions in your favorite surfing spots. We'll let you know if it's good for surfing!",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.1",
    "@raycast/utils": "^1.5.2",
    "flag-icons": "^6.6.6",
    "left-pad": "^1.3.0",
    "wind-scrape": "github:jeroentvb/wind-scrape#dist"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
