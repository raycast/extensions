{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "traggo",
  "title": "Traggo",
  "description": "Third-party extension to log time with tags using traggo",
  "icon": "extension-icon.png",
  "author": "tensor",
  "categories": [
    "Productivity",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "trackCommand",
      "title": "Track",
      "subtitle": "Start a new timer with tags",
      "description": "Describe what you do using tags and start a timer",
      "mode": "view"
    },
    {
      "name": "stopCommand",
      "title": "Stop Timer",
      "subtitle": "Stop a previously started timer",
      "description": "Stop a running timer",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@apollo/client": "^3.7.11",
    "@raycast/api": "^1.49.3",
    "cross-fetch": "^3.1.5",
    "date-fns": "^2.29.3",
    "graphql": "^16.6.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^3.3.0",
    "@graphql-codegen/near-operation-file-preset": "^2.5.0",
    "@graphql-codegen/typescript": "^3.0.3",
    "@graphql-codegen/typescript-operations": "^3.0.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.7",
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "codegen": "graphql-codegen"
  },
  "preferences": [
    {
      "name": "url",
      "description": "The URL of your traggo instance",
      "type": "textfield",
      "required": true,
      "title": "URL",
      "placeholder": "https://traggo.net"
    },
    {
      "name": "username",
      "description": "The username you use to login at traggo",
      "type": "textfield",
      "required": true,
      "title": "Username",
      "placeholder": "admin"
    },
    {
      "name": "password",
      "description": "The password you use to login at traggo",
      "type": "password",
      "required": true,
      "title": "Password"
    }
  ]
}
