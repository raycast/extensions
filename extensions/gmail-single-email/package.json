{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gmail-single-email",
  "title": "Gmail - Single Email",
  "description": "View the single latest email from your Gmail inbox",
  "version": "1.0.0",
  "icon": "gmail.png",
  "author": "tonka3000",
  "contributors": [
    "darksoothingshadow"
  ],
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "single-email",
      "title": "Single Email",
      "subtitle": "Gmail",
      "description": "Get the single latest email from inbox",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "clientid",
      "description": "Client ID",
      "type": "textfield",
      "required": true,
      "title": "OAuth Client ID"
    }
  ],
  "dependencies": {
    "@googleapis/gmail": "^5.0.0",
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.10.0",
    "cross-fetch": "^4.0.0",
    "googleapis": "^123.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^3.0.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}