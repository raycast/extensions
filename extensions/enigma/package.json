{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "enigma",
  "title": "Enigma - Convert values",
  "description": "Convert values between encodings",
  "categories": [
    "Data",
    "Developer Tools",
    "Productivity"
  ],
  "icon": "command-icon.png",
  "author": "cheeseburgoor",
  "license": "MIT",
  "commands": [
    {
      "name": "hex-to-decimal",
      "title": "Convert Hex to Decimal",
      "description": "hex ➡️ decimal",
      "mode": "view"
    },
    {
      "name": "string-to-hex",
      "title": "Convert String to Hex",
      "description": "string ➡️ hex",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@ethersproject/bignumber": "^5.6.0",
    "@raycast/api": "^1.30.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
