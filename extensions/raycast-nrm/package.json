{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-nrm",
  "title": "NPM Registry Manager",
  "description": "change the npm registry address",
  "icon": "nrm-icon.png",
  "author": "lihzsky",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "main",
      "title": "Nrm",
      "description": "change the npm registry address",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/request": "^8.1.5",
    "@raycast/api": "^1.62.0",
    "cross-fetch": "^4.0.0",
    "execa": "^8.0.1",
    "shell-env": "^4.0.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
