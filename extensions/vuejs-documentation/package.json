{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vuejs-documentation",
  "title": "Searching Vue.js Documentation",
  "description": "Search VueJS documentation (v3)",
  "icon": "vue-icon.png",
  "author": "DavidPascaud",
  "contributors": [
    "Lemon"
  ],
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Vue.js",
      "description": "Search Vue.js documentation from Raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.98.2",
    "algoliasearch": "^5.24.0",
    "lodash.groupby": "^4.6.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@types/lodash.groupby": "^4.6.6",
    "@types/node": "^22.15.17",
    "@types/react": "^19.1.3",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
