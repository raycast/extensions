{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "productlane",
  "title": "Productlane",
  "description": "Quickly add insights to Productlane",
  "icon": "command-icon.png",
  "author": "dimagrossman",
  "contributors": [
    "pernielsentikaer",
    "daniel_hougaard"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Submit Insight",
      "subtitle": "Productlane",
      "description": "Quickly submit an insight to Productlane",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.4",
    "@raycast/utils": "^1.5.2",
    "axios": "^1.3.4"
  },
  "preferences": [
    {
      "name": "apiKey",
      "type": "password",
      "required": true,
      "title": "API Key",
      "description": "Your Productlane API key.",
      "placeholder": "See https://productlane.io/settings/api"
    },
    {
      "name": "email",
      "description": "Default email used for insights",
      "type": "textfield",
      "required": true,
      "title": "Default Email",
      "placeholder": "your@email.com"
    }
  ],
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish",
    "prettier": "prettier --write src"
  }
}
