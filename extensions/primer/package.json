{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "primer",
  "title": "Primer",
  "description": "Search and navigate GitHub's Primer Design System",
  "icon": "command-icon.png",
  "author": "cole",
  "categories": [
    "Documentation",
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Primer",
      "description": "Search and navigate GitHub's Primer Design System",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.8",
    "@raycast/utils": "^1.5.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
