{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bibmanager",
  "title": "Bibmanager",
  "description": "Search Bibmanager Database from Raycast",
  "icon": "command-icon.png",
  "author": "aaronschneider",
  "version": "1.1.0",
  "license": "MIT",
  "categories": ["Productivity","Applications"],
  "commands": [
    {
      "name": "index",
      "title": "Search Papers",
      "description": "Search bibmanager",
      "subtitle": "bibmanager",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "python",
      "type": "textfield",
      "required": true,
      "title": "Location of python bin",
      "description": "Run 'which python' in the terminal to find out.",
      "default": "/usr/bin/python",
      "placeholder": "/usr/bin/python"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "bibtex": "^0.9.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
