{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "codegrepper",
  "title": "Code Grepper",
  "description": "Get code examples instantly",
  "icon": "codegrepper.png",
  "author": "emircanerkul",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Get Code Examples",
      "description": "List and a detail view of code examples",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.43.2",
    "@raycast/utils": "^1.4.11",
    "axios": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}