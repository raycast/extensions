{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "version": "1.0.10",
  "name": "raycast-translate-ge",
  "title": "Translate.ge",
  "description": "Raycast plugin for translating georgian words to english",
  "icon": "translate-logo.png",
  "author": "giokaxo",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Translate Word",
      "description": "Georgian Translation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.1",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}