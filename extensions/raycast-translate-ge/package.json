{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "version": "1.0.16",
  "name": "raycast-translate-ge",
  "title": "Translate.ge",
  "description": "Raycast plugin for translating georgian words to english",
  "icon": "translate-logo.png",
  "author": "giokaxo",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Translate Word",
      "description": "Georgian Translation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.66.0",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}