{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "typescript-documentation-search",
  "title": "TypeScript Documentation Search",
  "description": "Search in TypeScript Documentation",
  "icon": "command-icon.png",
  "author": "chezhe",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "TypeScript",
      "description": "Search in TypeScript Documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "lodash.unescape": "^4.0.1",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/lodash.unescape": "^4.0.6",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
