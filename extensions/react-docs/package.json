{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "react-docs",
  "title": "React Documentation",
  "description": "Quick links to the new React documentation",
  "icon": "icon.png",
  "author": "webdiego",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Documentation",
      "subtitle": "React",
      "description": "Quick links to the new React documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
