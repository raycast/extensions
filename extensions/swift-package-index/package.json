{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "swift-package-index",
  "title": "Swift Package Index",
  "description": "Search for Swift Packages via https://swiftpackageindex.com",
  "icon": "command-icon.png",
  "author": "maximkrouk",
  "contributors": [
    "japanese-goblinn"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Packages",
      "subtitle": "Swift Package Index",
      "description": "Search for Swift Packages via https://swiftpackageindex.com",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.0",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
