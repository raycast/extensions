{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "domainr",
  "title": "Domainr",
  "description": "Search domains availability with Domainr",
  "icon": "icon.png",
  "author": "fedevitaledev",
  "license": "MIT",
  "commands": [
    {
      "name": "domainr",
      "title": "Search Domain",
      "subtitle": "Domainr",
      "description": "Search domains availability with Domainr.",
      "mode": "view",
      "preferences": [
        {
          "name": "rapidApiKey",
          "type": "password",
          "required": true,
          "title": "RapidApi Key",
          "description": "Your account apiKey",
          "link": "https://rapidapi.com/domainr/api/domainr"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.1",
    "axios": "~0.21.1",
    "axios-cache-adapter": "^2.7.3",
    "fp-ts": "^2.11.5",
    "io-ts": "^2.2.16",
    "ts-pattern": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
