{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "matter",
  "title": "Matter",
  "description": "View your saved for later articles in your Matter account.",
  "icon": "logo.png",
  "author": "zan",
  "categories": [
    "Media",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Matter",
      "description": "View your saved for later articles in your Matter account.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.1",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/whatwg-fetch": "^0.0.33",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "preferences": [
    {
      "name": "matterToken",
      "title": "Matter Token",
      "description": "Your Matter token",
      "type": "textfield",
      "required": true
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
