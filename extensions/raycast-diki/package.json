{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-diki",
  "title": "Diki Translate",
  "description": "Diki Translate with autosuggestions",
  "icon": "command-icon.png",
  "author": "blazej_kustra",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Diki Translate",
      "description": "Diki Translate with autosuggestions",
      "mode": "view",
      "keywords": [
        "diki",
        "translate",
        "suggestions",
        "autocomplete"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "node-fetch": "^3.1.0",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
