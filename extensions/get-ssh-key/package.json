{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "get-ssh-key",
  "title": "Get SSH Key",
  "description": "Get your local SSH key in a split of a second.",
  "icon": "command-icon.png",
  "author": "Theon",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "pernielsentikaer",
    "j3lte"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Copy SSH Key",
      "description": "Get your local SSH key",
      "mode": "no-view"
    },
    {
      "name": "list",
      "title": "List SSH Key",
      "description": "Get a list of your local SSH key",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.76.0",
    "@raycast/utils": "^1.15.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx -y @raycast/api@latest publish"
  }
}
