{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "get-ssh-key",
  "title": "Get SSH Key",
  "description": "Get your local SSH key in a split of a second.",
  "icon": "command-icon.png",
  "author": "Tang",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "pernielsentikaer"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Copy SSH Key",
      "description": "Get your local SSH key",
      "mode": "no-view"
    },
    {
      "name": "list",
      "title": "List SSH Key",
      "description": "Get a list of your local SSH key",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.4"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
