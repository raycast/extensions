{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tureng-translate",
  "title": "Tureng Translate",
  "description": "Translate a word or phrases using tureng.com",
  "icon": "icon.png",
  "author": "berkaygure",
  "categories": [
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "translate",
      "title": "Translate",
      "description": "Translate a word or phrases using tureng.com",
      "mode": "view",
      "subtitle": "Tureng"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.13.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
