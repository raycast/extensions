{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dbt-documentation",
  "title": "dbt Documentation Search",
  "description": "Search the dbt documentation website https://docs.getdbt.com/",
  "icon": "dbt-icon.png",
  "author": "b-per",
  "license": "MIT",
  "categories": ["Documentation"],
  "commands": [
    {
      "name": "index",
      "title": "Search dbt Documentation",
      "description": "Search dbt documentation from Raycast.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.1",
    "algoliasearch": "^4.11.0",
    "prettier": "^2.5.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}