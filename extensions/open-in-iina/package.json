{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "open-in-iina",
  "description": "Open any video URL in your clipboard directly in IINA",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "author": "yeskunall",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "dependencies": {
    "@raycast/api": "^1.71.4",
    "run-applescript": "^7.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "icon": "icon.png",
  "commands": [
    {
      "name": "index",
      "title": "Open in IINA",
      "description": "Opens latest video (from clipboard) directly in IINA",
      "mode": "no-view"
    }
  ],
  "title": "Open in IINA"
}
