{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ember-api-documentation",
  "title": "Ember.js API Documentation",
  "description": "Search Ember.js Api Documentation and open it in the browser",
  "icon": "command-icon.png",
  "author": "tdwesten",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Ember.js",
      "description": "Search documentation and open it in the browser",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "@raycast/utils": "^1.4.8",
    "ts-json-api": "^1.1.3"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
