{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aimlab",
  "title": "Aimlab Tracker",
  "description": "Track player profiles and leaderboards on Aimlab",
  "icon": "command-icon.png",
  "author": "Whitespace",
  "categories": [
    "Web",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "view-leaderboards",
      "title": "View Leaderboards",
      "description": "Fetch leaderboards",
      "mode": "view"
    },
    {
      "name": "view-profile",
      "title": "View Profile",
      "description": "View your Aimlab profile",
      "mode": "view"
    },
    {
      "name": "view-creators",
      "title": "View Top Creators",
      "description": "View Aimlab Top Creators",
      "mode": "view"
    },
    {
      "name": "view-tasks",
      "title": "Search Tasks",
      "description": "Search Tasks",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "username",
      "type": "textfield",
      "title": "Aimlab username",
      "placeholder": "Please enter your Aimlab username",
      "required": true,
      "default": "",
      "description": "The username provided will be stored for easy access to your Aimlab profile."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.54.0",
    "@raycast/utils": "^1.1.0",
    "axios": "1.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}