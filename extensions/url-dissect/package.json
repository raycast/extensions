{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "url-dissect",
  "title": "Url Dissect",
  "description": "The URL Parser Tool efficiently breaks down URLs, providing details such as domain, protocol, and path.",
  "icon": "command-icon.png",
  "author": "NicolasMontone",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Url Dissect",
      "description": "Disect the URL from your clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}

