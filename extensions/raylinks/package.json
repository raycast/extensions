{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raylinks",
  "title": "Raylinks - Quick access to links",
  "description": "Blazing fast access to your most used links.",
  "icon": "command-icon.png",
  "author": "GLaDO8",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "browse-links",
      "title": "Browse Links",
      "subtitle": "Raylinks",
      "description": "Quickly browse and copy links to your clipboard.",
      "mode": "view"
    },
    {
      "name": "add-links",
      "title": "Add Links",
      "subtitle": "Raylinks",
      "description": "Add links for quick access.",
      "mode": "view"
    }
  ],
  "keywords": [
    "raylinks",
    "social links"
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "nanoid": "^3.3.2",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/uuid": "^8.3.4",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
