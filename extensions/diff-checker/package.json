{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "diff-checker",
  "title": "Diff Checker",
  "description": "Compare content of two texts",
  "icon": "icon.png",
  "author": "destiner",
  "categories": ["Data", "Developer Tools"],
  "license": "MIT",
  "commands": [
    {
      "name": "check-diff",
      "title": "Check Diff",
      "description": "Compare files line-by-line",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "diff": "^5.0.0"
  },
  "devDependencies": {
    "@types/diff": "^5.0.2",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
