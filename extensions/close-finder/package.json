{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "close-finder",
  "title": "Close Finder",
  "description": "Utils to close Finder from Raycast.",
  "icon": "command-icon.png",
  "author": "qeude",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "close-windows",
      "title": "Close Finder Windows",
      "description": "Closes all opened Finder windows without quitting the app.",
      "mode": "no-view"
    },
    {
      "name": "kill-finder",
      "title": "Kill Finder",
      "description": "Kill Finder.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
