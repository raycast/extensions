{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stretchly",
  "title": "Stretchly",
  "description": "Fastest way to skip to breaks.",
  "icon": "stretchly.png",
  "author": "u-ran",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "skip-to-next-mini-break",
      "title": "Skip to Next Mini Break",
      "subtitle": "Stretchly",
      "description": "Skip to Next Mini Break",
      "mode": "no-view"
    },
    {
      "name": "skip-to-next-long-break",
      "title": "Skip to Next Long Break",
      "subtitle": "Stretchly",
      "description": "Skip to Next Mini Break",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3"
  },
  "devDependencies": {
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
