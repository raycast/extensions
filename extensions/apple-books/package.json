{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "apple-books",
  "title": "Apple Books",
  "description": "Search Apple Books from Raycast",
  "icon": "icon.png",
  "author": "vimtor",
  "categories": [
    "Productivity",
    "Data",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Books",
      "description": "Search books and audiobooks in Apple Books",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.5.2",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
