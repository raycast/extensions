{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "regex-repl",
  "title": "RegEx REPL",
  "description": "Quickly write and test Regular Expressions (RegEx)",
  "icon": "command-icon.png",
  "author": "hongkiulam",
  "contributors": [
    "wxiaoyun"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "create-and-test-regex",
      "title": "Create And Test RegEx",
      "description": "Create and test a Regular Expression",
      "mode": "view",
      "preferences": [
        {
          "name": "use-last-used-pattern",
          "label": "Use last used pattern",
          "description": "Use the last used pattern as the default pattern",
          "type": "checkbox",
          "default": false,
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.96.0",
    "@raycast/utils": "^1.19.1",
    "regexpp": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
