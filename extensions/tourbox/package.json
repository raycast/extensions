{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tourbox",
  "title": "TourBox",
  "type": "module",
  "description": "Find Your Desired TourBox Preset",
  "icon": "tourbox-icon.png",
  "author": "litomore",
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Fun",
    "Media",
    "Productivity"
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search TourBox Presets",
      "description": "Find Your Desired Preset Below",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.83.2",
    "got": "^14.4.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.4",
    "@types/react": "^18.3.10",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2"
  }
}
