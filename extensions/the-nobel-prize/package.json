{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "the-nobel-prize",
  "title": "The Nobel Prize",
  "description": "",
  "icon": "command-icon.png",
  "author": "xmok",
  "categories": [
    "Data",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "laureates",
      "title": "Laureates",
      "description": "",
      "mode": "view"
    },
    {
      "name": "nobel-prizes",
      "title": "Nobel Prizes",
      "description": "",
      "mode": "view",
      "arguments": [
        {
          "name": "year",
          "type": "text",
          "placeholder": "YEAR",
          "required": false
        },
        {
          "name": "category",
          "type": "dropdown",
          "placeholder": "CATEGORY",
          "required": false,
          "data": [
            { "title": "Chemistry", "value": "che"},
            { "title": "Economics", "value": "eco"},
            { "title": "Literature", "value": "lit"},
            { "title": "Peace", "value": "pea"},
            { "title": "Physics", "value": "phy"},
            { "title": "Physiology or Medicine", "value": "med"}
          ]
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.71.3",
    "@raycast/utils": "^1.14.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
