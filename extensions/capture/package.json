{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "capture",
  "title": "Capture",
  "description": "Quickly create records in Capture with Raycast.",
  "icon": "command-icon.png",
  "author": "syhchen",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create New Record",
      "description": "Creates a new record in your Capture inbox.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "jwt",
      "type": "password",
      "required": true,
      "title": "JWT",
      "description": "Your Capture JWT",
      "placeholder": "Enter your Capture JWT"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
