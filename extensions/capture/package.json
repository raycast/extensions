{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "capture",
  "title": "Capture",
  "description": "Quickly capture to your Capture Inbox with Raycast.",
  "icon": "command-icon.png",
  "author": "syhchen",
  "owner": "eleventhlabs",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Capture to Inbox",
      "description": "Capture a new Record to the Inbox.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.0",
    "node-fetch": "^3.2.9"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
