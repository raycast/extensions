{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "numi-cli",
  "title": "Numi CLI",
  "description": "Query Numi from raycast using numi-cli interface",
  "icon": "numi-icon.png",
  "author": "viandwi24",
  "contributors": [
    "viandwi24"
  ],
  "categories": [
    "Productivity",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Query Numi",
      "description": "Enter your numi query",
      "mode": "view",
      "arguments": [
        {
          "name": "queryArgument",
          "placeholder": "1+1",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.54.1",
    "@raycast/utils": "^1.8.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}