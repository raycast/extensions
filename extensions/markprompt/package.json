{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "markprompt",
  "title": "Markprompt",
  "description": "Prompt your Markprompt project from Raycast",
  "icon": "extension-icon.png",
  "access": "public",
  "author": "nickrttn",
  "owner": "markprompt",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markprompt/raycast-extension.git"
  },
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Ask",
      "description": "Ask your Markprompt project a question",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "eventsource-parser": "^1.1.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.11.20",
    "@types/react": "18.2.58",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "token",
      "description": "Get a token in the Markprompt Project settings",
      "type": "password",
      "required": true,
      "title": "Project Token"
    }
  ],
  "packageManager": "npm@10.4.0+sha256.0e4e5986526a578a6cdc5647cf862efba03dfcb063672f90bc525433b893fba9"
}
