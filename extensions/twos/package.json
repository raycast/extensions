{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "twos",
  "title": "Twos Post",
  "description": "Add new 'thing' to Twos",
  "icon": "twos-icon.png",
  "author": "ekafyi",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Post to Twos",
      "description": "Add 'thing' to Twos (today or selected date)",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "twosUserId",
      "type": "textfield",
      "required": true,
      "title": "Twos User ID",
      "description": "Your Twos User ID",
      "placeholder": "User ID - sent with twosapp.com request payload"
    },
    {
      "name": "twosToken",
      "type": "password",
      "required": true,
      "title": "Twos Token",
      "description": "Your token",
      "placeholder": "Token - sent with twosapp.com request payload"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.9",
    "@types/react": "18.2.33",
    "eslint": "^8.52.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
