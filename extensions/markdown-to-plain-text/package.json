{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "markdown-to-plain-text",
  "title": "Markdown to Plain Text",
  "description": "Convert Markdown-formatted text to plain text format for quick viewing and editing.",
  "icon": "command-icon.png",
  "author": "skyline-123",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Markdown to Plain Text",
      "description": "Convert Markdown-formatted text to plain text format for quick viewing and editing.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "markdown-it": "^13.0.1",
    "markdown-it-plain-text": "^0.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
