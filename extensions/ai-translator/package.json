{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ai-translator",
  "title": "AI Translator",
  "description": "智能双向翻译工具，自动识别中英文并进行精准翻译",
  "icon": "./command-icon.png",
  "author": "joeseesun",
  "authorURL": "https://twitter.com/vista8",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ai-text-automation",
      "title": "AI Translator",
      "description": "使用AI自动翻译选中的文本",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
