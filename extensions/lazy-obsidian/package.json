{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lazy-obdisian",
  "title": "Lazy Obdisian",
  "description": "Context aware quick capture for Obsidian",
  "icon": "command-icon.png",
  "author": "millin_gabani",
  "owner": "trillhouse",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "capture",
      "title": "Obsidian Smart Capture",
      "description": "Capture whats on your screen into obsidian",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.1",
    "run-applescript": "^6.1.0",
    "yaml": "^2.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
