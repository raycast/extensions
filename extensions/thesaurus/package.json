{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "thesaurus",
  "title": "Thesaurus",
  "description": "Search Merrian-Webster's Thesaurus",
  "icon": "extension_icon.png",
  "author": "ABukSwienty",
  "contributors": [
    "YourMCGeek"
  ],
  "categories": [
    "Web",
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "thesaurus",
      "title": "Thesaurus Search",
      "description": "Thesaurus search",
      "mode": "view"
    },
    {
      "name": "thesaurus-search-selected",
      "title": "Thesaurus Search Selected",
      "description": "Search the thesaurus using the selected text",
      "mode": "view",
      "subtitle": "Thesaurus"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.96.0",
    "@raycast/utils": "^1.19.1",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "^22.14.1",
    "@types/react": "^19.1.2",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-devtools": "^6.1.1",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "platforms": [
    "macOS",
    "Windows"
  ]
}
