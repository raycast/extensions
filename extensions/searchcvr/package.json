{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "searchcvr",
  "title": "SearchCVR",
  "description": "Your personal search engine for danish CVR data.",
  "icon": "cvr-icon.png",
  "author": "gustavs",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "cvr",
      "title": "CVR",
      "description": "Easy and effortless access to danish CVR data without relying on websites.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.3",
    "axios": "^1.3.4",
    "dom-parser": "^0.1.6",
    "domparser": "^0.1.1",
    "fetch": "^1.1.0",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
