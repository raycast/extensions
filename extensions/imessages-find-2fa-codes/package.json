  "title": "Search iMessages & find 2FA codes",
  "description": "Let's you search your iMessages & find 2FA codes directly from Raycast",
  "icon": "icon.png",
  "author": "BalliAsghar",
  "categories": [
    "Communication",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "messages",
      "title": "Search iMessages",
      "description": "Search iMessages & Find 2FA Codes",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.0",
    "@raycast/utils": "^1.4.9"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
