{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "liba-ro_shortener",
  "title": "Liba.ro - URL Shortener",
  "description": "The Libaro powered URL shortener\nShorten your URL in seconds",
  "icon": "logo.png",
  "author": "libaro",
  "categories": [
    "Applications",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Shorten URL",
      "subtitle": "liba.ro",
      "description": "Shorten your URL through the liba.ro URL shortener",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
