{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "copy-gcp-icons",
  "title": "Copy GCP Icons",
  "description": "Copy Product Icons from Google Cloud Platform to Clipboard.",
  "icon": "icon.png",
  "author": "pomdtr",
  "license": "MIT",
  "commands": [
    {
      "name": "copy-gcp-icon",
      "title": "Copy Icon",
      "subtitle": "Google Cloud Platform",
      "description": "Copy Product Icons from Google Cloud Platform",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "primaryAction",
      "description": "Set the default action",
      "type": "dropdown",
      "required": false,
      "title": "Primary Action",
      "data": [
        {
          "title": "Copy PNG Icon",
          "value": "copyPng"
        },
        {
          "title": "Copy SVG Icon",
          "value": "copySvg"
        }
      ],
      "default": "png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.3",
    "download": "^8.0.0",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/download": "^8.0.1",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
