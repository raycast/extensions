{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "g-cloud",
  "title": "Google Cloud Cli",
  "description": "Manage your Google Cloud resources",
  "icon": "icon.png",
  "version": "0.0.45",
  "author": "ojowwalker77",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Manage Google Cloud",
      "description": "Manage your Google Cloud resources",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "gcloudPath",
      "title": "gcloud CLI Path",
      "description": "Path to the gcloud CLI executable",
      "type": "textfield",
      "required": false,
      "default": "gcloud"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2",
    "@raycast/utils": "^1.9.0",
    "dotenv": "^16.4.7",
    "execa": "^5.1.1",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "eslint": "^8.48.0",
    "prettier": "^3.0.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
