{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "storybook-launcher",
  "title": "Storybook Launcher",
  "description": "Browse and search stories across multiple Storybooks and open them in the browser",
  "icon": "storybook-launcher.png",
  "author": "vrn",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Storybook",
      "description": "Browse and launch stories",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0",
    "@raycast/utils": "^1.9.0",
    "nanoid": "^4.0.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
