{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "more-math-commands",
  "title": "More Math Commands",
  "description": "Implements some handy math functions that the built-in calculator lacks",
  "icon": "command-icon.png",
  "author": "yancheng_zhao",
  "categories": [
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Run Math Command",
      "description": "Run some handy math functions that the built-in calculator lacks",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
