{
  "name": "op-secrets",
  "title": "1password Secrets",
  "description": "Retrieve secrets from 1Password vault",
  "icon": "1password-icon.png",
  "author": "stevie_j_huh",
  "license": "MIT",
  "private": true,
  "commands": [
    {
      "name": "index",
      "title": "Search 1password Items",
      "description": "Search and retrieve secrets from 1Password",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.0",
    "op-secrets": "link:"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0",
    "typescript": "^5.1.6",
    "vitest": "^1.0.0"
  },
  "scripts": {
    "dev": "pnpx @raycast/api@latest dev",
    "build": "pnpx @raycast/api@latest build",
    "lint": "pnpx @raycast/api@latest lint",
    "lint:fix": "pnpx @raycast/api@latest lint --fix",
    "prepublish": "pnpm run lint:fix && pnpm run build",
    "publish": "pnpx @raycast/api@latest publish",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  }
}
