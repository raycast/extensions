{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "paper",
  "title": "Paper",
  "description": "Create and organize your ideas, work, etc., in Markdown files by category.",
  "icon": "command-icon.png",
  "author": "luctst",
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Paper List",
      "description": "List all your papers",
      "mode": "view"
    },
    {
      "name": "create-paper",
      "title": "Create New Paper",
      "description": "Create a new paper with categories",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.13.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
