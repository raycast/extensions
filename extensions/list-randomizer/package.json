{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "list-randomizer",
  "title": "List Randomizer",
  "description": "Randomizes a list of items",
  "icon": "command-icon.png",
  "author": "quuu",
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Randomize List",
      "description": "Add items to a list and randomize it\n",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.1",
    "fs": "^0.0.1-security"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
