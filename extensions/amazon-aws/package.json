{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aws",
  "title": "Amazon AWS",
  "description": "Integrate your AWS account in Raycast",
  "icon": "aws-logo.png",
  "author": "Falcon",
  "contributors": [
    "Hodglim",
    "JonathanWbn",
    "gebeto",
    "mommertf",
    "duboiss",
    "lbergantin"
  ],
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ec2",
      "title": "EC2 Instances",
      "subtitle": "Amazon AWS",
      "description": "List and Filter All your EC2 Instances",
      "mode": "view"
    },
    {
      "name": "ecs",
      "title": "ECS Clusters",
      "subtitle": "Amazon AWS",
      "description": "List and Filter All your ECS Clusters",
      "mode": "view"
    },
    {
      "name": "sqs",
      "title": "SQS Queues",
      "subtitle": "Amazon AWS",
      "description": "List your SQS Queues",
      "mode": "view"
    },
    {
      "name": "codepipeline",
      "title": "CodePipeline Pipelines",
      "subtitle": "Amazon AWS",
      "description": "List your CodePipeline Pipelines",
      "mode": "view"
    },
    {
      "name": "console",
      "title": "Console",
      "subtitle": "Amazon AWS",
      "description": "Launch AWS console links",
      "mode": "view"
    },
    {
      "name": "cloudformation",
      "title": "CloudFormation Stacks",
      "subtitle": "Amazon AWS",
      "description": "List your CloudFormation Stacks",
      "mode": "view"
    },
    {
      "name": "lambda",
      "title": "Lambda Functions",
      "subtitle": "Amazon AWS",
      "description": "List your Lambda Functions",
      "mode": "view"
    },
    {
      "name": "dynamodb",
      "title": "DynamoDB Tables",
      "subtitle": "Amazon AWS",
      "description": "List your DynamoDB Tables",
      "mode": "view"
    },
    {
      "name": "s3",
      "title": "S3 Buckets",
      "subtitle": "Amazon AWS",
      "description": "List your S3 Buckets",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.216.0",
    "@aws-sdk/client-codepipeline": "^3.216.0",
    "@aws-sdk/client-dynamodb": "^3.216.0",
    "@aws-sdk/client-ec2": "^3.216.0",
    "@aws-sdk/client-ecs": "^3.216.0",
    "@aws-sdk/client-lambda": "^3.216.0",
    "@aws-sdk/client-s3": "^3.216.0",
    "@aws-sdk/client-sqs": "^3.216.0",
    "@aws-sdk/client-ssm": "^3.218.0",
    "@aws-sdk/shared-ini-file-loader": "^3.215.0",
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.4.9"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
