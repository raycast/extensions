{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "author": "pondorasti",
  "license": "MIT",
  "categories": [
    "Fun",
    "Communication",
    "Media"
  ],
  "keywords": [
    "emoji",
    "genmoji"
  ],
  "icon": "extension-icon.png",
  "name": "emojis-com",
  "title": "emojis.com",
  "description": "Search emojis on emojis.com",
  "commands": [
    {
      "name": "search-emojis",
      "title": "Search Emojis",
      "description": "Search images from emojis.com and copy them to clipboard",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "gridColumns",
      "type": "dropdown",
      "title": "Grid Columns",
      "description": "Number of columns to display in the grid",
      "default": "8",
      "required": false,
      "data": [
        {
          "title": "3",
          "value": "3"
        },
        {
          "title": "5",
          "value": "5"
        },
        {
          "title": "8",
          "value": "8"
        },
        {
          "title": "12",
          "value": "12"
        }
      ]
    }
  ],
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "lint:fix": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  },
  "dependencies": {
    "@imgproxy/imgproxy-js-core": "^1.4.0",
    "@imgproxy/imgproxy-node": "^1.1.0",
    "@raycast/api": "^1.102.3",
    "@raycast/utils": "^1.17.0",
    "@tanstack/react-query": "^5.84.1",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  }
}
