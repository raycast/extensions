{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tabler",
  "title": "Tabler",
  "description": "Copy & Paste Tabler Icons",
  "icon": "extension_icon.png",
  "author": "zavbala",
  "categories": [
    "Web",
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "mode": "view",
      "name": "index",
      "title": "Show Tabler Icons",
      "description": "Take a look at all available icons"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "axios": "^1.5.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "lint": "ray lint",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "build": "ray build -e dist",
    "publish": "npx @raycast/api@latest publish"
  }
}
