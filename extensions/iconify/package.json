{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "iconify",
  "title": "Iconify â€” Search Icons",
  "description": "Search icons via Iconify",
  "icon": "icon.png",
  "author": "destiner",
  "contributors": [
    "sxn",
    "robert",
    "kuddl",
    "kevinxo328",
    "ridemountainpig",
    "pernielsentikaer"
  ],
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  },
  "commands": [
    {
      "name": "view-icons",
      "title": "View Icons",
      "subtitle": "Iconify",
      "description": "View available icons",
      "mode": "view"
    },
    {
      "name": "search-icons",
      "title": "Search All Icons",
      "subtitle": "Iconify",
      "description": "Search through all available icons",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "primaryAction",
      "type": "dropdown",
      "required": false,
      "title": "Primary action",
      "description": "Primary action to use",
      "default": "paste",
      "data": [
        {
          "title": "Paste <svg> to Active App",
          "value": "paste"
        },
        {
          "title": "Copy <svg> to Clipboard",
          "value": "copy"
        },
        {
          "title": "Paste Svg File to Active App",
          "value": "pasteFile"
        },
        {
          "title": "Copy Svg File to Clipboard",
          "value": "copyFile"
        },
        {
          "title": "Paste <name> to Active App",
          "value": "pasteName"
        },
        {
          "title": "Copy <name> to Clipboard",
          "value": "copyName"
        },
        {
          "title": "Copy Svg URL to Clipboard",
          "value": "copyURL"
        },
        {
          "title": "Copy Data URI to Clipboard",
          "value": "copyDataURI"
        }
      ]
    },
    {
      "name": "iconColor",
      "type": "dropdown",
      "required": false,
      "title": "Monochromatic Icon Color",
      "description": "Icon color to use in monochromatic icons",
      "default": "currentColor",
      "data": [
        {
          "title": "Default",
          "value": "currentColor"
        },
        {
          "title": "Custom",
          "value": "customColor"
        }
      ]
    },
    {
      "name": "customColor",
      "type": "textfield",
      "required": false,
      "title": "Custom Icon Color",
      "description": "Custom icon color to use in icons",
      "default": "#ffffff"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.0",
    "@raycast/utils": "^1.18.0",
    "axios": "^1.8.4",
    "react-hooks-global-state": "^2.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "platforms": [
    "macOS"
  ]
}
