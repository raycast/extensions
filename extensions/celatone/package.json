{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "celatone",
  "title": "Celatone",
  "description": "Search open links for address, contracts, blocks, transactions, and more on Celatone instances.",
  "icon": "celatone.png",
  "author": "alleslabs",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list-explorers",
      "title": "List Explorers",
      "description": "List all available Celatone instances",
      "mode": "view"
    },
    {
      "name": "search",
      "title": "Search",
      "description": "Search for address, contracts, blocks, transactions, and more on Celatone instances.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "axios": "^1.6.7",
    "lodash.debounce": "^4.0.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/lodash.debounce": "^4.0.9",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
