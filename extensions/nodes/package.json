{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "solana_nodes",
  "title": "Solana Nodes",
  "description": "Browse Solana Validator Stats from Stakewiz",
  "icon": "extension-logo.png",
  "author": "jstnw10",
  "categories": [
    "Data",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "menuBarStats",
      "title": "Solana Validator",
      "description": "Displays Validator Stats",
      "mode": "menu-bar"
    },
    {
      "name": "changeValidator",
      "title": "Change Validator",
      "subtitle": "Solana Validator",
      "description": "Change the validator to display stats for",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.4.8",
    "lucide-react": "^0.363.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
