{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "excel-to-markdown",
  "title": "Excel to Markdown",
  "description": "Convert Excel (.xlsx) files to Markdown tables",
  "icon": "extension-icon.png",
  "author": "chase_broyles",
  "license": "MIT",
  "commands": [
    {
      "name": "xlsx-to-md",
      "title": "Convert Xlsx to Markdown",
      "description": "Convert an Excel file to a Markdown table",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.91.1",
    "@raycast/utils": "^1.17.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "npm run lint -- --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}