{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "table-flip",
  "title": "Table Flip",
  "description": "Insert a shrug, table flip, or other discord kaomoji anywhere. What can I say? ¯\\_(ツ)_/¯",
  "icon": "command-icon.png",
  "author": "robigan",
  "categories": [
    "Fun",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "shrug",
      "title": "/shrug",
      "subtitle": "¯\\_(ツ)_/¯",
      "description": "Insert a shrug anywhere",
      "mode": "no-view"
    },
    {
      "name": "table-flip",
      "title": "/tableflip",
      "subtitle": "(╯°□°）╯︵ ┻━┻",
      "description": "Insert a table flip anywhere",
      "mode": "no-view"
    },
    {
      "name": "un-flip",
      "title": "/unflip",
      "subtitle": "┬─┬ ノ( ゜-゜ノ)",
      "description": "Insert an no flip anywhere",
      "mode": "no-view"
    },
    {
      "name": "no-flip",
      "title": "/noflip",
      "subtitle": "┬─┬ ノ( ゜-゜ノ)",
      "description": "Insert an no flip anywhere. Alternate command name to /unflip",
      "mode": "no-view",
      "disabledByDefault": true
    }
  ],
  "tools": [],
  "dependencies": {
    "@raycast/api": "^1.76.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}