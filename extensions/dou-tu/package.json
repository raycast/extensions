{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dou-tu",
  "title": "dou tu",
  "description": "An extension for easy retrieval of web emoji images while chatting.(上班时间斗个图🌟)",
  "icon": "icon.png",
  "author": "gaoyang",
  "categories": [
    "Media",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "dou tu",
      "subtitle": "doutu",
      "description": "An extension for easy retrieval of web emoji images while chatting.(上班时间斗个图🌟)",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "select_mode",
      "type": "dropdown",
      "required": true,
      "description": "Way to select images",
      "default": "actions",
      "data": [
        {
          "title": "Select By Enter Key",
          "value": "actions"
        },
        {
          "title": "Select By Click",
          "value": "click"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1",
    "@raycast/utils": "^1.3.0",
    "cheerio": "1.0.0-rc.12",
    "node-fetch": "^3.2.10",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
