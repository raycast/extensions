{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "zalgo-text",
  "title": "Zalgo Text Generator",
  "description": "Translates given text to Zalgo text",
  "icon": "command-icon.png",
  "author": "petermiles",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Generate Zalgo Text",
      "description": "Convert text to Zalgo text",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.4",
    "@raycast/utils": "^1.5.2",
    "to-zalgo": "^1.0.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
