{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "serverless-framework-docs",
  "title": "Serverless Framework Docs",
  "description": "Quickly search through serverless framework documentation and open it in the browser",
  "icon": "serverless.png",
  "author": "yemilgr",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Serverless Framework",
      "description": "Search Serverless Framework Documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
