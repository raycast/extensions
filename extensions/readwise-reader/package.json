{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "readwise-reader",
  "title": "Readwise Reader",
  "description": "Interact with Readwise Reader",
  "icon": "reader-logo.png",
  "author": "zach",
  "categories": [
    "Productivity",
    "Media",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-new",
      "title": "Open New",
      "description": "Opens Reader to new page",
      "mode": "no-view",
      "icon": "reader-logo.png"
    },
    {
      "name": "open-later",
      "title": "Open Later",
      "description": "Opens Reader to the later page",
      "mode": "no-view",
      "icon": "reader-logo.png"
    },
    {
      "name": "open-archive",
      "title": "Open Archive",
      "description": "Opens Reader to the archive page",
      "mode": "no-view",
      "icon": "reader-logo.png"
    },
    {
      "name": "open-home",
      "title": "Open Home",
      "description": "Opens Reader to the home page",
      "mode": "no-view",
      "icon": "reader-logo.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}