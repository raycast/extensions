{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wiggle-text",
  "title": "Text Wiggle",
  "description": "Create wiggly text walls for spamming. Satisfying to scroll through...",
  "icon": "command-icon.png",
  "author": "PineappleRind",
  "categories": [
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Wiggle",
      "description": "Enter your text and it'll be transformed into a text wiggle ready to spam",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
