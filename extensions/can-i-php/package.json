{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "can-i-php",
  "title": "Can I PHP?",
  "description": "Check if a certain function or method is available in different versions of PHP.",
  "icon": "command-icon.png",
  "author": "diana_scharf",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Can I PHP?",
      "description": "Check if a certain function or method is available in different versions of PHP.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.3",
    "@raycast/utils": "^1.13.3",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
