{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "induced-raycast-extension",
  "title": "Induced",
  "description": "A raycast wrapper for Induced's beta APIs",
  "icon": "command-icon.png",
  "author": "induced",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Session",
      "subtitle": "Induced API",
      "description": "Run a new session using Induced's API",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.13.3",
    "got": "^14.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@types/got": "^9.6.12",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
