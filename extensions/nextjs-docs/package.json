{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nextjs-docs",
  "title": "Next.js Documentation",
  "description": "Efficiently search Next.js documentation",
  "icon": "command-icon.png",
  "author": "PrasadChinwal",
  "contributors": [
    "xmok",
    "danieldeichfuss"
  ],
  "categories": [
    "Productivity",
    "Documentation",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Next.js Documentation",
      "description": "Search the Next.js documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/core": "^3.6.0",
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.16.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "@types/yaml-front-matter": "^4.1.0",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^5.2.2",
    "yaml-front-matter": "^4.1.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
