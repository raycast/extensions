{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nextjs-docs",
  "title": "Next.js Documentation",
  "description": "Efficiently search Next.js documentation",
  "icon": "command-icon.png",
  "author": "PrasadChinwal",
  "categories": [
    "Productivity",
    "Documentation",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Next.js Documentation",
      "description": "Search the Next.js documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/core": "^3.6.0",
    "@raycast/api": "^1.32.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/yaml-front-matter": "^4.1.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3",
    "yaml-front-matter": "^4.1.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
