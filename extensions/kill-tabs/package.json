{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "kill-tabs",
  "title": "Kill Tabs",
  "description": "Kills tabs for browsers.",
  "icon": "command-icon.png",
  "author": "sereneinserenade",
  "categories": [
    "Developer Tools",
    "Web",
    "Applications",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "killChromiumTabs",
      "title": "Kill Chromium Tabs",
      "subtitle": "Chromium",
      "description": "Kill All Chromium Tabs",
      "mode": "no-view"
    },
    {
      "name": "killChromeTabs",
      "title": "Kill Chrome Tabs",
      "subtitle": "Chrome",
      "description": "Kill All Chrome Tabs",
      "mode": "no-view"
    },
    {
      "name": "killBraveTabs",
      "title": "Kill Brave Tabs",
      "subtitle": "Brave",
      "description": "Kill All Brave Tabs",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.32.1",
    "kill-tabs": "^4.0.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
