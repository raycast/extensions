{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "cleanshotx",
    "title": "Cleanshot X",
    "description": "Capture and record your screen!",
    "icon": "command-icon.png",
    "author": "Aayush9029",
    "license": "MIT",
    "commands": [{
            "name": "capture-fullscreen",
            "title": "Capture Fullscreen",
            "subtitle": "CleanShot X",
            "description": "Fullscreen Screenshot",
            "mode": "no-view"
        },

        {
            "name": "capture-area",
            "title": "Capture Area",
            "subtitle": "CleanShot X",
            "description": "Selection Tool Capture",
            "mode": "no-view"
        },

        {
            "name": "capture-previous-area",
            "title": "Capture Previous Area",
            "subtitle": "CleanShot X",
            "description": "Previously Selected Area",
            "mode": "no-view"
        },

        {
            "name": "capture-window",
            "title": "Capture Window",
            "subtitle": "CleanShot X",
            "description": "Window Capture",
            "mode": "no-view"
        },

        {
            "name": "self-timer",
            "title": "Self Timer",
            "subtitle": "CleanShot X",
            "description": "Timer Based Screen Capture",
            "mode": "no-view"
        },

        {
            "name": "scrolling-capture",
            "title": "Scrolling Capture",
            "subtitle": "CleanShot X",
            "description": "Scrolling Capture Mode",
            "mode": "no-view"
        },

        {
            "name": "record-screen",
            "title": "Record Screen",
            "subtitle": "CleanShot X",
            "description": "Record Entire Screen",
            "mode": "no-view"
        },

        {
            "name": "text-recognition",
            "title": "Text Recognition",
            "subtitle": "CleanShot X",
            "description": "Text Recognition Using Ocr",
            "mode": "no-view"
        },

        {
            "name": "annotate",
            "title": "Annotate",
            "subtitle": "CleanShot X",
            "description": "Annotate Mode",
            "mode": "no-view"
        },

        {
            "name": "open-from-clipboard",
            "title": "Open from clipboard",
            "subtitle": "CleanShot X",
            "description": "Open Image From Clipboard",
            "mode": "no-view"
        },

        {
            "name": "toggle-desktop-icons",
            "title": "Toggle Desktop icons",
            "subtitle": "CleanShot X",
            "description": "Toggle Desktop Icons Visibility",
            "mode": "no-view"
        },

        {
            "name": "restore-recently-closed",
            "title": "Restore recently closed",
            "subtitle": "CleanShot X",
            "description": "Restore Recently Closed Overlay",
            "mode": "no-view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.26.3",
        "open": "^8.4.0"
    },
    "devDependencies": {
        "@types/node": "~16.10.0",
        "@types/react": "^17.0.28",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "react-devtools": "^4.19.2",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "lint": "ray lint"
    },
    "version": "1.0.0",
    "main": "index.js"
}