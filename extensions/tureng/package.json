{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tureng",
  "title": "Tureng Turkish-English Dictionary",
  "description": "Translate Turkish words to English and the other way around",
  "icon": "command-icon.png",
  "author": "myucesan",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Dictionary",
      "subtitle": "Tureng",
      "description": "Turkish-English dictionary",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.2",
    "random-id": "^1.0.4"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
