{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "yapi",
  "title": "YAPI",
  "description": "YApi's document transform to typescript interfaces",
  "icon": "yapi.png",
  "author": "xiaokyo",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "YApi To Typescript",
      "subtitle": "Transform to Typescript Interfaces",
      "description": "YApi's document transform to typescript interfaces",
      "mode": "view",
      "arguments": [
        {
          "name": "id",
          "placeholder": "id",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "preferences": [
    {
      "name": "yapiHost",
      "description": "YApi Host",
      "type": "textfield",
      "required": true,
      "placeholder": "YApi Host",
      "title": "YApi Host"
    },
    {
      "name": "yapiCookie",
      "description": "YApi Cookie",
      "type": "textfield",
      "required": true,
      "placeholder": "YApi Cookie",
      "title": "YApi Cookie"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.4",
    "@raycast/utils": "^1.8.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}