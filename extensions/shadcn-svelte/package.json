{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shadcn-svelte",
  "title": "Shadcn/Svelte",
  "description": "Browses shadcn/svelte documentation, components, and commands.",
  "icon": "extension-icon.png",
  "author": "selemondev",
  "categories": [
    "Developer Tools",
    "Documentation",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-documentation",
      "title": "Show Documentation",
      "description": "Browses shadcn/svelte documentation.",
      "mode": "view"
    },
    {
      "name": "search-components",
      "title": "Show Components",
      "description": "Browses shadcn/svelte components.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@octokit/rest": "^21.0.0",
    "@raycast/api": "^1.77.1",
    "@raycast/utils": "^1.16.1",
    "octokit": "^4.0.2",
    "ofetch": "^1.3.4"
  },
  "devDependencies": {
    "@octokit/types": "^13.5.0",
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "^5.5.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
