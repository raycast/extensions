{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "naver-search",
  "title": "Naver Search",
  "description": "An extension for Naver search with autosuggestions.",
  "icon": "command-icon.png",
  "author": "kw-lee",
  "license": "MIT",
  "commands": [
    {
      "name": "na-search",
      "title": "Search",
      "subtitle": "Naver",
      "description": "Search Naver for",
      "mode": "view"
    },
    {
      "name": "ns-search",
      "title": "Shopping",
      "subtitle": "Naver",
      "description": "Search Naver Shopping for",
      "mode": "view"
    },
    {
      "name": "nae-search",
      "title": "Endic Search",
      "subtitle": "Naver",
      "description": "Search Naver English dictionary for",
      "mode": "view"
    },
    {
      "name": "nak-search",
      "title": "Krdic Search",
      "subtitle": "Naver",
      "description": "Search Naver Korean dictionary for",
      "mode": "view"
    },
    {
      "name": "nah-search",
      "title": "Handic Search",
      "subtitle": "Naver",
      "description": "Search Naver Hanjs dictionary for",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "i": "^0.3.7"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3",
    "node-fetch": "3.2.0"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
