{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "renaming",
  "title": "Renaming",
  "description": "Renaming is a simple tool to rename files and folders in a batch.",
  "version": "1.0.1",
  "keywords": [
    "rename",
    "finder",
    "file",
    "folder",
    "directory",
    "productivity",
    "utility",
    "system"
  ],
  "icon": "renaming-icon.png",
  "author": "theo_daguier",
  "contributors": [
    "pernielsentikaer",
    "ridemountainpig"
  ],
  "categories": [
    "Productivity",
    "Media",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Rename File(s)",
      "description": "Rename file(s) in a batch.",
      "mode": "view"
    },
    {
      "name": "replace",
      "title": "Replace File(s) Characters",
      "description": "Replace characters in file(s) names.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.12.5"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
