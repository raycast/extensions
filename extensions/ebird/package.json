{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ebird",
  "title": "eBird",
  "description": "Exploring bird species from eBird",
  "icon": "command-icon.png",
  "author": "OozyGrub",
  "categories": [
    "Data",
    "Fun",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "searchBirdTaxons",
      "title": "Search Bird Taxons",
      "description": "Exploring bird species from eBird",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "EBIRD_API_TOKEN",
      "type": "password",
      "required": true,
      "title": "EBIRD_API_TOKEN",
      "description": "Get API Key at https://ebird.org/api/keygen",
      "placeholder": "Get API Key at https://ebird.org/api/keygen"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.1",
    "@raycast/utils": "^1.4.8",
    "axios": "^1.3.4",
    "fuse.js": "^6.6.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
