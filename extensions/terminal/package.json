{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "terminal",
  "title": "Terminal",
  "description": "Control Terminal with Raycast",
  "icon": "command-icon.png",
  "author": "ron-myers",
  "categories": [
    "Applications",
    "Developer Tools",
    "Productivity",
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "new-termninal-window",
      "title": "New Terminal Window",
      "description": "Launch a new window in Terminal",
      "mode": "no-view"
    },
    {
      "name": "new-terminal-tab",
      "title": "New Terminal Tab",
      "description": "Launch a new tab in Terminal",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.43.2",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "platforms": [
    "macOS"
  ]
}
