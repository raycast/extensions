{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cilium-docs",
  "title": "Cilium Docs",
  "description": "An extension for Raycast that let's you access Cilium Docs through Raycast.",
  "icon": "command-icon.png",
  "author": "dario_mader",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Cilium Docs",
      "description": "Search the Cilium Docs",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.0",
    "@raycast/utils": "^1.10.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
