{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "facetime",
  "title": "FaceTime",
  "description": "Make FaceTime calls with Raycast",
  "icon": "facetime.png",
  "author": "ron-myers",
  "license": "MIT",
  "commands": [
    {
      "name": "call",
      "title": "FaceTime Call",
      "description": "Opens FaceTime ready to make a call",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
