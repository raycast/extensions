{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "manga-calendar",
  "title": "Manga Mexico",
  "description": "Show a calendar with the latest manga releases of Mexican editorials.",
  "icon": "command-icon.png",
  "author": "mrolivo",
  "categories": [
    "Fun",
    "News",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Latest Publications",
      "description": "See the current month's releases.",
      "mode": "view"
    },
    {
      "name": "collections",
      "title": "Browse Publications",
      "description": "Browse through all manga collections published in Mexico.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0",
    "@raycast/utils": "^1.9.0",
    "cheerio": "1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
