{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wp-cli-command-explorer",
  "title": "WordPress WP-CLI Commands Explorer",
  "description": "Browse and search WP-CLI commands with their descriptions and subcommands",
  "icon": "command-icon.png",
  "author": "aslamdoctor",
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "wpcli",
      "title": "WordPress WP-CLI Commands",
      "description": "Browse and search WP-CLI commands",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.98.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}