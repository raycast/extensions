{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mockify",
  "title": "Mockify",
  "description": "Converts any text you enter to a sarcastic/mocking text so you can aNnOy yOuR fRiEndS!",
  "icon": "extension_icon.png",
  "author": "Cavallando",
  "categories": [
    "Fun",
    "Communication",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "mock-text",
      "title": "Mock Text",
      "subtitle": "Mock your friends!",
      "description": "Converts any text to a sarcastic/mocking text",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
