{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "re-mind",
  "title": "Re-mind",
  "description": "Re-mind is a curated collection of design-related links compiled by Danilo Campos.",
  "icon": "remind-icon.png",
  "author": "daniloc",
  "contributors": [
    "peduarte"
  ],
  "categories": [
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "view-re-mind",
      "title": "View Links",
      "description": "View, search or filter Re-mind bookmarks.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1",
    "@raycast/utils": "^1.12.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
