{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "image-to-ascii",
  "title": "Image to Ascii",
  "description": "Convert image to Ascii",
  "icon": "extension-icon.png",
  "author": "ridemountainpig",
  "categories": [
    "Applications",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "convert-image-to-ascii",
