{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-icons",
  "title": "Raycast Icons",
  "author": "petr",
  "contributors": [
    "peduarte",
    "xilopaint"
  ],
  "description": "Browse, copy and modify Raycast icons.",
  "icon": "icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Raycast Icons",
      "description": "Browse, copy and modify Raycast icons",
      "mode": "view"
    }
  ],
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@raycast/api": "1.81.0",
    "@raycast/utils": "^1.16.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
