{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-icons",
  "title": "Raycast Icons",
  "author": "petr",
  "contributors": [
    "peduarte",
    "xilopaint"
  ],
  "description": "Browse, copy and modify Raycast icons.",
  "icon": "icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Raycast Icons",
      "description": "Browse, copy and modify Raycast icons",
      "mode": "view"
    }
  ],
  "platforms": [
    "macOS",
    "Windows"
  ],
  "dependencies": {
    "@raycast/api": "1.98.4",
    "@raycast/utils": "^1.19.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.14.1",
    "@types/react": "19.1.2",
    "eslint": "^9.25.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
