{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-icons",
  "title": "Raycast Icons",
  "author": "petr",
  "contributors": [
    "peduarte"
  ],
  "description": "Browse, copy and modify Raycast icons.",
  "icon": "icon.png",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Raycast Icons",
      "description": "Browse, copy and modify Raycast icons",
      "mode": "view"
    }
  ],
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@raycast/api": "1.36.0",
    "lodash": "^4.17.21"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop --target release"
  }
}
