{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "openailatexocr",
  "title": "OpenAI LaTeX OCR",
  "description": "A LaTeX OCR by OpenAI API",
  "icon": "command-icon.png",
  "author": "Arthals",
  "contributors": [
    "FrankMao"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "required": true,
      "name": "token",
      "title": "API Key",
      "description": "API Key from OpenAI",
      "type": "password"
    },
    {
      "required": false,
      "name": "base",
      "title": "Base URL",
      "description": "Base URL",
      "type": "textfield"
    },
    {
      "required": false,
      "name": "model",
      "title": "Model",
      "description": "Model used to OCR",
      "type": "textfield"
    }
  ],
  "commands": [
    {
      "name": "index",
      "title": "OpenAI LaTeX OCR",
      "description": "LaTeX OCR by OpenAI",
      "mode": "no-view"
    },
    {
      "name": "config",
      "title": "OpenAI LaTeX OCR Config",
      "description": "Config for OpenAI LaTeX OCR",
      "mode": "view",
      "icon": "command-icon.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.10.0",
    "@types/axios": "^0.14.0",
    "axios": "^1.5.0",
    "copy-paste": "^1.5.3",
    "form-data": "^4.0.0",
    "openai": "^4.86.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/copy-paste": "^1.1.31",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}