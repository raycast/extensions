{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "roam-research",
  "title": "Roam Research",
  "description": "Roam Research With Raycast",
  "icon": "favicon.png",
  "author": "dive2Pro",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Roam Research",
      "description": "Roam Research",
      "mode": "view"
    },
    {
      "name": "random",
      "title": "Random Note",
      "description": "Roam Research",
      "mode": "view"
    },
    {
      "name": "search",
      "title": "Search Note",
      "description": "Roam Research",
