{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "is-ullrich-raycast-giphy",
  "title": "is.ullrich.raycast.giphy",
  "description": "Giphy Search with Shuffle",
  "icon": "command-icon.png",
  "author": "ullrich",
  "categories": ["Fun"],
  "keywords": ["giphy", "gif", "jif", "search", "shuffle"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Giphy",
      "subtitle": "Giphy Search",
      "description": "Gif or Jif, no matter what you call it. Find it on Jiffy, ehm... Giphy.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "@giphy/js-types": "5.0.0",
    "eslint": "^8.51.0",
    "node-fetch": "^3.1.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
