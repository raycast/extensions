{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "whatsapp",
  "title": "WhatsApp",
  "description": "Open WhatsApp chats",
  "icon": "whatsapp.png",
  "author": "vimtor",
  "license": "MIT",
  "commands": [
    {
      "name": "open-chat",
      "title": "Open Chat",
      "subtitle": "WhatsApp",
      "description": "Opens saved WhatsApp chats",
      "mode": "view"
    },
    {
      "name": "create-chat",
      "title": "Create Chat",
      "subtitle": "WhatsApp",
      "description": "Creates WhatsApp chat in the extension",
      "mode": "view"
    },
    {
      "name": "import-contacts",
      "title": "Import Contacts",
      "subtitle": "WhatsApp",
      "description": "Imports Apple contacts as WhatsApp chats",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.6",
    "run-jxa": "^3.0.0",
    "phone": "^3.1.10"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}