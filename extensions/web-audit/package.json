{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "web-audit",
  "title": "Web Audit",
  "description": "Find SEO related data of any given webpage.",
  "icon": "web-audit_icon.png",
  "author": "flornkm",
  "contributors": [
    "ezzylan"
  ],
  "categories": [
    "Developer Tools",
    "Data",
    "Web"
  ],
  "screenshots": [
    "analyze-website.png",
    "analyze-results.png",
    "info-website.png",
    "info-results.png"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "analyze",
      "title": "Analyze Website",
      "description": "Find SEO related data of any given webpage.",
      "mode": "view",
      "arguments": [
        {
          "name": "url",
          "placeholder": "https://raycast.com",
          "type": "text",
          "identifier": "url",
          "required": true
        }
      ]
    },
    {
      "name": "info",
      "title": "Get Website Info",
      "description": "Find general information about a given webpage.",
      "mode": "view",
      "arguments": [
        {
          "name": "url",
          "placeholder": "https://raycast.com",
          "type": "text",
          "identifier": "url",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.94.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.11",
    "@types/react": "19.0.12",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
