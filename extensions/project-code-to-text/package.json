{
  "name": "project-code-to-text",
  "title": "Project Code to Text",
  "description": "Consolidates project source code, structure, and metadata into a single text file optimized for AI analysis or archiving.",
  "icon": "command-icon.png",
  "author": "borzov",
  "contributors": [
    "ridemountainpig"
  ],
  "license": "MIT",
  "dependencies": {
    "@raycast/api": "^1.97.1",
    "@raycast/utils": "^1.17.0",
    "ignore": "^5.3.1",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "~22.13.10",
    "@types/react": "^19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.2.5",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "commands": [
    {
      "name": "generate-project-code",
      "title": "Generate Project Code for AI",
      "subtitle": "Project Code to Text",
      "description": "Select a project directory to generate a consolidated code file.",
      "mode": "view",
      "icon": "command-icon.png"
    }
  ],
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "keywords": [
    "code",
    "project",
    "ai",
    "consolidation",
    "export",
    "text",
    "analysis"
