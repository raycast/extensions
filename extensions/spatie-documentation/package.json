{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spatie-documentation",
  "title": "Spatie Documentation",
  "description": "Spatie Documentation Search",
  "icon": "spatie-icon.png",
  "author": "danyelkeddah",
  "license": "MIT",
  "contributors": [],
  "commands": [
    {
      "name": "laravel-permission",
      "title": "Laravel Permission",
      "subtitle": "Spatie",
      "description": "Search Spatie Laravel Permission",
      "mode": "view",
      "preferences": [
        {
          "name": "spatieLaravelPermissionVersion",
          "type": "dropdown",
          "required": true,
          "title": "Spatie Laravel Permission Version",
          "description": "Specify the Laravel Permission version it will search the docs for.",
          "default": "v5",
          "data": [
            {
              "title": "v5",
              "value": "v5"
            },
            {
              "title": "v4",
              "value": "v4"
            },
            {
              "title": "v3",
              "value": "v3"
            },
            {
              "title": "v2",
              "value": "v2"
            }
          ]
        }
      ]
    },
    {
      "name": "laravel-backup",
      "title": "Laravel Backup",
      "subtitle": "Spatie",
      "description": "Search Spatie Laravel Backup",
      "mode": "view",
      "preferences": [
        {
          "name": "spatieLaravelBackupVersion",
          "type": "dropdown",
          "required": true,
          "title": "Spatie Laravel Backup Version",
          "description": "Specify the Laravel Backup version it will search the docs for.",
          "default": "v8",
          "data": [
            {
              "title": "v8",
              "value": "v8"
            },
            {
              "title": "v7",
              "value": "v7"
            },
            {
              "title": "v6",
              "value": "v6"
            },
            {
              "title": "v5",
              "value": "v5"
            },
            {
              "title": "v4",
              "value": "v4"
            },
            {
              "title": "v3",
              "value": "v3"
            }
          ]
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.0",
    "algoliasearch": "^4.12.0",
    "striptags": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
