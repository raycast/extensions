{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "seedr",
  "title": "Seedr",
  "description": "Streamline your torrent management with the Seedr.cc Raycast extension, enabling you to effortlessly add magnet links from the clipboard, manage downloads, and more.",
  "icon": "command-icon.png",
  "author": "Aayush9029",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "manage-seeds",
      "title": "Manage Seeds",
      "subtitle": "seedr.cc",
      "description": "View and manage your seeds.",
      "mode": "view"
    },
    {
      "name": "add-seed",
      "title": "Add Seed",
      "subtitle": "seedr.cc",
      "description": "Add a magnet link from system clipboard.",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "cookie",
      "title": "Cookie",
      "description": "Seedr.cc cookie",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.0",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}