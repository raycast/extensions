{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "musicthread",
  "title": "MusicThread",
  "description": "Create a thread and add links to a thread in MusicThread",
  "icon": "command-icon.png",
  "author": "ChucKN0risK",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "addLink",
      "title": "Add Link",
      "description": "Add link to a thread in MusicThread",
      "mode": "view"
    },
    {
      "name": "createThread",
      "title": "Create Thread",
      "description": "Create new thread in MusicThread",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "type": "password",
      "required": true,
      "title": "Personal Access Token",
      "description": "View extension's README for instructions.",
      "placeholder": "a1b2...c3d4"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3",
    "axios": "^0.27.2",
    "react-use": "^17.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.7",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}