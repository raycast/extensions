{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fotmob",
  "title": "Fotmob",
  "version": "0.1.0",
  "description": "Football match schedule, result, table",
  "icon": "command-icon.png",
  "author": "iamlas",
  "contributors": [
    "j3lte",
    "Antitoxic9639"
  ],
  "categories": [
    "News",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "matchday",
      "title": "Match Day",
      "subtitle": "Fotmob",
      "description": "View matchday schedule, result",
      "mode": "view"
    },
    {
      "name": "favorite",
      "title": "Favorite",
      "subtitle": "Fotmob",
      "description": "List favorite teams, players, leagues",
      "mode": "view"
    },
    {
      "name": "team",
      "title": "Team Detail",
      "subtitle": "Fotmob",
      "description": "View detail of a team by team's id",
      "mode": "view",
      "arguments": [
        {
          "name": "teamId",
          "type": "text",
          "description": "Team ID",
          "placeholder": "Enter team ID",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.76.0",
    "@raycast/utils": "^1.16.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "eslint": "^8.51.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}