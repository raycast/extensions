{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "fotmob",
  "title": "Fotmob",
  "version": "0.1.0",
  "description": "Football match schedule, result, table",
  "icon": "command-icon.png",
  "author": "iamlas",
  "categories": [
    "News",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "matchday",
      "title": "Match Day",
      "description": "View matchday schedule, result",
      "mode": "view"
    },
    {
      "name": "favorite",
      "title": "Favorite",
      "description": "List favorite teams, players, leagues",
      "mode": "view"
    },
    {
      "name": "team",
      "title": "Team detail",
      "description": "View detail of a team by team's id",
      "mode": "view",
      "arguments": [
        {
          "name": "teamId",
          "type": "text",
          "description": "Team ID",
          "placeholder": "Enter team ID",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.3",
    "@raycast/utils": "^1.9.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
