{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "minion-ipsum",
  "title": "Minion-ipsum",
  "description": "Minions ipsum hahaha wiiiii gelatooo poopayee aaaaaah.",
  "icon": "command-icon.png",
  "author": "vigosan",
  "categories": [
    "Data",
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "minion-ipsum",
      "title": "Generate Minion Ipsum",
      "subtitle": "Create Minion-themed placeholder text",
      "description": "Generates paragraphs of 'Minion Ipsum' and copy them to the clipboard",
      "mode": "no-view",
      "arguments": [
        {
          "name": "numberOfParagraphs",
          "type": "text",
          "required": false,
          "placeholder": "1"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
