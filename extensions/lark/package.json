{
  "name": "lark",
  "title": "Lark Documents",
  "version": "0.0.1",
  "description": "Quickly search documents from Feishu/Lark.",
  "icon": "logo.png",
  "author": "tmk",
  "license": "MIT",
  "commands": [
    {
      "name": "recent-docs",
      "title": "Recent Documents",
      "subtitle": "Lark",
      "description": "Open recently used documents",
      "mode": "view"
    },
    {
      "name": "search-docs",
      "title": "Search Documents",
      "subtitle": "Lark",
      "description": "Search and open documents in browser",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "type",
      "type": "dropdown",
      "required": true,
      "title": "Type",
      "description": "Your app type",
      "data": [
        {
          "title": "Feishu",
          "value": "feishu"
        },
        {
          "title": "Lark",
          "value": "lark"
        }
      ]
    },
    {
      "name": "subdomain",
      "type": "textfield",
      "required": true,
      "title": "Subdomain",
      "description": "Subdomain of your Feishu/Lark enterprise e.g. bytedance",
      "placeholder": "Subdomain of your Feishu/Lark enterprise"
    },
    {
      "name": "spaceSession",
      "type": "password",
      "required": true,
      "title": "Space Session",
      "description": "Your Space Session. Open your docs homepage and copy it in Cookies.",
      "placeholder": "Enter your space session"
    },
    {
      "name": "recentListCount",
      "type": "textfield",
      "required": false,
      "title": "Recent List Count",
      "description": "Items count when fetching recent list",
      "placeholder": "Enter recent list count",
      "default": "15"
    }
  ],
  "scripts": {
    "lint": "ray lint",
    "build": "ray build -e dist",
    "dev": "ray develop"
  },
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "axios": "^0.23.0",
    "dayjs": "^1.10.7",
    "string": "^3.3.3"
  },
  "devDependencies": {
    "@types/node": "~16.11.1",
    "@types/react": "^17.0.30",
    "@types/string": "^0.0.31",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.20.0",
    "typescript": "^4.4.4"
  }
}
