{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bmrks",
  "title": "(Basic) Bookmarks",
  "description": "Companion bookmarking utility for bmrks.com",
  "categories": [
    "Applications",
    "Productivity"
  ],
  "icon": "icon.png",
  "author": "rauno",
  "license": "MIT",
  "commands": [
    {
      "name": "bookmark",
      "title": "Bookmark",
      "description": "Bookmark the currently active browser tab",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "email",
      "type": "textfield",
      "required": true,
      "title": "Email",
      "description": "Your email on bmrks.com",
      "placeholder": "joe@doe.com"
    },
    {
      "name": "password",
      "type": "password",
      "required": true,
      "title": "Password",
      "description": "Your password on bmrks.com",
      "placeholder": "********"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1",
    "@raycast/utils": "^1.10.0",
    "@supabase/supabase-js": "^2.39.1",
    "color-string": "^1.9.1",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/color-string": "^1.5.5",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
