{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "maven-central-repository",
  "title": "Maven Central Repository",
  "description": "Official search by the maintainers of Maven Central Repository.",
  "icon": "search-maven-central-repository.png",
  "author": "koinzhang",
  "categories": [
    "Developer Tools",
    "Documentation",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-maven-central-repository",
      "title": "Search Maven Central Repository",
      "description": "Official search by the maintainers of Maven Central Repository.",
      "mode": "view",
      "arguments": [
        {
          "name": "repository",
          "placeholder": "Repository",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.1",
    "@raycast/utils": "^1.15.0",
    "axios": "1.6.0"
  },
  "devDependencies": {
    "@types/node": "~20.12.12",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
