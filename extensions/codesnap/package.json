{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "codesnap",
  "title": "CodeSnap",
  "description": "Creates a beautiful image of your code snippet",
  "icon": "command-icon.png",
  "author": "Mokkapps",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Image From Code",
      "description": "Redirects to Codesnap.dev where you can further edit the image",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "@raycast/utils": "^1.4.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
