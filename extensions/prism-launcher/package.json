{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "prism-launcher",
  "title": "Prism Launcher",
  "description": "Launch Prism Launcher instances straight from Raycast",
  "icon": "extension-icon.png",
  "author": "lockieluke3389",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Launch Instance",
      "subtitle": "Prism Launcher",
      "description": "Action for launching a Prism Launcher instance",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@types/fs-extra": "^11.0.4",
    "config-ini-parser": "^1.6.1",
    "fs-extra": "^11.2.0",
    "modern-async": "^2.0.0",
    "react-if": "^4.1.5",
    "use-async-effect": "^2.2.7"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
