{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mailsy",
  "title": "Mailsy",
  "description": "Quickly Generate Disposable Email Address and view received mails.",
  "icon": "send.png",
  "author": "BalliAsghar",
  "categories": [
    "Communication",
    "Web"
  ],
  "keywords": [
    "mailsy",
    "mails",
    "email",
    "disposable",
    "disposable email",
    "temporary email",
    "temporary",
    "temp"
  ],
  "license": "MIT",
  "commands": [
    {
      "title": "Mailsy",
      "description": "View & Manage Mailsy",
      "name": "mailsy",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0",
    "axios": "^1.6.2",
    "node-html-markdown": "^1.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
