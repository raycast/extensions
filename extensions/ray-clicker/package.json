{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ray-clicker",
  "title": "Ray Clicker",
  "description": "Idle clicker game for Raycast with upgrades, prestige, and fun events.",
  "icon": "icon.png",
  "author": "JonathanRReed",
  "license": "MIT",
  "categories": ["Fun"],
  "commands": [
    {
      "name": "index",
      "title": "Ray Clicker",
      "subtitle": "Idle Clicker Game",
      "description": "Play the idle clicker game with upgrades",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.102.3",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "^22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "npx @raycast/api@latest build -e dist",
    "dev": "npx @raycast/api@latest develop",
    "fix-lint": "npx @raycast/api@latest lint --fix",
    "lint": "npx @raycast/api@latest lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
