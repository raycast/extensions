{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cuid-generator",
  "title": "CUID Generator",
  "description": "A quick way to generate CUIDs",
  "icon": "command-icon.png",
  "author": "dgrcode",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "generate",
      "title": "Copy CUID",
      "description": "Generates a new CUID",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.3",
    "cuid": "^2.1.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
