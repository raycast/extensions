{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wueaddress",
  "title": "WueAddress For Würzburg University",
  "description": "Contact Search for Julius-Maximilians-Universität Würzburg (JMU).",
  "icon": "wue.png",
  "author": "janschlatter",
  "categories": [
    "Communication",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search WueAdress",
      "subtitle": "Search for Contact",
      "description": "Displaying contact details for JMU Personnel",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.7",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint --fix",
    "publish": "npx @raycast/api@latest publish"
  }
}
