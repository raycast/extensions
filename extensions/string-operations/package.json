{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "string-operations",
  "title": "String Operations",
  "description": "Prettify, change case, base 64 decode, parse url, remove duplicates, and more",
  "icon": "command-icon.png",
  "author": "fatih",
  "license": "MIT",
  "categories": ["Developer Tools", "Productivity"],
  "commands": [
    {
      "name": "index",
      "title": "String Operations",
      "description": "Change the text in the clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.41.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.4.3",
    "jest": "^29.2.1",
    "ts-node": "^10.9.1"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
