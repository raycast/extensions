{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sanity",
  "title": "Sanity",
  "description": "Quickly search for Sanity projects you have access to",
  "icon": "sanity-icon.png",
  "author": "marcusforsberg",
  "categories": [
    "Developer Tools",
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Quickly search for Sanity projects you have access to",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.3",
    "@raycast/utils": "^1.9.0",
    "@sanity/cli": "^3.15.1",
    "@sanity/client": "^6.4.6",
    "configstore": "^6.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/configstore": "^6.0.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
