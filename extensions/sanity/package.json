{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sanity",
  "title": "Sanity",
  "description": "Quickly search for Sanity projects you have access to",
  "icon": "sanity-icon.png",
  "author": "marcusforsberg",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Developer Tools",
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Quickly search for Sanity projects you have access to",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0",
    "@sanity/cli": "^3.57.4",
    "@sanity/client": "^6.21.3",
    "configstore": "^6.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/configstore": "^6.0.0",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
