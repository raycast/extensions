{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tailwindcss",
  "title": "Tailwind CSS Documentation",
  "description": "Quickly search Tailwind CSS documentation and open it in the browser",
  "icon": "tailwind-icon.png",
  "author": "vimtor",
  "license": "MIT",
  "commands": [
    {
      "name": "search-documentation",
      "title": "Search CSS Documentation",
      "subtitle": "Tailwind",
      "description": "Search Tailwind CSS documentation and open it in the browser",
      "mode": "view"
    }
  ],
  "categories": [
    "Documentation",
    "Developer Tools"
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "jsdom": "^18.0.0",
    "react": "^17.0.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.29",
    "@types/lodash": "^4.14.176",
    "@types/jsdom": "^16.2.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
