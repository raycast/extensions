{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cinemas-nos",
  "title": "Cinemas NOS",
  "description": "Shows all movies and sessions in display by Cinemas NOS.",
  "icon": "command-icon.png",
  "author": "diogo_ferrer",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Browse Sessions",
      "description": "Shows all movies and schedules in display by Cinemas NOS.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.3",
    "@raycast/utils": "^1.9.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
