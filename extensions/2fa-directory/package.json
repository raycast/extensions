{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "2fa-directory",
  "title": "2FA Directory",
  "description": "View a list of 2FA enabled sites.",
  "icon": "command-icon.png",
  "author": "andreaselia",
  "contributors": [
    "pernielsentikaer",
    "xmok"
  ],
  "categories": [
    "Applications",
    "Data",
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search 2FA Directory",
      "description": "Search through sites that allow you to enable 2FA.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.78.0",
    "@raycast/utils": "^1.16.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
