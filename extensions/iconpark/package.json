{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "iconpark",
  "title": "IconPark",
  "description": "IconPark gives access to more than 2000 high-quality icons without API key and network.",
  "icon": "icon-park.png",
  "author": "koinzhang",
  "categories": [
    "Design Tools",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-icons",
      "title": "Search Icons",
      "subtitle": "IconPark",
      "description": "Search and custom icons in more than 2000 high-quality IconPark icons.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@icon-park/svg": "^1.4.0",
    "@raycast/api": "^1.36.0",
    "d3-color": "^3.1.0",
    "js-base64": "^3.7.2"
  },
  "devDependencies": {
    "@types/d3-color": "^3.1.0",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
