{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "iconpark",
  "title": "IconPark",
  "description": "IconPark gives access to more than 2000 high-quality icons without API key and network.",
  "icon": "icon-park.png",
  "author": "koinzhang",
  "categories": [
    "Design Tools",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-icons",
      "title": "Search Icons",
      "subtitle": "IconPark",
      "description": "Search and custom icons in more than 2000 high-quality IconPark icons.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.78.0",
    "@icon-park/svg": "^1.4.2",
    "d3-color": "^3.1.0",
    "js-base64": "^3.7.7",
    "@raycast/utils": "^1.16.1",
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@types/d3-color": "^3.1.3",
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
