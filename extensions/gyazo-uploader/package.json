{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gyazo-uploader",
  "title": "Gyazo Uploader",
  "description": "Upload clipboard image to Gyazo",
  "icon": "upload-icon.png",
  "author": "udus",
  "version": "1.0.0",
  "contributors": [
    "qaynam"
  ],
  "categories": [
    "Media",
    "Productivity",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "upload-gyazo-from-clipboard",
      "title": "Upload Gyazo From Clipboard",
      "subtitle": "upload gyazo",
      "description": "upload screenshot from clipboard",
      "mode": "no-view"
    },
    {
      "name": "gyazo-unloader",
      "icon": "gyazo-logo.png",
      "title": "Gyazo Raycast Plugin",
      "subtitle": "Gyazo Raycast Plugin",
      "description": "Raycast Plugin for Gyazo. Upload screenshot from clipboard to Gyazo. Requires Gyazo API key. Set your Gyazo API key in the preferences.",
      "mode": "menu-bar"
    }
  ],
  "preferences": [
    {
      "name": "gyazo-api-key",
      "type": "password",
      "description": "Your Gyazo API key",
      "required": true,
      "title": "API Key"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.94.3",
    "@raycast/utils": "^1.19.1",
    "file-type": "^20.4.1",
    "gyazo-api": "^0.3.1",
    "gyazo-api-ts": "^0.1.4",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.5",
    "@types/node": "^18.8.3",
    "@types/react": "^18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
