{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ruler",
  "title": "Ruler",
  "description": "Calculate Distance Between two point",
  "icon": "icon.png",
  "author": "anwarulislam",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "ruler",
      "title": "Ruler",
      "subtitle": "Calculate Distance",
      "description": "Calculate Distance Between two point",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.54.0",
    "execa": "^7.1.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "build-swift": "swift build --configuration=release --arch arm64 --arch x86_64 && mv .build/apple/Products/Release/ruler assets",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
