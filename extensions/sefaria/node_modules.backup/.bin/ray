#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/api/bin/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/api/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/api/bin/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/api/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules/@raycast/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/@raycast+api@1.100.3_@types+node@22.13.10_@types+react@19.0.10/node_modules:/Users/davidnagar/code/extensions/sefaria/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@raycast/api/bin/run.js" "$@"
else
  exec node  "$basedir/../@raycast/api/bin/run.js" "$@"
fi
