{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "anilist-airing-schedule",
  "title": "Anilist Airing Schedule",
  "description": "Get airing schedule of anime from Anilist.co",
  "icon": "anilist-icon-512x512.png",
  "author": "yeskunall",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Get Airing Schedule",
      "description": "Get the airing schedule of anime from Anilist.co",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "1.47.3",
    "@raycast/utils": "1.4.16",
    "date-fns": "2.29.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "prettier": "2.8.4",
    "typescript": "4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
