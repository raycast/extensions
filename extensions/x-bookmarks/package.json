{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "x-bookmarks",
  "version": "1.0.0",
  "title": "X Bookmarks",
  "description": "View and search X bookmarks",
  "icon": "extension-icon.png",
  "author": "nima_txt",
  "license": "MIT",
  "commands": [
    {
      "name": "bookmarks",
      "title": "X Bookmarks",
      "description": "View and search X bookmarks",
      "mode": "view"
    }
  ],
  "tools": [
    {
      "name": "get-bookmarks",
      "title": "get bookmarks",
      "description": "Export Firefox bookmarks to markdown"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.93.2",
    "@raycast/utils": "^1.17.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "testing": "tsx src/readthem.ts",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish",
    "start": "ts-node src/readthem.ts"
  },
  "files": [
    "assets/**/*"
  ]
}
