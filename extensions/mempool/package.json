  "icon": "bitcoin.png",
  "author": "dillionverma",
  "categories": [
    "Finance"
  ],
  "keywords": [
    "bitcoin",
    "mempool",
    "transaction",
    "fees"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
