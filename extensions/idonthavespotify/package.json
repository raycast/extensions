{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "idonthavespotify",
  "title": "I Don't Have Spotify",
  "description": "Convert Spotify Links to YouTube, Apple Music, Tidal and Soundcloud",
  "version": "1.0.0",
  "author": "sjdonado",
  "icon": "icon.png",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "convert",
      "title": "Convert Spotify Link",
      "description": "Paste a Spotify Link and convert it to YouTube, Apple Music, Tidal and Soundcloud",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.0",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
