  {
    "key": "Exiting",
    "commands": [
      {
        "kbd": ":w",
        "text": "write (save) the file, but don't exit"
      },
      {
        "kbd": ":w !sudo tee %",
        "text": "write out the current file using sudo"
      },
      {
        "kbd": ":wq or :x or ZZ",
        "text": "write (save) and quit"
      },
      {
        "kbd": ":q",
        "text": "quit (fails if there are unsaved changes)"
      },
      {
        "kbd": ":q! or ZQ",
        "text": "quit and throw away unsaved changes"
      },
      {
        "kbd": ":wqa",
        "text": "write (save) and quit on all tabs"
      }
    ]
  },

  {
    "key": "Search and replace",
    "commands": [
      {
        "kbd": "/pattern",
        "text": "search for pattern"
      },
      {
        "kbd": "?pattern",
        "text": "search backward for pattern"
      },
      {
        "kbd": "\\vpattern",
        "text": "'very magic' pattern: non-alphanumeric characters are interpreted as special regex symbols (no escaping needed)"
      },
      {
        "kbd": "n",
        "text": "repeat search in same direction"
      },
      {
        "kbd": "N",
        "text": "repeat search in opposite direction"
      },
      {
        "kbd": ":%s/old/new/g",
        "text": "replace all old with new throughout file"
      },
      {
        "kbd": ":%s/old/new/gc",
        "text": "replace all old with new throughout file with confirmations"
      },
      {
        "kbd": ":noh[lsearch]",
        "text": "remove highlighting of search matches"
      }
    ]
  },

  {
    "key": "Tabs",
    "commands": [
      {
        "kbd": ":tabnew or :tabnew {page.words.file}",
