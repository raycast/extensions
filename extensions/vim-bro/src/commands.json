      {
        "kbd": "Ctrl + y",
        "text": "move screen up one line (without moving cursor)"
      },
      {
        "kbd": "Ctrl + b",
        "text": "move back one full screen"
      },
      {
        "kbd": "Ctrl + f",
        "text": "move forward one full screen"
      },
      {
        "kbd": "Ctrl + d",
        "text": "move forward 1/2 a screen"
      },
      {
        "kbd": "Ctrl + u",
        "text": "move back 1/2 a screen"
      }
    ]
  },

  {
    "key": "insert",
    "commands": [
      {
        "kbd": "i",
        "text": "insert before the cursor"
      },
      {
        "kbd": "I",
        "text": "insert at the beginning of the line"
      },
      {
        "kbd": "a",
        "text": "insert (append) after the cursor"
      },
      {
        "kbd": "A",
        "text": "insert (append) at the end of the line"
      },
      {
        "kbd": "o",
        "text": "append (open) a new line below the current line"
      },
      {
        "kbd": "O",
        "text": "append (open) a new line above the current line"
      },
      {
        "kbd": "ea",
        "text": "insert (append) at the end of the word"
      },
      {
        "kbd": "Ctrl + h",
        "text": "delete the character before the cursor during insert mode"
      },
      {
        "kbd": "Ctrl + w",
        "text": "delete word before the cursor during insert mode"
      },
      {
        "kbd": "Ctrl + j",
        "text": "begin new line during insert mode"
      },
      {
        "kbd": "Ctrl + t",
        "text": "indent (move right) line one shiftwidth during insert mode"
      },
      {
        "kbd": "Ctrl + d",
        "text": "de-indent (move left) line one shiftwidth during insert mode"
      },
      {
        "kbd": "Ctrl + n",
        "text": "insert (auto-complete) next match before the cursor during insert mode"
      },
      {
        "kbd": "Ctrl + p",
        "text": "insert (auto-complete) previous match before the cursor during insert mode"
      },
      {
        "kbd": "Ctrl + rx",
        "text": "insert the contents of register x"
      },
      {
        "kbd": "Ctrl + ox",
        "text": "Temporarily enter normal mode to issue one normal-mode command x."
      },
      {
        "kbd": "Esc",
        "text": "exit insert mode"
      }
    ]
  },

  {
    "key": "Insert mode - inserting/appending text",
    "commands": [
      {
        "kbd": "r",
        "text": "replace a single character."
      },
      {
        "kbd": "R + ESC",
        "text": "replace more than one character, until + is pressed."
      },
      {
        "kbd": "J",
        "text": "join line below to the current one with one space in between"
      },
      {
        "kbd": "gJ",
        "text": "join line below to the current one without space in between"
      },
      {
        "kbd": "gwip",
        "text": "reflow paragraph"
      },
      {
        "kbd": "g~",
        "text": "switch case up to motion"
      },
      {
        "kbd": "gu",
        "text": "change to lowercase up to motion"
      },
      {
        "kbd": "gU",
        "text": "change to uppercase up to motion"
      },
      {
        "kbd": "cc",
        "text": "change (replace) entire line"
      },
      {
        "kbd": "C",
        "text": "change (replace) to the end of the line"
      },
      {
        "kbd": "c$",
        "text": "change (replace) to the end of the line"
      },
      {
        "kbd": "ciw",
        "text": "change (replace) entire word"
      },
      {
        "kbd": "cw or ce",
        "text": "change (replace) to the end of the word"
      },
      {
        "kbd": "s",
        "text": "delete character and substitute text"
      },
      {
        "kbd": "S",
