[
  {
    "key": "Global",
    "commands": [
      {
        "kbd": ":h[elp] keyword",
        "text": "open help for keyword"
      },
      {
        "kbd": ":sav[eas] file",
        "text": "save file as"
      },
      {
        "kbd": ":clo[se]",
        "text": "close current pane"
      },
      {
        "kbd": ":ter[minal]",
        "text": "open a terminal window"
      },
      {
        "kbd": "K",
        "text": "open man page for word under the cursor"
      }
    ]
  },
  {
