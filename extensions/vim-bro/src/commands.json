      {
        "kbd": "xp",
        "text": "transpose two letters (delete and paste)"
      },
      {
        "kbd": "u",
        "text": "undo"
      },
      {
        "kbd": "U",
        "text": "restore (undo) last changed line"
      },
      {
        "kbd": "Ctrl + r",
        "text": "redo"
      },
      {
        "kbd": ".",
        "text": "repeat last command"
