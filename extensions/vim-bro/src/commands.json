      {
        "kbd": "{",
        "text": "jump to previous paragraph (or function/block, when editing code)"
      },
      {
        "kbd": "zz",
        "text": "center cursor on screen"
      },
      {
        "kbd": "Ctrl + e",
        "text": "move screen down one line (without moving cursor)"
