{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "country-lookup",
  "title": "Country Lookup",
  "description": "Quickly lookup countries and get informational data",
  "icon": "command-icon.png",
  "author": "pernielsentikaer",
  "contributors": [
    "j3lte"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Country",
      "description": "Quickly lookup countries",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "title": "Coat of Arms",
      "name": "showCoatOfArms",
      "type": "checkbox",
      "required": false,
      "default": true,
      "label": "Display the Coat of Arms by default",
      "description": "Show coat of arms, looks amazing but might take some space"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.76.1",
    "@raycast/utils": "^1.16.0",
    "dequal": "^2.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.14.2",
    "@types/react": "18.3.3",
    "eslint": "^8.51.0",
    "prettier": "^3.3.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx -y @raycast/api@latest publish"
  }
}
