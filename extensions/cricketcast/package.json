{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cricketcast",
  "title": "CricketCast",
  "description": "Check the cricket score through Raycast!",
  "icon": "command-icon.png",
  "author": "sampoder",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Cricket Scores",
      "description": "View all of the ongoing cricket scores",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3",
    "node-fetch": "^3.1.0",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
