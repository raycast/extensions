{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "snippetslab",
  "title": "SnippetsLab",
  "description": "Search your SnippetsLab snippets from Raycast",
  "icon": "command-icon.png",
  "author": "1874",
  "homepage": "https://github.com/SnippetsLab-Raycast#readme",
  "categories": [
    "Developer Tools", "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Find in SnippetsLab",
      "description": "Search snippets for the provided string",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.4.8",
    "execa": "6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
