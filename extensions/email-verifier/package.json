{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "email-verifier",
  "title": "Email Verifier",
  "description": "A tool for email verification without sending any emails",
  "icon": "command-icon.png",
  "author": "HerbertLu",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Security",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "email-verifier",
      "title": "Check Email",
      "description": "Check if an email address exists",
      "mode": "view",
      "arguments": [
        {
          "name": "email",
          "placeholder": "email@example.com",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
