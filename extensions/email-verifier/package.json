{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "email-verifier",
  "title": "Email Verifier",
  "description": "A tool for email verification without sending any emails",
  "icon": "command-icon.png",
  "author": "HerbertLu",
  "categories": [
    "Security",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "email-verifier",
      "title": "Check Email",
      "description": "Check if an email address exists",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.1",
    "@raycast/utils": "^1.4.8",
    "axios": "^1.2.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
