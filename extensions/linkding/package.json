{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linkding",
  "title": "Linkding",
  "description": "Manage Linkding through Raycast",
  "icon": "command-icon.png",
  "author": "InteractiveNinja",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-bookmarks",
      "title": "Search Linkding",
      "description": "Searches through Linkding Bookmarks",
      "mode": "view"
    },
    {
      "name": "manage-account",
      "title": "Manage Linkding Accounts",
      "description": "Mange your Linkding Accounts and Servers",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.0",
    "axios": "^1.3.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
