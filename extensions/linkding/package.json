{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linkding",
  "title": "Linkding",
  "description": "Manage Linkding through Raycast",
  "icon": "command-icon.png",
  "author": "InteractiveNinja",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-bookmarks",
      "title": "Search Linkding",
      "description": "Searches through Linkding Bookmarks",
      "mode": "view"
    },
    {
      "name": "manage-account",
      "title": "Manage Linkding Accounts",
      "description": "Mange your Linkding Accounts and Servers",
      "mode": "view"
    },
    {
      "name": "create-bookmarks",
      "title": "Create Bookmark",
      "description": "Create new Bookmarks",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.0",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^18.8.4",
    "@types/react": "^18.2.27",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
