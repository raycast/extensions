{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "linkding",
  "title": "Linkding",
  "description": "Manage Linkding through Raycast",
  "icon": "command-icon.png",
  "author": "InteractiveNinja",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-bookmarks",
      "title": "Search Linkding",
      "description": "Searches through Linkding Bookmarks",
      "mode": "view"
    },
    {
      "name": "manage-account",
      "title": "Manage Linkding Accounts",
      "description": "Mange your Linkding Accounts and Servers",
      "mode": "view"
    },
    {
      "name": "create-bookmarks",
      "title": "Create Bookmark",
      "description": "Create new Bookmarks",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.91.0",
    "@raycast/utils": "^1.18.1",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
