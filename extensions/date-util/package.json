{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "date-util",
  "title": "date util",
  "description": "Convert timestamps and datetime strings into various formats.",
  "icon": "command-icon.png",
  "author": "ruhengChen",
  "contributors": [
  
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "date util",
      "description": "Convert timestamps and datetime strings into various formats.",
      "mode": "view",
      "arguments": [
        {
          "name": "queryText",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.30.2",
    "dayjs": "^1.10.8"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.8.8",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
