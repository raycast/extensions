{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "date-util",
  "title": "Date Util",
  "description": "Convert timestamps and datetime strings into various formats.",
  "icon": "command-icon.png",
  "author": "crh",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Date Util",
      "description": "Convert timestamps and datetime strings into various formats.",
      "mode": "view",
      "arguments": [
        {
          "name": "queryText",
          "placeholder": "Query",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "dayjs": "^1.10.8",
    "@raycast/api": "^1.59.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}