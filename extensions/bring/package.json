{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bring",
  "title": "Bring!",
  "description": "Add items to your Bring! shopping lists",
  "icon": "bring-icon.png",
  "author": "amuelli",
  "contributors": [
    "ilian",
    "xilopaint"
  ],
  "categories": [
    "Productivity"
  ],
  "preferences": [
    {
      "type": "textfield",
      "name": "email",
      "required": true,
      "title": "Email",
      "description": "Email address of your Bring! account",
      "default": ""
    },
    {
      "type": "password",
      "name": "password",
      "required": true,
      "title": "Password",
      "description": "Your Bring! password"
    }
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Add Items to List",
      "description": "Add items to a Bring! list",
      "mode": "view",
      "keywords": ["shopping", "bring", "groceries"]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.68.1",
    "@raycast/utils": "^1.12.5",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
