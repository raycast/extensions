{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "websocket-debugging",
  "title": "Websocket Debugging",
  "description": "Allows you to debug Pusher",
  "icon": "extension_icon.png",
  "author": "RhysLees",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "PusherAppID",
      "title": "Pusher App ID",
      "label": "Pusher App ID",
      "description": "Pusher App ID",
      "type": "textfield",
      "default": "",
      "required": true
    },
    {
      "name": "PusherAppKey",
      "title": "Pusher App Key",
      "label": "Pusher App Key",
      "description": "Pusher App Key",
      "type": "textfield",
      "default": "",
      "required": true
    },
    {
      "name": "PusherAppSecret",
      "title": "Pusher App Secret",
      "label": "Pusher App Secret",
      "description": "Pusher App Secret",
      "type": "textfield",
      "default": "",
      "required": true
    },
    {
      "name": "PusherAppCluster",
      "title": "Pusher App Cluster",
      "label": "Pusher App Cluster",
      "description": "Pusher Debuging",
      "type": "textfield",
      "default": "",
      "required": true
    },
    {
      "name": "PusherAppEncrypted",
      "title": "Pusher App Encrypted",
      "label": "Pusher App Encrypted",
      "description": "Pusher Encrypted",
      "type": "checkbox",
      "default": false,
      "required": false
    }
  ],
  "commands": [
    {
      "name": "pusher",
      "title": "Pusher Debug",
      "description": "Debug Pusher Websockets",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.2",
    "@raycast/utils": "^1.4.16",
    "nanoid": "^4.0.0",
    "node-fetch": "^3.3.0",
    "pusher": "^5.1.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "laravel-echo": "^1.14.2",
    "prettier": "^2.5.1",
    "pusher-js": "^8.0.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
