{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nanoid",
  "title": "Generate Nanoid",
  "description": "A tiny, secure, URL friendly, unique string ID generator for JavaScript",
  "icon": "command-icon.png",
  "author": "roelvan",
  "contributors": [
    "mikevalstar"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "nanoid",
      "title": "Generate Nanoid",
      "description": "Copy a new nanoid",
      "mode": "no-view",
      "arguments": [
        {
          "name": "len",
          "placeholder": "ID Length",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.86.0",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
