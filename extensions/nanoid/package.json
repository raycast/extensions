{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nanoid",
  "title": "Generate Nanoid",
  "description": "A tiny, secure, URL friendly, unique string ID generator for JavaScript",
  "icon": "command-icon.png",
  "author": "roelvan",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "nanoid",
      "title": "Generate Nanoid",
      "description": "Copy a new nanoid",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.0",
    "nanoid": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
