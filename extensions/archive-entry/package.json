{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "archive-entry",
  "title": "Archive Entry",
  "description": "Add a new entry to the Archive Website",
  "icon": "icon.png",
  "author": "linuscodes",
  "categories": [
    "Media",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Add Entry to Archive",
      "description": "Add a new entry to the Archive Website",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.10.1",
    "btoa": "^1.2.1",
    "formdata-node": "^6.0.3",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
