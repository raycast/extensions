{
  "name": "mldocs",
  "title": "Machine Learning Library APIs",
  "description": "Raycast Commands for Searching ML APIs",
  "icon": "command-icon.png",
  "commands": [
    {
      "name": "index",
      "title": "Search Machine Learning Library APIs",
      "description": "Search Machine Learning Library APIs",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "shouldFilterTensorflow",
      "type": "checkbox",
      "required": false,
      "title": "Filtering",
      "label": "Tensorflow",
      "description": "Do not search Tensorflow (tf.*) APIs"
    },
    {
      "name": "shouldFilterTensorflowIO",
      "type": "checkbox",
      "required": false,
      "title": "",
      "label": "Tensorflow IO",
      "description": "Do not search Tensorflow IO (tfio.*) APIs"
    },
    {
      "name": "shouldFilterTensorflowAddOns",
      "type": "checkbox",
      "required": false,
      "title": "",
      "label": "Tensorflow Addons",
      "description": "Do not search Tensorflow IAddons(tfa.*) APIs"
    },
    {
      "name": "shouldFilterTensorflowDataset",
      "type": "checkbox",
      "required": false,
      "title": "",
      "label": "Tensorflow Dataset",
      "description": "Do not search Tensorflow Dataset (tfds.*) APIs"
    },
    {
      "name": "shouldFilterPyTorch",
      "type": "checkbox",
