    "@raycast/api": "^1.25.0",
    "node-fetch": "^3.0.0",
    "nanoid": "^3.1.30",
    "moment": "^2.29.1",
    "he": "^1.2.0",
    "fuse.js":"^6.4.6"
  },
  "devDependencies": {
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
