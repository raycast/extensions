{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mldocs",
  "title": "MLDocs",
  "description": "Raycast Commands for Searching Different ML APIs",
  "icon": "command-icon.png",
  "author": "reckoning-dev",
  "version": "1.0.0",
  "license": "MIT",
  "commands": [
    {
      "name": "tf",
      "title": "Search Tensorflow APIs",
      "subtitle": "MLDocs",
      "description": "Search Tensorflow APIs",
      "mode": "view"
    },
    {
      "name": "torch",
      "title": "Search Pytorch APIs",
      "subtitle": "MLDocs",
      "description": "Search Pytorch APIs",
      "mode": "view"
    },
    {
      "name": "pandas",
      "title": "Search Pandas APIs",
      "subtitle": "MLDocs",
      "description": "Search Pandas Library APIs",
      "mode": "view"
    },
    {
      "name": "sklearn",
      "title": "Search Scikit-Learn APIs",
      "subtitle": "MLDocs",
      "description": "Search Scikit-Learn Library APIs",
      "mode": "view"
    },
    {
      "name": "numpy",
      "title": "Search Numpy APIs",
      "subtitle": "MLDocs",
      "description": "Search Numpy Library APIs",
      "mode": "view"
    },
    {
      "name": "matplotlib",
      "title": "Search Matplotlib and Plotly APIs",
      "subtitle": "MLDocs",
      "description": "Search Matplotlib and Plotly Library APIs",
      "mode": "view"
    },
    {
      "name": "seaborn",
      "title": "Search Seaborn APIs",
      "subtitle": "MLDocs",
      "description": "Search Seaborn Library APIs",
      "mode": "view"
    },
    {
      "name": "statsmodels",
      "title": "Search Statsmodels APIs",
      "subtitle": "MLDocs",
      "description": "Search Statsmodels Library APIs",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.1",
    "node-fetch": "^3.0.0",
    "nanoid": "^3.1.30",
    "moment": "^2.29.1",
    "he": "^1.2.0",
    "fuse.js":"^6.4.6"
  },
  "devDependencies": {
    "@types/node": "^16.11.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
