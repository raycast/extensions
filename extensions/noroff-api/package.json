{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "noroff-api",
  "title": "Noroff API",
  "description": "Simple Raycast extension to open the Noroff API documentation in your browser.",
  "icon": "noroff-logo-emblem.png",
  "author": "kyrregjerstad",
  "license": "MIT",
  "version": "0.0.1",
  "commands": [
    {
      "name": "index",
      "title": "Show List and Detail",
      "description": "Template for showing a static list and a detail view",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
