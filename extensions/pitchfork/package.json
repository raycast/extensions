{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pitchfork",
  "title": "Pitchfork",
  "description": "See Pitchfork Album Reviews",
  "icon": "pitchfork_icon.png",
  "author": "koayon",
  "license": "MIT",
  "categories": [
    "News",
    "Media",
    "Web"
  ],
  "commands": [
    {
      "name": "reviews",
      "title": "See Reviews",
      "subtitle": "Pitchfork",
      "description": "Show Recent Pitchfork Album Reviews",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.0",
    "@raycast/utils": "^1.4.16",
    "date-fns": "^2.25.0",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
