{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dot-new",
  "title": "dot-new",
  "description": "Start something .new! .new links are shortcuts to your favorite actions on the websites you love",
  "icon": "command-icon.png",
  "author": "natterstefan",
  "contributors": [
    "xmok",
    "ViGeng"
  ],
  "categories": [
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": ".new",
      "subtitle": "Start something new!",
      "description": "Start something .new! .new links are shortcuts to your favorite actions on the websites you love.",
      "mode": "view",
      "keywords": [
        "new",
        "dot-new"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.19.0",
    "fuse.js": "^6.5.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "tabletojson": "^2.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "scripts": {
    "generate-dataset": "ts-node ./scripts/generate-dataset.ts",
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
