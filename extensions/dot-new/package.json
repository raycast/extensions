{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dot-new",
  "title": "dot-new",
  "description": "Start something .new! .new links are shortcuts to your favorite actions on the websites you love",
  "icon": "command-icon.png",
  "author": "natterstefan",
  "categories": [
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": ".new",
      "subtitle": "Start something new!",
      "description": "Start something .new! .new links are shortcuts to your favorite actions on the websites you love.",
      "mode": "view",
      "keywords": [
        "new",
        "dot-new"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "fuse.js": "^6.5.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "tabletojson": "^2.0.7",
    "ts-node": "^10.7.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "generate-dataset": "ts-node ./scripts/generate-dataset.ts",
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
