{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hostloc",
  "title": "hostloc",
  "description": "You can quickly access the 20 latest posts of hostloc at the moment. Note: The hostloc home page can be reached by visiting https://www.hostloc.com/ through your browser.",
  "icon": "favicon.png",
  "author": "J3n5en",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "loc",
      "title": "Show Latest Posts",
      "subtitle": "hostloc",
      "description": "Shows the latest 20 hostloc posts",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.0",
    "got": "^12.1.0",
    "rss-parser": "^3.12.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}