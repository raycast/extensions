{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dropbox",
  "title": "Dropbox",
  "description": "Dropbox Extension, List Files, Search Files, and more",
  "icon": "dropbox.png",
  "author": "chyroc",
  "license": "MIT",
  "commands": [
    {
      "name": "search-dropbox-files",
      "title": "Dropbox Files Search",
      "description": "Search Files directly in Dropbox",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "dropbox_access_token",
      "title": "Dropbox Access Token",
      "description": "Dropbox access token",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "dropbox": "^10.27.0",
    "dropbox-v2-api": "^2.5.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
