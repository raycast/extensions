{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dropbox",
  "title": "Dropbox",
  "description": "Dropbox Extensions",
  "icon": "dropbox.png",
  "author": "chyroc",
  "license": "MIT",
  "commands": [
    {
      "name": "save-url",
      "title": "Save URL",
      "subtitle": "Dropbox",
      "description": "Save url to Dropbox",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "title": "Dropbox Token",
      "required": true,
      "type": "textfield",
      "description": "dropbox access token",
      "placeholder": "Input Dropbox access token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "dropbox": "^10.24.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
