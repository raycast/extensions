{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dropbox",
  "title": "Dropbox",
  "description": "Dropbox Extension, List Files, Search Files, and more",
  "icon": "dropbox.png",
  "author": "chyroc",
  "contributors": [
    "xmok"
  ],
  "license": "MIT",
  "preferences": [
    {
      "name": "dropbox_access_token",
      "title": "Dropbox Access Token",
      "description": "Dropbox Access Token",
      "type": "password",
      "placeholder": "xx.xxx...xxx",
      "required": false
    }
  ],
  "commands": [
    {
      "name": "search-dropbox-files",
      "title": "Dropbox Files Search",
      "description": "Search Files directly in Dropbox",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0",
    "dropbox": "^10.34.0",
    "dropbox-v2-api": "^2.5.2",
    "node-fetch": "^3.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
