{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "syntax-fm",
  "title": "Syntax.fm",
  "description": "Syntax.fm at your fingertips.",
  "icon": "icon.png",
  "author": "msmps",
  "categories": [
    "Media",
    "News"
  ],
  "keywords": [
    "podcast",
    "syntax"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search Episodes",
      "subtitle": "Syntax.fm",
      "description": "List all episodes",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.14.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
