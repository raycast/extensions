{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "defichain-dobby",
  "title": "Defichain Dobby",
  "description": "Show your current Defichain vaults, setup in https://defichain-dobby.com/",
  "icon": "command-icon.png",
  "author": "adrian-schnell",
  "categories": [
    "Finance",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "dobby",
      "title": "dobby",
      "subtitle": "Defichain Vault",
      "description": "Load current vault details",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.32.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}