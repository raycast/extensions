{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mlb-scores",
  "title": "MLB Scores",
  "description": "Shows current MLB game scores.",
  "icon": "logo.png",
  "author": "jso8910",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View MLB scores",
      "description": "Get scores of today's MLB games.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "node-fetch": "^3.2.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
