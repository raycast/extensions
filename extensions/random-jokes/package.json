{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "random-jokes",
  "title": "Random Jokes",
  "description": "A Raycast extension that provides random jokes to brighten your day.",
  "icon": "icon.png",
  "author": "luiggircardoso",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "get-random-joke",
      "title": "Get Random Joke",
      "description": "Fetch a random joke.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.100.3",
    "@raycast/utils": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "title-case": "^3.0.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.18.4",
    "@types/react": "18.2.27",
    "eslint": "^9.30.1",
    "prettier": "^3.0.3",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "type": "commonjs"
}
