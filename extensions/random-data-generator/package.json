{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "random",
  "title": "Random Data Generator",
  "description": "Generate random data using Faker library",
  "icon": "random.png",
  "author": "loris",
  "contributors": [
    "Calvein",
    "pernielsentikaer",
    "vimtor",
    "j3lte",
    "stefanmatar"
  ],
  "license": "MIT",
  "categories": [
    "Data",
    "Developer Tools"
  ],
  "keywords": [
    "faker"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Generate Random Data",
      "description": "Generate random data using Faker library",
      "mode": "view"
    },
    {
      "name": "open-quicklink",
      "title": "Open Quicklink",
      "description": "Used internally to run quicklinks from the generate command.",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "name": "defaultAction",
      "type": "dropdown",
      "data": [
        {
          "title": "Copy to Clipboard",
          "value": "copy"
        },
        {
          "title": "Paste in Active App",
          "value": "paste"
        }
      ],
      "title": "Default Action",
      "description": "Choose the default action for the generated data.",
      "required": false,
      "default": "copy",
      "value": "copy"
    }
  ],
  "dependencies": {
    "@bugsnag/cuid": "^3.0.2",
    "@faker-js/faker": "^7.6.0",
    "@raycast/api": "^1.66.2",
    "is-url": "^1.2.4",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bugsnag__cuid": "^3.0.2",
    "@types/is-url": "^1.2.32",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.52",
    "@raycast/eslint-config": "^1.0.8",
    "eslint": "^8.56.0",
    "react-devtools": "^4.28.4",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
