{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "moneylover",
  "title": "MoneyLover",
  "description": "MoneyLover is a personal finance app that helps you manage your money, budget, and track your spending.",
  "icon": "moneylover-icon.png",
  "author": "lachero",
  "categories": [
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "view-wallets",
      "title": "View Wallets",
      "subtitle": "MoneyLover",
      "description": "View your wallets",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "email",
      "title": "Email",
      "description": "Email of your Money Lover account",
      "type": "textfield",
      "required": true
    },
    {
      "name": "password",
      "title": "Password",
      "description": "Password of your Money Lover account",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "moneylover": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
