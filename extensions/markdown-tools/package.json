{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "markdown-tools",
  "title": "Markdown-tools",
  "description": "A Raycast Extension that helps with formatting text to markdown",
  "icon": "MarkdownTable.png",
  "author": "uxkjaer",
  "categories": [
    "Developer Tools",
    "Productivity",
    "Design Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Markdown Tools",
      "description": "Template for submitting a form",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "csv-to-markdown-table": "^1.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
