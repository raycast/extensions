{
  "name": "canvascast",
  "title": "CanvasCast",
  "description": "Access the Canvas LMS from Raycast",
  "icon": "canvas.png",
  "author": "Yoda",
  "license": "MIT",
  "contributors": [
    "yug2005"
  ],
  "preferences": [
    {
      "name": "token",
      "type": "textfield",
      "required": true,
      "title": "Token",
      "placeholder": "6115~Vfxthpk4Bui862dmW28SmgR2ynvXBR3hYE6X3TjiLdnWo4gCRuFkMndwHi71B1G6",
      "description": "Access token from Canvas Settings"
    },
    {
      "name": "domain",
      "type": "textfield",
      "required": true,
      "title": "Domain",
      "placeholder": "canvas.instructure.com",
      "description": "Domain for your canvas LMS. Do not include URL; just the domain."
    }
  ],
  "commands": [
    {
      "name": "index",
      "title": "My Courses",
      "subtitle": "Canvas",
      "description": "View the Canvas dashboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@api-blueprints/pathmaker": "^1.0.0",
    "@raycast/api": "^1.25.1",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
