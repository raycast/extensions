{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "color-hunt",
  "title": "Color Hunt",
  "description": "Color Palettes for Designers and Artists",
  "icon": "color-hunt-icon.png",
  "author": "Lemon",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Palettes",
      "description": "Discover the newest hand-picked palettes of Color Hunt",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.73.2",
    "@raycast/utils": "^1.15.0",
    "cross-fetch": "^4.0.0",
    "glob": "^10.3.15",
    "satori": "^0.10.13"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
