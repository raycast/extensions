{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ai-gen",
  "title": "ai-gen",
  "description": "Generate images using AI",
  "icon": "command-icon.png",
  "author": "josephschmitt",
  "categories": [
    "Media",
    "Fun",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Image",
      "subtitle": "DALL-E image generation",
      "description": "Generate a new AI image based on your text prompt",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}