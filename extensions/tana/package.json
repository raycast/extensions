{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tana",
  "title": "Tana",
  "description": "Write notes in Raycast, and capture them in Tana.",
  "icon": "tana-icon.png",
  "author": "cheslip",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "quickAddCommand",
      "title": "Quick Add",
      "subtitle": "Tana",
      "description": "Create a new node",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "nodeApiToken",
      "description": "API token for your capture node",
      "type": "password",
      "required": true,
      "title": "API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "@raycast/utils": "^1.4.8",
    "axios": "^1.1.2"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
