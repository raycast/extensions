{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tana",
  "title": "Tana",
  "description": "Write notes in Raycast, and capture them in Tana.",
  "icon": "tana-icon.png",
  "author": "cheslip",
  "contributors": [
    "neil_brown"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "commands": [
    {
      "name": "quickAddCommand",
      "title": "Quick Add",
      "subtitle": "Tana",
      "description": "Create a new node",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "nodeApiToken",
      "description": "API token for your capture node",
      "type": "password",
      "required": true,
      "title": "API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "@raycast/utils": "^1.4.8",
    "node-fetch": "^3.3.2",
    "readline": "^1.3.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "typescript": "^5.2.2"
  }
}
