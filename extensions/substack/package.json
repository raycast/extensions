{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "substack",
  "title": "Substack",
  "description": "Search Substack posts and profiles from Raycast",
  "icon": "command-icon.png",
  "author": "j3lte",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-substack-posts",
      "title": "Search Posts",
      "subtitle": "Substack",
      "description": "Search Substack posts",
      "mode": "view"
    },
    {
      "name": "search-substack-profiles",
      "title": "Search Profiles",
      "subtitle": "Substack",
      "description": "Search Substack profiles",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.71.4",
    "@raycast/utils": "^1.14.0",
    "ahooks": "^3.7.11",
    "cross-fetch": "^4.0.0",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "eslint": "^8.51.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
