{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "developer-tools",
  "title": "Developer Tools",
  "description": "A collection of developer tools at your fingertips",
  "icon": "icon.png",
  "author": "wxiaoyun",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "timestamp-converter",
      "title": "Timestamp Converter",
      "description": "Convert Unix timestamps and display their components with timezone support",
      "mode": "view"
    },
    {
      "name": "url-encode-decode",
      "title": "Url Encode / Decode",
      "description": "Encode / Decode a URL",
      "mode": "view"
    },
    {
      "name": "json-unescape",
      "title": "Json Unescape",
      "description": "Unescape a JSON string",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "latest",
    "@raycast/utils": "latest",
    "slashes": "^3.0.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "^22.14.1",
    "@types/react": "19.1.2",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
