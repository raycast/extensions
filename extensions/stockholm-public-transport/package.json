{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stockholm-public-transport",
  "title": "SL",
  "description": "Departures for public transport in Stockholm",
  "icon": "command-icon.png",
  "author": "albingroen",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "departures",
      "title": "Departures",
      "subtitle": "Departures for a specific station",
      "description": "View departures for a specific station and travel method",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.2",
    "axios": "^1.3.5",
    "swr": "^2.1.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish",
    "format": "prettier --write ."
  }
}
