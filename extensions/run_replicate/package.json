{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "run_replicate",
  "title": "Run Replicate Models",
  "description": "Run replicate models",
  "icon": "replicate.png",
  "author": "charlieholtz",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Replicate",
      "description": "Run replicate model",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "token",
      "type": "password",
      "title": "API Token",
      "description": "Add your token from Replicate",
      "placeholder": "Token",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.7",
    "delay": "^5.0.0",
    "fs": "^0.0.1-security",
    "node-fetch": "^3.3.0",
    "open": "^8.4.2",
    "open-graph-scraper": "^5.1.1",
    "replicate": "^0.4.0",
    "run-applescript": "^6.1.0",
    "tempy": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
