{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "avatar",
  "title": "Avatar",
  "description": "An avatar library for designers and developers. You can choose between simple identicons and lovely designed characters.",
  "icon": "avatar-icon.png",
  "author": "koinzhang",
  "categories": [
    "Design Tools",
    "Developer Tools",
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "generate-avatar",
      "title": "Generate Avatar",
      "subtitle": "Avatar",
      "description": "Generate your own avatar with custom seed.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@multiavatar/multiavatar": "^1.0.7",
    "@raycast/api": "^1.35.1",
    "axios": "^0.27.2",
    "d3-color": "^3.1.0",
    "fs-extra": "^10.1.0",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/d3-color": "^3.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
