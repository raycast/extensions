{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "party-parrot",
  "title": "Party Parrot",
  "description": "Celebrating the good times with some dancing party parrot in Raycast.",
  "icon": "icon.png",
  "author": "thomas",
  "license": "MIT",
  "categories": [
    "Fun"
  ],
  "platforms": [
    "macOS",
    "Windows"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Celebrate",
      "subtitle": "Party Parrot",
      "description": "Show a dancing party parrot in Raycast.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "use-interval": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^3.5.3",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
