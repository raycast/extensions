{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mongodb-objectid",
  "title": "Mongodb ObjectId",
  "description": "Convert Mongodb ObjectId to Timestamp",
  "icon": "command-icon.png",
  "author": "wheatdog",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Convert MongoDB ObjectId",
      "description": "Convert MongoDB ObjectId to time",
      "mode": "view",
      "arguments": [
        {
          "name": "input_id",
          "placeholder": "Object ID",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.3",
    "dayjs": "^1.11.7",
    "mongodb": "^5.5.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
