{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mongodb-objectid",
  "title": "MongoDB ObjectId",
  "description": "A Raycast extension that transform between MongoDB ObjectId and timestamps.",
  "icon": "extension-icon.png",
  "author": "tssujt",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "convert-objectid-to-timestamp",
      "title": "Convert ObjectId to Timestamp",
      "description": "Convert ObjectId to Timestamp",
      "mode": "view",
      "arguments": [
        {
          "name": "objectid",
          "placeholder": "ObjectId",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "convert-timestamp-to-objectid",
      "title": "Convert Timestamp to ObjectId",
      "description": "Convert Timestamp to ObjectId",
      "mode": "view",
      "arguments": [
        {
          "name": "timestamp",
          "placeholder": "timestamp",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.87.4",
    "@raycast/utils": "^1.17.0",
    "mongodb": "^6.11.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
