{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "neue-dokumente-erstellen",
  "title": "Neue Dokumente erstellen",
  "description": "Hiermit kannst du schnell neue Dokumente erstellen",
  "icon": "command-icon.png",
  "author": "borgardt_sven",
  "license": "MIT",
  "commands": [
    {
      "name": "dokumente_erstellen",
      "title": "Dokumente erstellen",
      "description": "Schnell neue Dokumente erstellen",
      "mode": "view"
    },
    {
      "name": "dokumente_durchsuchen",
      "title": "Dokumente durchsuchen",
      "description": "Schnell auf Dokumente zugreifen",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",  
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}