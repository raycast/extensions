{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "webflow-sites",
  "title": "Webflow",
  "description": "Supercharged productivity for Webflow. Bring Webflow to every corner of your OS. Open any project from anywhere. Site settings and more.",
  "icon": "webflow-logo.png",
  "author": "peeks",
  "contributors": [
    "pernielsentikaer"
  ],
  "keywords": [
    "webflow",
    "web",
    "development",
    "peeks"
  ],
  "categories": [
    "Productivity",
    "Design Tools",
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Sites",
      "subtitle": "Webflow",
      "description": "List all your sites. Choose to open a site in designer or in production.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "webflowToken",
      "title": "Webflow API Token",
      "description": "Generate at: https://www.peeks.co/raycast",
      "type": "password",
      "required": true,
      "placeholder": "Paste Webflow API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0",
    "@raycast/utils": "^1.4.11"
  },
  "devDependencies": {
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
