{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "webflow-sites",
  "title": "Webflow",
  "description": "Supercharged productivity for Webflow. Bring Webflow to every corner of your OS. Open any project from anywhere. Site settings and more.",
  "icon": "webflow-logo.png",
  "author": "peeks",
  "contributors": [
    "pernielsentikaer",
    "xmok"
  ],
  "keywords": [
    "webflow",
    "web",
    "development",
    "peeks"
  ],
  "categories": [
    "Productivity",
    "Design Tools",
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Sites",
      "subtitle": "Webflow",
      "description": "List all your sites. Choose to open a site in designer or in production.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "webflowToken",
      "title": "Webflow API Token",
      "description": "Generate at: https://www.peeks.co/raycast",
      "type": "password",
      "required": false,
      "placeholder": "Paste Webflow API Token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0",
    "cross-fetch": "^4.0.0",
    "webflow-api": "^3.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "@types/request": "^2.48.8",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "react": "^18.2.0",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
