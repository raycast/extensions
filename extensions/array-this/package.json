{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "array-this",
  "title": "Array This",
  "description": "Format list of items into an array.",
  "icon": "command-icon.png",
  "author": "matheus_de_paula_domingos",
  "categories": [
    "Developer Tools",
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Array This",
      "description": "Returns an array from a list of values separated by break lines.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
