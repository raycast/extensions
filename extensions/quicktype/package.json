{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "quicktype",
    "title": "Quicktype",
    "description": "Generate Models and Serializers from JSON",
    "icon": "icon.png",
    "author": "Aayush9029",
    "categories": [
        "Developer Tools"
    ],
    "license": "MIT",
    "commands": [{
        "name": "index",
        "title": "Quicktype",
        "subtitle": "Convert JSON to Model",
        "description": "Generate models and serializers from JSON, schema, and GraphQL for working with data quickly & safely in any programming language.",
        "mode": "view"
    }],
    "dependencies": {
        "@raycast/api": "^1.31.0",
        "quicktype-core": "^6.0.71"
    },
    "devDependencies": {
        "@types/node": "~16.10.0",
        "@types/react": "^17.0.28",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "react-devtools": "^4.19.2",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint"
    }
}