{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "json2ts",
  "title": "Json2TS",
  "version": "0.0.1",
  "description": "Generate TypeScript interfaces from JSON",
  "icon": "command-icon.png",
  "author": "gbarba",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Json2ts",
