{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-maven-repository",
  "title": "Google Maven Repository",
  "description": "Show and search for artifacts in Google's Maven repository.",
  "icon": "google-maven.png",
  "author": "koinzhang",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "keywords": [
    "maven",
    "google",
    "repository",
    "artifact",
    "android",
    "library",
    "dependency"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "show-google-maven-repository",
      "title": "Show Google Maven Repository",
      "icon": "show-google-maven.png",
      "description": "Show All artifacts in Google's Maven repository.",
      "mode": "view"
    },
    {
      "name": "search-google-maven-repository",
      "title": "Search Google Maven Repository",
      "icon": "search-google-maven-repository.png",
      "description": "Search for an artifact in Google's Maven repository.",
      "mode": "view",
      "arguments": [
        {
          "name": "repository",
          "placeholder": "Repository",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.76.0",
    "@raycast/utils": "^1.15.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "~20.14.2",
    "@types/react": "^18.3.3",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.3.1",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
