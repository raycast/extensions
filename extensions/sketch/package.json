{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "sketch",
  "title": "Sketch",
  "description": "Frictionless Workspace Browsing",
  "icon": "icon.png",
  "author": "aesign",
  "license": "MIT",
  "preferences": [
    {
      "name": "email",
      "title": "Email",
      "description": "Your Sketch Account's email",
      "type": "textfield",
      "required": true,
      "placeholder": "Insert your Sketch email"
    },
    {
      "name": "password",
      "title": "Password",
      "description": "Your Sketch Account's password",
      "type": "password",
      "required": true,
      "placeholder": "Insert your Sketch password"
    }
  ],
  "commands": [
    {
      "name": "searchDocuments",
      "title": "Documents",
      "subtitle": "Sketch",
      "description": "Browse your Sketch Documents",
      "mode": "view"
    },
    {
      "name": "searchProjects",
      "title": "Projects",
      "subtitle": "Sketch",
      "description": "Browse your Sketch Projects",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "javascript-time-ago": "^2.3.9",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
