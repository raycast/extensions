{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "defly-io",
  "title": "Defly.io Server Info",
  "description": "Get defly.io teams and defuse info",
  "icon": "command-icon.png",
  "author": "isra",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Get Teams",
      "description": "Get defly.io teams and defuse info",
      "mode": "view",
      "icon": "command-icon.png",
      "arguments": [
        {
          "name": "region",
          "placeholder": "Region",
          "type": "text",
          "required": false
        },
        {
          "name": "port",
          "placeholder": "port",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.49.1",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
