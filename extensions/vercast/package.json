{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vercast",
  "title": "Vercel",
  "description": "View and modify your Vercel projects and deployments.",
  "icon": "icon.png",
  "author": "MaxLeiter",
  "license": "MIT",
  "access": "public",
  "contributors": [
    "matt",
    "dgrcode"
  ],
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Explore your Vercel projects",
      "mode": "view"
    },
    {
      "name": "search-deployments",
      "title": "Search Deployments",
      "description": "Explore your Vercel deployments",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Vercel access token",
      "placeholder": "Enter your Access Token",
      "link": "https://vercel.com/account/tokens"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.1",
    "@raycast/utils": "^1.0.6",
    "node-fetch": "^3.1.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "lint:fix": "ray lint --fix"
  }
}
