{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vercast",
  "title": "Vercel",
  "description": "View and modify your Vercel projects, deployments and domains.",
  "icon": "icon.png",
  "license": "MIT",
  "author": "MaxLeiter",
  "owner": "vercel",
  "access": "public",
  "contributors": [
    "matt",
    "dgrcode",
    "samuelkraft",
    "xmok"
  ],
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Explore your Vercel projects",
      "mode": "view"
    },
    {
      "name": "search-deployments",
      "title": "Search Deployments",
      "description": "Explore your Vercel deployments",
      "mode": "view"
    },
    {
      "name": "search-components",
      "title": "Search Components",
      "description": "Search the Vercel styleguide",
      "mode": "view"
    },
    {
      "name": "search-domains",
      "title": "Search Domains",
      "description": "Explore your Vercel domains",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Vercel access token",
      "placeholder": "Enter your Access Token",
      "link": "https://vercel.com/account/tokens"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.16.2",
    "node-fetch": "^3.1.0",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
