{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "vercast",
  "title": "Vercel",
  "description": "View and modify your Vercel projects and deployments.",
  "icon": "icon.png",
  "license": "MIT",
  "author": "MaxLeiter",
  "owner": "vercel",
  "access": "public",
  "contributors": [
    "matt",
    "dgrcode",
    "bgiovand"
  ],
  "commands": [
    {
      "name": "search-projects",
      "title": "Search Projects",
      "description": "Explore your Vercel projects",
      "mode": "view"
    },
    {
      "name": "search-deployments",
      "title": "Search Deployments",
      "description": "Explore your Vercel deployments",
      "mode": "view"
    },
    {
      "name": "search-components",
      "title": "Search Components",
      "description": "Search the Vercel styleguide",
      "mode": "view"
    },
    {
      "name": "menubar-list-deployments",
      "title": "List Deployments",
      "description": "List your Vercel deployments",
      "mode": "menu-bar",
      "interval": "1m"
    }
  ],
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Vercel access token",
      "placeholder": "Enter your Access Token",
      "link": "https://vercel.com/account/tokens"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.1",
    "@raycast/utils": "^1.4.14",
    "node-fetch": "^3.3.0",
    "swr": "^2.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "react-devtools": "^4.27.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "lint:fix": "ray lint --fix"
  }
}
