{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "transmit",
  "title": "Transmit",
  "description": "Displays a list of servers bookmarked in Transmit for quick connecting.",
  "icon": "command-icon.png",
  "author": "kennonb",
  "categories": [
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Servers",
      "subtitle": "Transmit",
      "description": "List and open Transmit servers",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@jxa/global-type": "^1.3.5",
    "@jxa/run": "^1.3.5",
    "@raycast/api": "^1.34.1",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
