{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "quick-call",
  "title": "Quick Phone Call",
  "description": "Make a phone call with the default telephone app.",
  "icon": "phone-icon.png",
  "author": "karbassi",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "quick-call",
      "title": "Phone Call",
      "description": "Make a quick phone call",
      "mode": "no-view",
      "arguments": [
        {
          "name": "number",
          "placeholder": "phone number",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.6",
    "@types/node": "~20.5.0",
    "@types/react": "^18.2.20",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.1",
    "react-devtools": "^4.28.0",
    "typescript": "^5.1.6"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
