{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "neodb",
  "title": "NeoDB",
  "description": "Search NeoDB items and view item details",
  "icon": "command-icon.png",
  "author": "SevicheCC",
  "categories": [
    "Web",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search",
      "title": "Search",
      "subtitle": "NeoDB",
      "description": "Search items",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.4",
    "@raycast/utils": "^1.8.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}