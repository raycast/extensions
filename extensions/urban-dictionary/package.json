{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "urban-dictionary",
  "title": "Urban Dictionary Search",
  "description": "Search on Urban Dictionary",
  "icon": "urban.png",
  "author": "pernielsentikaer",
  "contributors": [
    "zhenpewu",
    "xilopaint"
  ],
  "categories": [
    "Web",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Urban Dictionary",
      "description": "Lookup terms on Urban Dictionary",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "type": "text",
          "required": false,
          "placeholder": "Query"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.81.0",
    "@raycast/utils": "^1.16.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
