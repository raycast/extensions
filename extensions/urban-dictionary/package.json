{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "urban-dictionary",
  "title": "Urban Dictionary Search",
  "description": "Search on urban dictionary",
  "icon": "urban.png",
  "author": "Sheraff",
  "contributors": [
    "zhenpewu",
    "pernielsentikaer"
  ],
  "categories": [
    "Web",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Urban Dictionary",
      "description": "Lookup terms on Urban Dictionary",
      "mode": "view",
      "arguments": [
        {
          "name": "query",
          "type": "text",
          "required": false,
          "placeholder": "Query"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.53.0",
    "@raycast/utils": "^1.7.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "publish": "npx @raycast/api@latest publish",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
