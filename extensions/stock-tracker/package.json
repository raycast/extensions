{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stock-tracker",
  "title": "Stock Tracker",
  "description": "Shows stock market data for individual stocks or your portfolio by using Yahoo Finance.",
  "icon": "extension-icon.png",
  "author": "hmarr",
  "categories": [
    "Finance"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Stocks",
      "description": "Look up stocks by symbol",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "cross-fetch": "^3.1.5"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
