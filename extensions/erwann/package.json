{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "erwann",
  "title": "Erwann",
  "description": "A simple plugin that says Bonjour",
  "icon": "command-icon.png",
  "author": "umar_farouk_umar",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "bonjour",
      "title": "Say Bonjour",
      "description": "Says Bonjour",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.60.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.6",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^8.19.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}