{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "speed-dial",
  "title": "Speed Dial",
  "description": "Stop asking for meeting URLs and add them to speed dial to open with one click",
  "icon": "extension-icon.png",
  "author": "osama_aamer",
  "categories": [
    "Communication",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list-rooms",
      "title": "View Rooms",
      "description": "Show a list of your saved meeting rooms",
      "mode": "view",
      "arguments": [
        {
          "name": "room",
          "placeholder": "Name",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "add-room",
      "title": "Add Room",
      "description": "Add a new meeting room",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.3",
    "@raycast/utils": "^1.7.1",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}