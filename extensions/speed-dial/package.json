{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "speed-dial",
  "title": "Speed Dial",
  "description": "Add contacts to speed dial to start meetings with one click",
  "icon": "command-icon.png",
  "author": "osama_aamer",
  "categories": [
    "Communication",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Contacts",
      "description": "Show saved contacts",
      "mode": "view"
    },
    {
      "name": "form",
      "title": "Add Contact",
      "description": "Add a new contact",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.3",
    "@raycast/utils": "^1.7.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}