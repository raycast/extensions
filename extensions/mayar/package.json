{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mayar",
  "title": "Mayar",
  "description": "Mayar Raycast Extensions",
  "icon": "mayar-command-icon.png",
  "author": "friantom",
  "categories": [
    "Web",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "mayar",
      "title": "Mayar Balance",
      "description": "My Mayar balance",
      "mode": "view",
      "preferences": [
        {
          "name": "required-password",
          "type": "password",
          "required": true,
          "title": "Mayar API Key",
          "description": "Your api key",
          "placeholder": "Enter your api key here"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "1.38.0",
    "@raycast/utils": "1.0.3",
    "axios": "1.3.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}