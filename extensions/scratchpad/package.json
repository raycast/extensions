{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "scratchpad",
  "title": "Scratchpad",
  "description": "Creates a scratchpad file and opens it in an editor",
  "icon": "scratchpad-icon.png",
  "author": "asnimansari",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Scratchpad",
      "description": "Creates a new Scratchpad file",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.75.1",
    "@raycast/utils": "^1.16.1",
    "fs-extra": "^11.2.0",
    "moment": "^2.30.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "3.2.5",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "categories": [
    "Productivity",
    "Developer Tools"
  ]
}
