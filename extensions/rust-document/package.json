{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rust-documentation",
  "title": "Rust Documentation",
  "description": "Search Rust documentation quickly",
  "icon": "rust.png",
  "author": "waynexia",
  "categories": [
    "Developer Tools",
    "Documentation",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Rust",
      "description": "Search Rust documentation quickly",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.41.1",
    "got": "^12.5.2",
    "node-html-parser": "^6.1.4"
  },
  "devDependencies": {
    "@types/node": "16.10.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}