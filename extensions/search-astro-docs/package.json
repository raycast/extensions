{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "search-astro-docs",
  "title": "search-astro-docs",
  "description": "Search the Astro Documentation right from Raycast",
  "icon": "astro-search-icon.png",
  "author": "ElianCodes",
  "categories": [
    "Documentation",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Astro Documentation",
      "description": "Search the Astro documentation right from Raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
