{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "outline-page",
  "title": "Outline Page",
  "description": "Check for CSS bugs by outlining all the elements in your web page.",
  "icon": "command-icon.png",
  "author": "praveenjuge",
  "categories": [
    "Developer Tools",
    "Design Tools"
  ],
  "keywords": [
    "webpage",
    "outline",
    "CSS",
    "outlining",
    "design",
    "designer",
    "developer",
    "development"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "outline-page",
      "title": "Outline Page",
      "description": "Outline all the elements in this page.",
      "mode": "no-view",
      "keywords": [
        "outline",
        "webpage",
        "grid"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.14.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.12.7",
    "@types/react": "18.3.1",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
