{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "oh-my-zsh-git-alias",
  "title": "Oh My Zsh Git Alias",
  "description": "Quick search git alias.",
  "icon": "icon.png",
  "author": "yufeikang",
  "categories": [
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Git Alias",
      "description": "Search Oh-My-Zsh Git Alias",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
