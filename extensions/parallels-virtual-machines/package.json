{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "parallels-virtual-machines",
  "title": "Parallels Virtual Machines",
  "description": "Launch and control virtual machines in Parallels Desktop.",
  "icon": "command-icon.png",
  "author": "danpalmer",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Virtual Machines",
      "subtitle": "Parallels Desktop",
      "description": "Shows virtual machines in Parallels Desktop.",
      "keywords": [
        "open",
        "parallels",
        "vm"
      ],
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "@raycast/utils": "^1.4.0",
    "node-fetch": "^3.0.0",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  },
  "platforms": [
    "macOS"
  ]
}
