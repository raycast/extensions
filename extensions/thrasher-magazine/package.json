{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "thrasher-magazine",
  "title": "Thrasher Magazine",
  "description": "Read the latest news of Thrasher Magazine",
  "icon": "thrasher-logo.png",
  "author": "Whitespace",
  "categories": [
    "News",
    "Media",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Thrasher Magazine",
      "description": "Get the latest Thrasher news",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.10.0",
    "user-agents": "^1.0.1444"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/user-agents": "^1.0.2",
    "eslint": "^7.32.0",
    "prettier": "^3.0.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
