{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "trending-list",
  "title": "Trending List",
  "description": "Quickly access and view the trending searches of popular platforms, such as Zhihu, Baidu, Weibo, and Bilibili.",
  "icon": "command-icon.png",
  "author": "wenyang.jay",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Trending List",
      "description": "show trending list",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.1",
    "@raycast/utils": "^1.7.0",
    "cheerio": "^1.0.0-rc.12",
    "form-data": "^4.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
