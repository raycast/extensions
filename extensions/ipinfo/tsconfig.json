{
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["src/**/*.ts", "src/**/*.tsx", "raycast-env.d.ts"],
  "compilerOptions": {
    "target": "es2020",
    "lib": ["es2020"],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "jsx": "react-jsx",
    "rootDir": "src",
    "outDir": "dist",
    "skipLibCheck": true
  }
}
