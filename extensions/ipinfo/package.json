{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ipinfo",
  "title": "IPInfo",
  "description": "Fetch and display information about IP addresses",
  "icon": "ipinfo.png",
  "author": "ChrisWho",
  "categories": [
    "Developer Tools",
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Query IP Information",
      "description": "Fetch and display information about an IP address",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "node-fetch": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
