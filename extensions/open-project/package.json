{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "open-project",
  "title": "Open Project",
  "description": "Search through your list of projects and quickly open them in your editor",
  "icon": "command-icon.png",
  "author": "amir-ziaei",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open Project",
      "description": "Search through your list of projects and quickly open them in your editor",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "@raycast/utils": "^1.4.8",
    "fuse.js": "^7.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
