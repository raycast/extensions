{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "onenote",
  "title": "OneNote",
  "description": "Search Microsoft OneNote notes",
  "icon": "onenote-sf-icon.png",
  "author": "alewandowski",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Notes",
      "description": "Search notes in the Microsoft OneNote app",
      "mode": "view"
    },
    {
      "name": "browse",
      "title": "Browse Notes",
      "description": "Browse Microsoft Onenote notes hierarchy",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "@raycast/utils": "^1.4.16",
    "dateformat": "^5.0.3",
    "file-url": "^4.0.0",
    "run-applescript": "^6.1.0",
    "sql.js": "^1.8.0"
  },
  "devDependencies": {
    "@types/dateformat": "^5.0.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/sql.js": "^1.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
