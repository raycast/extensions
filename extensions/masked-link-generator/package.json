{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "masked-link-generator",
  "title": "Masked Link Generator",
  "description": "Input a destination URL and the title of the link and receive a masked link in your clipboard.",
  "icon": "command-icon.png",
  "author": "Visual-Studio-Coder",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Masked Link",
      "description": "Input a valid URL and a title, and receive a masked link in your clipboard",
      "mode": "no-view",
      "keywords": [
        "masked",
        "link",
        "generator",
        "URL",
        "Markdown",
        "text",
        "Discord",
        "embed"
      ],
      "arguments": [
        {
          "name": "name",
          "type": "text",
          "placeholder": "Title",
          "required": true
        },
        {
          "name": "url",
          "type": "text",
          "placeholder": "URL",
          "required": true
        }

      ]

    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}