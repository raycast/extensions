{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "php-docs",
  "title": "PHP Documentation Search",
  "description": "Quickly search through official PHP documentation",
  "icon": "php.png",
  "author": "comes",
  "version": "1.0.0",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Developer Tools",
    "Documentation",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "php",
      "title": "Search Documentation",
      "subtitle": "PHP",
      "description": "Search the PHP.net documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.2",
    "cheerio": "*",
    "node-fetch": "^3.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "node-ray": "^1.18.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
