{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mac-app-store-search",
  "title": "Mac App Store Search",
  "description": "Search for apps in the Mac App Store from Raycast!",
  "icon": "icon.png",
  "author": "say4n",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search App Store",
