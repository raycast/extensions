{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cursor",
  "title": "Cursor",
  "description": "Search the Cursor Documentation right from Raycast",
  "icon": "cursor-icon.png",
  "author": "degouville",
  "categories": ["Documentation", "Developer Tools"],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
