{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-wca",
  "title": "WCA",
  "description": "Shows you the WCA profile for a name or WCA ID",
  "icon": "command-icon.png",
  "author": "EvanZhouDev",
  "categories": [
    "Fun"
  ],
  "contributors": [
    "Visual-Studio-Coder"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search WCA",
      "description": "Search for WCA profile their WCA ID",
      "mode": "view",
      "arguments": [
        {
          "name": "ID",
          "placeholder": "WCA ID",
          "type": "text",
          "required": true
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.1",
    "@raycast/utils": "^1.9.0",
    "cheerio": "^1.0.0-rc.12"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
