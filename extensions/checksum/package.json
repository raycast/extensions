{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "checksum",
  "title": "Checksum",
  "description": "An awesome, simple to use app that can do checksums. More hashing algorithms coming soon",
  "icon": "command_icon.png",
  "author": "Ek217",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Run a checksum",
      "description": "Run a checksum",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}