{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dr-news",
  "title": "DR News",
  "description": "Showcases news updates from DR, Denmark's premier broadcaster, categorized by various topics.",
  "icon": "icon.png",
  "author": "kenanbendz",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "News"
  ],
  "license": "MIT",
  "keywords": [
    "DR",
    "Danmarks Radio"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Get Latest News",
      "subtitle": "DR News",
      "description": "Get the latest news from DR.dk",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.59.0",
    "@raycast/utils": "^1.10.0",
    "rss-parser": "^3.13.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^18.8.4",
    "@types/react": "^18.2.27",
    "eslint": "^7.32.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
