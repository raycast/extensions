{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "multilinks",
    "title": "Multilinks",
    "description": "Open multiple links at once",
    "icon": "command-icon.png",
    "author": "abhisheksatre",
    "categories": [
        "Productivity"
    ],
    "license": "MIT",
    "commands": [
        {
            "name": "multi-links",
            "title": "Multilinks",
            "description": "List of Multilinks, edit delete or open.",
            "mode": "view"
        },
        {
            "name": "create-link",
            "title": "Create MultiLink",
            "description": "Create new MultiLink",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.35.2"
    },
    "devDependencies": {
        "@types/node": "~16.10.0",
        "@types/react": "^17.0.28",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint"
    }
}
