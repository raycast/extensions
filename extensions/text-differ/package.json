{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "text-differ",
  "title": "Text Differ",
  "description": "Opens Text Differ from selected files",
  "icon": "command-icon.png",
  "author": "ErikFisher",
  "categories": [
    "Applications",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "open-with-text-differ",
      "title": "Open With Text Differ",
      "description": "When two files are selected in finder the command will open them in Text Differ",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.63.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
