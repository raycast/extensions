{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "screen-sharing-recents",
  "title": "Screen Sharing Recents",
  "description": "Adds shortcuts to recent screen sharing locations",
  "icon": "command-icon.png",
  "author": "bix",
  "categories": [
    "Productivity"
  ],
  "keywords": [
    "screen-sharing"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Recent Sessions",
      "subtitle": "Screen Sharing",
      "description": "Gets a searchable list of Screen Sharing recent locations.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
