{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-gauthenticator",
  "title": "Raycast Gauthenticator",
  "description": "Quickly access your two-factor authentication codes.",
  "icon": "command-icon.png",
  "author": "Dong",
  "license": "MIT",
  "keywords": [
    "auth",
    "2fa",
    "authentication",
    "gauth"
  ],
  "commands": [
    {
      "name": "index",
      "title": "Generate 2FA Code",
      "description": "Quickly access your two-factor authentication codes.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.27.1",
    "hi-base32": "^0.5.1",
    "ini": "^2.0.0"
  },
  "devDependencies": {
    "@types/ini": "^1.3.31",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
