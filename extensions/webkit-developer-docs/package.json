{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "webkit-developer-docs",
  "title": "WebKit Developer Documentation",
  "description": "Search and view WebKit docs",
  "icon": "command-icon.png",
  "author": "KevinBatdorf",
  "categories": [
    "Productivity",
    "Developer Tools",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "WebKit",
      "description": "View and search WebKit developer documentation",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.50.2",
    "@raycast/utils": "^1.5.2",
    "html2commonmark": "^0.6.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
