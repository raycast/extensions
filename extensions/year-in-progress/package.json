{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "year-in-progress",
  "title": "Year in Progress",
  "description": "Stay motivated by see the progress of the year.",
  "icon": "icon.png",
  "author": "thomas",
  "contributors": [
    "anothertempore",
    "akshay_khade",
    "teemu_suvinen"
  ],
  "pastContributors": [
    "danulqua"
  ],
  "categories": [
    "Productivity",
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "year-in-progress",
      "title": "Progress",
      "description": "Displays the year in progress inline.",
      "mode": "no-view",
      "interval": "4h"
    },
    {
      "name": "index",
      "title": "Show X in Progress in Menu Bar",
      "description": "Display X In Progress In Menu Bar",
      "mode": "menu-bar",
      "interval": "2m"
    },
    {
      "name": "x-in-progress",
      "title": "X in Progress",
      "description": "Display and manage X In Progress In Command",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "weekStartsOn",
      "description": "Week starts date",
      "type": "dropdown",
      "required": true,
      "title": "Week Starts On",
      "placeholder": "Monday",
      "data": [
        {
          "title": "Sunday",
          "value": "0"
        },
        {
          "title": "Monday",
          "value": "1"
        },
        {
          "title": "Tuesday",
          "value": "2"
        },
        {
          "title": "Wednesday",
          "value": "3"
        },
        {
          "title": "Thursday",
          "value": "4"
        },
        {
          "title": "Friday",
          "value": "5"
        },
        {
          "title": "Saturday",
          "value": "6"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.8",
    "date-fns": "^2.29.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
