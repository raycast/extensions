{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chatgpt3-prompt",
  "title": "ChatGPT3 Prompt",
  "description": "Prompt list for interacting with ChatGPT3",
  "icon": "command-icon.png",
  "author": "denizyoldas",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "GPT3 Prompts Search",
      "description": "OpenAI GPT3 Prompt List",
      "mode": "view",
      "preferences": [
        {
          "name": "primaryAction",
          "type": "dropdown",
          "description": "The primary action to perform on a selected prompt.",
          "required": false,
          "title": "Primary Action",
          "default": "copy",
          "data": [
            {
              "title": "Paste to Active App",
              "value": "paste"
            },
            {
              "title": "Copy to Clipboard",
              "value": "copy"
            }
          ]
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "axios": "^1.3.3",
    "csv-parse": "^5.3.5"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
