{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "spotify-player",
  "title": "Spotify Player",
  "description": "Search and Play Spotify music from the Raycast, control your library with convinient commands like Like",
  "icon": "icon.png",
  "author": "mattisssa",
  "contributors": [
    "dillionverma",
    "andreaselia"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "searchTracks",
      "title": "Search Tracks",
      "subtitle": "Spotify Player",
      "description": "Search & Play tracks in Spotify",
      "mode": "view"
    },
    {
      "name": "searchAlbums",
      "title": "Search Albums",
      "subtitle": "Spotify Player",
      "description": "Search & Play albums in Spotify",
      "mode": "view"
    },
    {
      "name": "searchPlaylists",
      "title": "Search Playlists",
      "subtitle": "Spotify Player",
      "description": "Search & Play playlists in Spotify",
      "mode": "view"
    },
    {
      "name": "searchArtists",
      "title": "Search Artists",
      "subtitle": "Spotify Player",
      "description": "Search Artists in Spotify",
      "mode": "view"
    },
    {
      "name": "like_current_song",
      "title": "Like Current Song",
      "subtitle": "Spotify Player",
      "description": "Likes currently playing song",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.0",
    "lodash": "^4.17.21",
    "node-fetch": "^3.2.3",
    "run-applescript": "^6.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.180",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "js-base64": "^3.6.1",
    "open": "^8.2.1",
    "prettier": "^2.6.2",
    "react-devtools": "^4.19.2",
    "spotify-web-api-node": "^5.0.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}