{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "connect-to-vpn",
  "title": "Connect to VPN",
  "description": "Connect to/disconnect from VPN",
  "icon": "command-icon.png",
  "author": "sato11",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Show Network Services",
