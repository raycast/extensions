{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dig",
  "title": "Dig - DNS Lookup",
  "description": "DNS lookup and nameserver query using the built-in host command",
  "icon": "command-icon.png",
  "author": "danielbahl",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "pernielsentikaer",
    "j3lte"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Query Hostname",
      "subtitle": "dig",
      "description": "Returns NS-records from the hostname",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.3",
    "@raycast/utils": "^1.13.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/node": "~20.11.28",
    "@types/react": "^18.2.67",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
