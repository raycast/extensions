{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "power-save",
  "title": "Power Save",
  "description": "Toggle Mac OS power saving (Low Power Mode)",
  "icon": "command-icon.png",
  "author": "arian",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Toggle Low Power",
      "description": "Toggle power saving setting",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.67.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
