{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "valheim-wiki",
  "title": "Valheim Wiki Search",
  "description": "Search the Valheim Wiki, display a preview and open the full article in your browser.",
  "icon": "command-icon.png",
  "author": "constantin_prum",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Wiki",
      "subtitle": "Valheim",
      "description": "Searches the Valheim Wiki",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.3",
    "@raycast/utils": "^1.17.0",
    "cheerio": "^1.0.0",
    "node-html-markdown": "^1.3.0",
    "react-native-cheerio": "^1.0.0-rc.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
