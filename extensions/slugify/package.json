{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "slugify",
  "title": "Slugify",
  "description": "A quick way to slugify text",
  "icon": "slugify.png",
  "author": "unreadablename",
  "categories": [
    "Developer Tools",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Slugify",
      "description": "Slugify text using different options",
      "mode": "view"
    },
    {
      "name": "slugify",
      "title": "Slugify Clipboard",
      "description": "Overwrite the clipboard contents with an slugified string",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "slugify": "^1.6.5"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
