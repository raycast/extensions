{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-muisearch",
  "title": "@mui",
  "description": "Search @mui docs",
  "icon": "mui-icon.png",
  "author": "ubeytdemir",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
