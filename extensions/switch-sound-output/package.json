{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "switch-sound-output",
  "title": "Switch Sound Output",
  "description": "Switches the sound output.",
  "icon": "command-icon.png",
  "author": "hladik-dan",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Switch Sound Output",
      "description": "Switches the sound output.",
      "mode": "view",
      "preferences": [
        {
          "name": "utility",
          "title": "Utility",
          "description": "Location of the \"switch-audio\" utility.",
          "type": "textfield",
          "required": true,
          "default": "/opt/homebrew/bin/switch-audio"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.26.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
