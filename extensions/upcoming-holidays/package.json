{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "upcoming-holidays",
  "title": "Upcoming Holidays",
  "description": "Show upcoming holidays for a country",
  "icon": "calendar-icon.png",
  "author": "rodrigouroz",
  "categories": [
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Upcoming Holidays",
      "description": "Allows you to see the upcoming holidays for a country",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.35.2",
    "country-locale-map": "^1.8.0",
    "moment": "^2.29.3",
    "node-fetch": "^3.2.4",
    "swr": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
