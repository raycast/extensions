{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "raycast-monkeytype-theme",
  "title": "Raycast MonkeyType Theme Explorer",
  "description": "This \"Raycast MonkeyType Theme\" extension provides MonkeyType themes for Raycast.",
  "icon": "command-icon.png",
  "author": "ridemountainpig",
  "categories": [
    "Applications",
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "raycast-monkeytype-theme",
      "title": "MonkeyType Themes",
      "description": "Show detail of MonkeyType themes for Raycast.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "@raycast/utils": "^1.15.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "format": "npx prettier --write .",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
