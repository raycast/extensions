{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "stoicquotes",
  "title": "Stoic Quotes",
  "description": "Gives a random quote from one of the stoic philosophers - Marcus Aurelius, Seneca or Epictetus",
  "icon": "command-icon.png",
  "author": "rrajath",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Random Stoic Quote",
      "description": "Get a random quote from one of the stoic philosophers - Marcus Aurelius, Seneca or Epictetus",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "@raycast/utils": "^1.4.16"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.8.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
