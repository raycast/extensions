{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cc0-lib",
  "title": "CC0-LIB - Browse CC0 Content",
  "description": "Browse library of CC0 content",
  "icon": "cc0-lib-circle.png",
  "author": "0xN1",
  "categories": [
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Get Random Content",
      "description": "Get random content from cc0-lib",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.55.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@raycast/utils": "^1.9.0",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "version": "1.0.5"
}
