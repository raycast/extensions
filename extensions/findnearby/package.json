{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "findnearby",
  "title": "Google Maps Explorer",
  "description": "Explore new places near you",
  "icon": "command-icon.png",
  "author": "lichinlin",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Find Nearby",
      "subtitle": "Google Maps",
      "description": "Find places nearby on Google Maps.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.1",
    "@raycast/utils": "^1.9.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
