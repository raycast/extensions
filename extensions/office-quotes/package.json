{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "office-quotes",
  "title": "Office Quotes",
  "description": "An extension that displays quotes from NBC's beloved mockumentary series 'The Office'.",
  "icon": "office-api-icon.png",
  "author": "braydoncoyer",
  "contributors": [
    "sandypockets",
    "xmok"
  ],
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "'The Office' Quotes",
      "description": "Quotes from the 'The Office' TV series",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.76.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
