{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "uptime",
  "title": "Uptime",
  "description": "Show how long ago your mac was turned on",
  "icon": "command-icon.png",
  "author": "iaroslav_naiden",
  "categories": [
    "System"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Uptime",
      "description": "Show how long ago your mac was turned on",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0",
    "@raycast/utils": "^1.9.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "version": "0.0.0"
}
