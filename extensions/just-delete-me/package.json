{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "just-delete-me",
  "title": "Just Delete Me",
  "description": "A directory of direct links to delete your account from web services.",
  "icon": "command-icon.png",
  "author": "andreaselia",
  "contributors": [
    "pernielsentikaer"
  ],
  "categories": [
    "Data",
    "Documentation",
    "Security",
    "Web",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Deleteable Sites",
      "description": "A list of sites with information on how to delete your account if possible.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.5",
    "@raycast/utils": "^1.5.2"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
