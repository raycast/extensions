{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "just-delete-me",
  "title": "Just Delete Me",
  "description": "A directory of direct links to delete your account from web services.",
  "icon": "command-icon.png",
  "author": "andreaselia",
  "contributors": [
    "pernielsentikaer",
    "xmok"
  ],
  "categories": [
    "Data",
    "Documentation",
    "Security",
    "Web",
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Deleteable Sites",
      "description": "A list of sites with information on how to delete your account if possible.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.78.0",
    "@raycast/utils": "^1.16.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
