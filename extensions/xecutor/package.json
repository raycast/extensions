{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "xecutor",
  "title": "Xecutor",
  "description": "Create presets to launch multiple macOS applications and/or urls from within Raycast",
  "icon": "xecutor.png",
  "author": "GastroGeek",
  "license": "MIT",
  "categories": [
    "Applications",
    "Productivity",
    "Web"
  ],
  "keywords": [
    "execute",
    "launcher",
    "shortcut"
  ],
  "commands": [
    {
      "name": "presets",
      "title": "Manage Presets",
      "description": "Create presets to launch multiple macOS applications and/or urls from within Raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.3",
    "lodash.xorby": "^4.7.0",
    "url-parse": "^1.5.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/lodash.xorby": "^4.7.7",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
