{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "bitfinex",
  "title": "Bitfinex",
  "description": "A Raycast extension built for Bitfinex margin funding",
  "icon": "bitfinex-raycast.png",
  "author": "Yukai",
  "license": "MIT",
  "commands": [
    {
      "name": "funding-offers",
      "title": "Manage Funding Offers",
      "subtitle": "Bitfinex",
      "description": "Manage Funding Offer History on Bitfinex",
      "mode": "view"
    },
    {
      "name": "funding-interests",
      "title": "Show Funding Interests",
      "subtitle": "Bitfinex",
      "description": "Show Daily Funding Interest on Bitfinex",
      "mode": "view"
    },
    {
      "name": "lending-rates",
      "title": "Show Lending Rates",
      "subtitle": "Bitfinex",
      "description": "Show Lending Rate History on Bitfinex",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "title": "Bitfinex API Key",
      "name": "api_key",
      "required": true,
      "type": "password",
      "description": "Bitfinex API Key"
    },
    {
      "title": "Bitfinex API Secret",
      "name": "api_secret",
      "required": true,
      "type": "password",
      "description": "Bitfinex API Secret"
    },
    {
      "title": "Default Rate View",
      "name": "default_rate_view",
      "type": "dropdown",
      "description": "Default Rate View",
      "required": true,
      "default": "list",
      "data": [
        {
          "value": "list",
          "title": "List"
        },
        {
          "value": "chart",
          "title": "Chart"
