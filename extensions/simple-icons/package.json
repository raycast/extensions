{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "simple-icons",
  "title": "Simple Icons",
  "type": "module",
  "description": "Browse, Search, and Copy 3000+ popular brand icons.",
  "icon": "simpleicons-icon.png",
  "author": "litomore",
  "categories": [
    "Design Tools",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Simple Icons",
      "description": "Browse, search, and copy 3000+ free SVG icons for popular brands",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.70.2",
    "got": "^14.2.1",
    "simple-icons": "^11.10.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.69",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
