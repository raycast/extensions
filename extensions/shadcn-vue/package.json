{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "shadcn-vue",
  "title": "Shadcn/Vue",
  "description": "Browses shadcn/vue documentation, components, and commands.",
  "icon": "shadcn-vue-icon.png",
  "author": "selemondev",
  "categories": [
    "Developer Tools",
    "Documentation",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "search-documentation",
      "title": "Show Documentation",
      "description": "Browses shadcn/vue documentation.",
      "mode": "view"
    },
    {
      "name": "search-components",
      "title": "Show Components",
      "description": "Browses shadcn/vue components.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.73.2",
    "@raycast/utils": "^1.15.0",
    "ofetch": "^1.3.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
