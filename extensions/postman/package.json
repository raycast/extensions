{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "postman",
  "title": "Postman",
  "description": "Send GET requests in your postman collections.",
  "icon": "postman.png",
  "author": "tarikyildizci",
  "categories": [
    "Applications",
    "Developer Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "sendRequest",
      "title": "Send Request",
      "subtitle": "Postman",
      "description": "Choose a GET request and get a response.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3",
    "@raycast/utils": "^1.4.16",
    "node-fetch": "^3.3.0"
  },
  "preferences": [
    {
      "name": "accessToken",
      "type": "password",
      "required": true,
      "title": "Access Token",
      "description": "Your Postman API key.",
      "placeholder": "Go to your Postman profile to get one."
    }
  ],
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
