{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "gmail",
  "title": "GMail",
  "description": "GMail Extension to manage gmail",
  "icon": "gmail.png",
  "author": "tonka3000",
  "categories": [
    "Communication"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "gmail",
      "title": "Mails",
      "description": "Get the inbox",
      "mode": "view"
    },
    {
      "name": "newwebmail",
      "title": "New Web Mail",
      "description": "Open a new EMail in the Browser",
      "mode": "no-view",
      "icon": "gmail.svg"
    }
  ],
  "preferences": [
    {
      "name": "clientid",
      "description": "Client ID",
      "type": "textfield",
      "required": true,
      "title": "OAuth Client ID"
    }
  ],
  "dependencies": {
    "@googleapis/gmail": "^5.0.0",
    "@raycast/api": "^1.56.1",
    "cross-fetch": "^4.0.0",
    "googleapis": "^123.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}