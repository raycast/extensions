    },
    {
      "name": "accessKeyId",
      "type": "textfield",
      "required": true,
      "title": "AccessKeyId",
      "description": "Please enter accessKeyId",
      "placeholder": "Enter AccessKeyId"
    },
    {
      "name": "accessKeySecret",
      "type": "password",
      "required": true,
      "title": "AccessKeySecret",
      "description": "Please enter accessKeySecret",
      "placeholder": "Enter AccessKeySecret"
    },
    {
      "name": "bucket",
      "type": "textfield",
      "required": true,
      "title": "Bucket",
      "description": "Please enter bucket",
      "placeholder": "Enter Bucket"
    },
    {
      "name": "endPoint",
      "type": "textfield",
      "required": false,
      "title": "EndPoint",
      "description": "Please enter endPoint",
      "placeholder": "http(s)://"
    },
    {
      "name": "useCustomDomain",
      "type": "checkbox",
      "required": false,
      "label": "Use Custom Domain",
      "description": "Use custom domain name"
    },
    {
      "name": "pageSize",
      "type": "textfield",
      "required": false,
      "default": "100",
      "title": "Page Size",
      "description": "Page size of fetching oss data, default is 100, limit to 1000",
      "placeholder": "Enter Page Size"
    },
    {
      "name": "downloadLoc",
      "type": "textfield",
      "required": false,
      "title": "File Download Location",
      "description": "Please enter file download location, default is Desktop",
      "placeholder": "Enter File Download Location"
    },
    {
      "name": "streamThreshold",
      "type": "dropdown",
      "required": false,
      "title": "Download by Stream Threshold",
      "description": "When the file size exceeds the threshold, streaming background downloads are used",
      "default": "50",
      "data": [
        {
          "title": "10M",
          "value": "10"
        },
        {
          "title": "50M",
          "value": "50"
        },
        {
          "title": "100M",
          "value": "100"
        },
        {
          "title": "200M",
          "value": "200"
        }
      ]
    }
  ],
  "keywords": [
    "oss",
    "browser",
    "oss-browser",
    "oss browser",
    "aliyun",
    "aliyun oss"
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "ali-oss": "^6.17.1",
    "got": "^12.3.0",
    "raycast-toolkit": "^1.0.6"
  },
  "devDependencies": {
    "@types/ali-oss": "^6.16.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
