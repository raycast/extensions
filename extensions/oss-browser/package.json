        {
          "title": "oss-cn-guangzhou",
          "value": "oss-cn-guangzhou"
        },
        {
          "title": "oss-cn-chengdu",
          "value": "oss-cn-chengdu"
        },
        {
          "title": "oss-cn-hongkong",
          "value": "oss-cn-hongkong"
        },
        {
          "title": "oss-us-west-1",
          "value": "oss-us-west-1"
        },
        {
          "title": "oss-us-east-1",
          "value": "oss-us-east-1"
        },
        {
          "title": "oss-ap-northeast-1",
          "value": "oss-ap-northeast-1"
