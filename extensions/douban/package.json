{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "douban",
  "title": "Douban",
  "description": "Search Something on Douban",
  "icon": "douban.png",
  "author": "crazyczy",
  "categories": [
    "Web"
  ],
  "license": "MIT",
  "commands": [
