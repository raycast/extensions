{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "unogs",
  "title": "Unogs",
  "description": "A Raycast extension for Unogs",
  "icon": "command-icon.png",
  "author": "mohitkyadav",
  "version": "1.0.0",
  "categories": [
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Unogs",
      "subtitle": "Unogs",
      "description": "Search for your favorite Netflix title through Ungos db and find out their regions, audio and subtitle details.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.33.2",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "preferences": [
    {
      "name": "rapidApiKey",
      "title": "RapidAPI Key",
      "description": "API --> https://rapidapi.com/unogs/api/unogs/",
      "type": "password",
      "default": "",
      "required": true
    }
  ],
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
