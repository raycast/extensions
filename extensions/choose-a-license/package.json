{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "choose-a-license",
  "title": "Choose a License",
  "description": "Read, copy, and paste Markdown licenses from choosealicense.com",
  "icon": "icon.png",
  "author": "sandypockets",
  "contributors": ["Aayush9029"],
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Choose a License",
      "subtitle": "choosealicense.com",
      "description": "Quickly add a license to your open source project",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.32.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
