{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "twitter-video-downloader",
  "title": "Twitter/X Video Downloader",
  "description": "Download videos from Twitter/X with ease",
  "icon": "icon.png",
  "author": "kaanrkaraman",
  "categories": [
    "Media",
    "Fun",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Download Video",
      "description": "Download video from Twitter/X with various options",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.56.0",
    "axios": "^1.5.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "2.8.8",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
