{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "otp-manager",
  "title": "Otp/2fa Manager",
  "description": "Manage your two-factor authentication OTP codes",
  "icon": "command-icon.png",
  "author": "elalecs",
  "categories": [
    "Security"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View Otp/2fa Codes",
      "description": "Display your OTP/2FA codes",
      "mode": "view"
    },
    {
      "name": "add",
      "title": "Add Otp/2fa",
      "description": "Add a new OTP/2FA code manually",
      "mode": "view"
    },
    {
      "name": "import",
      "title": "Import Otp/2fa Codes",
      "description": "Import OTP/2FA codes from a JSON file",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.19.1",
    "thirty-two": "^1.0.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "eslint": "^8.57.1",
    "prettier": "^3.5.3",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
