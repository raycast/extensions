{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "json-inline",
  "title": "JSON inline",
  "description": "A extension to converts a JSON to inline",
  "icon": "command-icon.png",
  "author": "tiagobarros01",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "JSON inline",
      "description": "Open the extension",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}