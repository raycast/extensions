{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "css-gg",
  "title": "CSS.GG",
  "description": "Open-source CSS, SVG and Figma UI Icons Available in SVG Sprite, styled-components, NPM & API",
  "icon": "css-gg.png",
  "author": "astrit",
  "license": "MIT",
  "categories": [
    "Productivity",
    "Design Tools",
    "Developer Tools"
  ],
  "commands": [
    {
      "name": "css-gg",
      "title": "Search Icons",
      "subtitle": "css.gg",
      "description": "Search in Open-source CSS, SVG and Figma UI Icons",
      "mode": "view"
    },
    {
      "name": "css-gg-glyphs",
      "title": "Search Glyphs",
      "subtitle": "css.gg",
      "description": "Search in 6000+ glyphs from CSS.GG",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.8.0",
    "css.gg": "^2.1.4",
    "node-fetch": "^3.1.0",
    "react": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/node": "~20.3.1",
    "@types/react": "^18.2.13",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}