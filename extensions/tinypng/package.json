{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "tinypng",
  "title": "TinyPNG",
  "description": "Compress selected images with TinyPNG.",
  "icon": "tinypng-icon.png",
  "author": "kawamataryo",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "compressImages",
      "title": "Compress Images",
      "description": "Compress selected images with TinyPNG.",
      "mode": "no-view"
    }
  ],
  "preferences": [
    {
      "title": "API Key",
      "name": "apiKey",
      "description": "TinyPNG API Key.",
      "type": "password",
      "required": true
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.39.1",
    "node-fetch": "^3.2.10"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
