{
    "compilerOptions": {
        "target": "es2020",
        "lib": [
            "es2020"
        ],
        "jsx": "react-jsx",
        "module": "commonjs",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": [
        "src/**/*"
    ]
}