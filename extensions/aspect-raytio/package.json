{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "aspect-raytio",
  "title": "Aspect Raytio",
  "description": "Calculate easily the aspect ratio for different sizes based on a width or height.",
  "icon": "icon.png",
  "author": "ickas",
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Calculate Aspect Raytio",
      "description": "Calculate easily the aspect ratio for different sizes based on a width or height.",
      "keywords": [
        "aspect",
        "ratio",
        "calculate",
        "size",
        "width",
        "height"
      ],
      "mode": "view",
      "arguments": [
        {
          "name": "width",
          "placeholder": "Width",
          "type": "text"
        },
        {
          "name": "height",
          "placeholder": "Height",
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
