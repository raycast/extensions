{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "compress-pdf",
  "title": "Compress PDF",
  "description": "Compress PDF files",
  "icon": "command-icon.png",
  "author": "felixhaeberle",
  "categories": [
    "Applications",
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Compress PDF",
      "description": "Compress PDF files",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "api_key",
      "type": "password",
      "required": true,
      "title": "API key",
      "description": "Get your API key at cloudconvert.com",
      "placeholder": "Get your API key at cloudconvert.com"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.43.2",
    "cloudconvert": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
