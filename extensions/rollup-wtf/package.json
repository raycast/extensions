{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "rollup-wtf",
  "title": "Rollup.wtf",
  "description": "Raycast Version of Rollup.wtf",
  "icon": "extension-icon.png",
  "author": "Osprey",
  "license": "MIT",
  "categories": [
    "Finance",
    "Web"
  ],
  "commands": [
    {
      "name": "view-rollup-stats",
      "title": "View Rollup Stats",
      "description": "Compare stats between various L2s and Rollups",
      "mode": "view"
    },
    {
      "name": "view-blob-stats",
      "title": "View Blob Stats",
      "description": "Compare stats between various DA Layers",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.84.3",
    "@raycast/utils": "^1.17.0",
    "eventsource": "^2.0.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/eventsource": "^1.1.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
