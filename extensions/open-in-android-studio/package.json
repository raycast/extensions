{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "open-in-android-studio",
  "title": "Open in Android Studio",
  "description": "Open current Finder directory, selected folder in Android Studio, Add your favorite projects for quick access.",
  "icon": "icon.png",
  "author": "3llomi",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Open in Android Studio",
      "description": "Open the selected folder or current folder in Finder in Android Studio",
      "mode": "no-view"
    },
    {
      "name": "add-favorite-project",
      "title": "Add Android Project to Favorites",
      "description": "Add Android Project to Favorites",
      "mode": "view"
    },
    {
      "name": "show-favorite-projects",
      "title": "Show Favorite Android Projects",
      "description": "Show Favorite Android Studio Projects",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}