{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "utm-campaign-builder",
  "title": "UTM Campaign Builder",
  "description": "Create a campaign URL with UTM attributes",
  "icon": "icon.png",
  "author": "vimtor",
  "contributors": [
    "xmok"
  ],
  "categories": [
    "Communication",
    "Web",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Create Campaign URL",
      "description": "Create a campaign URL with UTM attributes",
      "keywords": [
        "utm"
      ],
      "mode": "view",
      "arguments": [
        {
          "name": "utm",
          "placeholder": "Existing UTM",
          "required": false,
          "type": "text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.79.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "^20.8.10",
    "@types/react": "^18.3.3",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react": "^18.2.0",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
