{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "obsidian-smart-capture",
  "title": "Obsidian Smart Capture",
  "description": "Smart capture anything into Obsidian",
  "icon": "command-icon.png",
  "author": "millin_gabani",
  "contributors": [
    "jimmy-b36"
  ],
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "capture",
      "title": "Obsidian Smart Capture",
      "description": "Capture whats on your screen into obsidian",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.1",
    "@raycast/utils": "^1.9.1",
    "node-fetch": "^3.3.2",
    "node-html-markdown": "^1.3.0",
    "node-html-parser": "^7.0.1",
    "yaml": "^2.3.1",
    "youtube-transcript": "^1.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "platforms": [
    "macOS"
  ]
}
