{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "whosampled",
  "title": "WhoSampled",
  "description": "Search WhoSampled.com from the currently playing song in Spotify, Apple Music, or search for a song and artists",
  "icon": "icon2.png",
  "author": "truex",
  "contributors": [
    "pernielsentikaer",
    "anirudh_subramanyam"
  ],
  "categories": [
    "Web",
    "Fun",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "whosampled",
      "title": "WhoSampled",
      "description": "Search WhoSampled.com from the currently playing song in Spotify",
      "mode": "no-view"
    },
    {
      "name": "appleMusic",
      "title": "WhoSampled Apple Music",
      "description": "Search WhoSampled.com from the currently playing song in Apple Music",
      "mode": "no-view"
    },
    {
      "name": "search",
      "title": "WhoSampled Search",
      "description": "Search WhoSampled.com for a song and artists",
      "mode": "no-view",
      "arguments": [
        {
          "name": "song",
          "placeholder": "Song Name",
          "description": "The song to search for",
          "type": "text",
          "required": true
        },
        {
          "name": "artists",
          "placeholder": "Artists",
          "description": "The artist of the song",
          "type": "text",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.46.0",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
