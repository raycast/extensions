{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "roll-d20",
  "title": "Roll D20",
  "description": "Rolls a d20, in other words, displays a random number between 1 and 20.",
  "icon": "command-icon.png",
  "author": "kayla",
  "categories": [
    "Fun"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Roll D20",
      "description": "Shows the result of your d20 roll.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.42.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
