{
	"$schema": "https://json.schemastore.org/tsconfig",
	"include": ["src/**/*", "raycast-env.d.ts"],
	"compilerOptions": {
		"lib": ["ES2023"],
		"module": "ESNext",
		"target": "ES2023",
		"strict": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"moduleResolution": "bundler",
		"resolveJsonModule": true
	}
}
