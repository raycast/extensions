{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "crawldoc",
  "title": "CrawlDoc - Documentations Search Engine",
  "description": "Allow searching through multiple documentations platforms",
  "icon": "icon.png",
  "author": "johan-perso",
  "categories": [
    "Developer Tools",
    "Documentation",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentations",
      "description": "Search for specific queries on multiples docs",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.4",
    "@raycast/utils": "^1.4.8"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}