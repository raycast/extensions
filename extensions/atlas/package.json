{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "atlas",
  "title": "Atlas",
  "description": "Internal tools for Atlas",
  "icon": "command-icon.png",
  "author": "yuanming",
  "license": "MIT",
  "commands": [
    {
      "name": "mtdRevenue",
      "title": "MTD Atlas Revenue",
      "description": "Quick check on Atlas MTD revenue",
      "mode": "no-view",
      "interval": "1h"
    },
    {
      "name": "topTenRevenueMerchants",
      "title": "MTD Top 10 Merchants by Atlas Revenue",
      "description": "MTD Top 10 Merchants by Atlas Revenue",
      "mode": "view"
    },
    {
      "name": "topTenAdyenGmvMerchants",
      "title": "MTD Top 10 Merchants by Adyen GMV",
      "description": "MTD Top 10 Merchants by Adyen GMV",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@raycast/api": "^1.63.0",
    "@raycast/utils": "^1.10.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/pg": "^8.10.9",
    "@types/react": "18.2.27",
    "@types/ssh2": "^1.11.18",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
