{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "eblu",
  "title": "Eblu",
  "description": "EBlu - Easy bluetooth management for your Mac",
  "icon": "extension-icon.png",
  "author": "sadesguy",
  "categories": [
    "System",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "eb",
      "title": "Ebluetooth",
      "description": "Easy Bluetooth device management with search and quick connect features",
      "mode": "view",
      "keywords": [
        "bluetooth",
        "connect",
        "devices",
        "headphones",
        "speakers"
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.91.2",
    "@raycast/utils": "^1.17.0"
  },
  "preferences": [
    {
      "name": "maxDevices",
      "title": "Maximum Devices",
      "description": "Number of devices to show when not searching",
      "type": "textfield",
      "required": false,
      "default": "5",
      "minimum": "1",
      "maximum": "20"
    }
  ],
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.11",
    "@types/node": "20.8.10",
    "@types/react": "18.3.3",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "files": [
    "assets"
  ]
}
