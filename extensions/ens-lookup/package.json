{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "ens-lookup",
  "title": "ENS Lookup",
  "description": "",
  "icon": "command-icon.png",
  "author": "mal1",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "ENS Lookup",
      "subtitle": "Lookup an address associated with an ENS",
      "description": "",
      "mode": "no-view",
      "arguments": [
        {
          "name": "name",
          "placeholder": "name.eth",
          "type": "text",
          "required": true
        }
      ],
      "preferences": [
        {
          "name": "foundryLocation",
          "description": "The location of your Foundry installation",
          "type": "textfield",
          "required": true,
          "default": "~/.foundry"
        },
        {
          "name": "rpcUrl",
          "description": "An Ethereum RPC URL",
          "type": "textfield",
          "required": true,
          "default": "https://web3-trial.cloudflare-eth.com/v1/mainnet"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
