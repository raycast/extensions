{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "lucide-icons",
  "title": "Lucide Icons Search",
  "description": "View the Lucide icon set.",
  "icon": "command-icon.png",
  "author": "basil",
  "categories": [
    "Design Tools",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Icons",
      "subtitle": "Lucide",
      "description": "Grid view of Lucide icons",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.1"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "lucide-static": "^0.84.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "prebuild": "node prebuild.js",
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
