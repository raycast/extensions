{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "dia",
  "title": "Dia",
  "description": "Extension for The Browser Company's new browser, currently in alpha.",
  "icon": "appIcon.png",
  "author": "tleo19",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "new-window",
      "title": "New Window",
      "subtitle": "Dia",
      "description": "Create a new window with an optional profile.",
      "mode": "no-view",
      "arguments": [
        {
          "name": "profile",
          "placeholder": "Profile",
          "type": "text",
          "required": false
        }
      ]
    },
    {
      "name": "new-incognito-window",
      "title": "New Incognito Window",
      "subtitle": "Dia",
      "description": "Create a new incognito window.",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.96.0",
    "@raycast/utils": "^1.17.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
