{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "send-to-flomo",
  "title": "Send to flomo",
  "description": "Send your MEMOs to flomo",
  "icon": "command-icon.png",
  "author": "ahonn",
  "license": "MIT",
  "contributors": [
    "fankaidev",
    "pernielsentikaer"
  ],
  "commands": [
    {
      "name": "send-to-flomo",
      "title": "Send MEMO",
      "subtitle": "flomo",
      "description": "Send your thoughts to flomo",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "api",
      "title": "MEMO API URL",
      "description": "Your MEMO API URL",
      "type": "textfield",
      "required": true
    },
    {
      "name": "defaultTags",
      "title": "Default tags",
      "description": "The default tags that comes with each MEMO â€” separated by space",
      "type": "textfield",
      "default": "#raycast",
      "required": false
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.37.0",
    "axios": "^0.24.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
