{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "1loc",
  "title": "1 LOC - JavaScript Utilities in single line of code",
  "description": "Browse JavaScript Utilities from 1loc.dev",
  "icon": "command-icon.png",
  "author": "kud",
  "categories": [
    "Developer Tools"
  ],
  "contributors": [
    "sxn"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search JavaScript Utilities",
      "subtitle": "1 LOC",
      "description": "Browse JavaScript Utilities from 1loc.dev",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "type": "password",
      "required": false,
      "title": "Personal Access Token",
      "description": "https://docs.github.com/en/authentication/keeping-your-account-and-data-secure/creating-a-personal-access-token"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.31.0",
    "axios": "0.25.0",
    "change-case": "4.1.2",
    "gray-matter": "4.0.3",
    "raycast-toolkit": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
