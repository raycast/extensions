{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "1loc",
  "title": "1 LOC - JavaScript Utilities in Single Line of Code",
  "description": "Browse JavaScript Utilities from 1loc.dev",
  "icon": "command-icon.png",
  "author": "kud",
  "categories": [
    "Developer Tools"
  ],
  "keywords": [
    "1loc",
    "code",
    "JavaScript",
    "utilities"
  ],
  "contributors": [
    "kgrhartlage",
    "sxn"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search JavaScript Utilities",
      "subtitle": "1 LOC",
      "description": "Browse JavaScript utilities from 1loc.dev",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "personalAccessToken",
      "type": "password",
      "required": true,
      "title": "GitHub Personal Access Token",
      "description": "Required to authenticate with GitHub. See the official documentation for more information."
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.51.2",
    "axios": "^1.4.0",
    "change-case": "^4.1.2",
    "gray-matter": "^4.0.3",
    "raycast-toolkit": "^1.0.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "react-devtools": "^4.27.7",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint",
    "fix-lint": "ray lint --fix"
  }
}
