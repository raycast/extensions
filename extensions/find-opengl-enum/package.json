{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "find-opengl-enum",
  "title": "Find OpenGL Enum",
  "description": "convert any decimal/hexadecimal code into actual OpenGL enum",
  "icon": "command-icon.png",
  "author": "visual_eugen",
  "contributors": [
    "litomore"
  ],
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Find OpenGL Enum",
      "description": "convert any decimal/hexadecimal code into actual OpenGL enum",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.100.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "24.0.3",
    "@types/react": "19.1.8",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
