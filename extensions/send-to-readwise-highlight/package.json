{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "send-to-readwise-highlight",
  "title": "Send to Readwise Highlight",
  "description": "Input note to readwise directly.",
  "icon": "readwise-icon.png",
  "author": "Lyson",
  "categories": [
    "Applications",
    "Data",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Send to Readwise",
      "subtitle": "Daily Fleeting Note",
      "description": "A useful tool to input text to readwise.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.1",
    "@raycast/utils": "^1.10.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  },
  "preferences": [
    {
      "name": "apiKey",
      "title": "API Key",
      "description": "请输入你的Readwise API Key",
      "type": "password",
      "required": true
    }
  ]
}
