{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "laby-net",
  "title": "Laby.net",
  "description": "Search and view Minecraft user profiles and textures like skins and capes with data from laby.net.",
  "icon": "command-icon.png",
  "author": "niklaswa",
  "categories": [
    "Data",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "profile-search",
      "title": "Search Profiles",
      "subtitle": "laby.net",
      "description": "Searches Minecraft user profile via username.",
      "mode": "view"
    },
    {
      "name": "texture-search",
      "title": "Search Textures",
      "subtitle": "laby.net",
      "description": "Searches Minecraft skins & capes",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.84.2",
    "axios": "^0.27.2"
  },
  "devDependencies": {
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
