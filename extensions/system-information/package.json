{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "system-information",
  "title": "System Information",
  "description": "Gives you quick access to your system information so that you never have to open System Settings.",
  "icon": "command-icon.png",
  "author": "Vaibhav-Satishkumar",
  "categories": [
    "Developer Tools",
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "View System Information",
      "description": "View your system information",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.57.2",
    "macos-release": "^3.2.0",
    "macos-version": "^6.0.0",
    "systeminformation": "^5.20.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
