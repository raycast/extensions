{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "xcodes",
  "title": "Xcodes",
  "description": "Manage several versions of Xcode",
  "icon": "command-icon.png",
  "author": "pacadil",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "list",
      "title": "Xcodes List",
      "description": "List all available Xcode versions",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.0",
    "@raycast/utils": "^1.9.1",
    "adm-zip": "^0.5.10",
    "download": "^8.0.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/adm-zip": "^0.5.0",
    "@types/download": "^8.0.1",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
