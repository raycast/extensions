{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "glyph-search",
  "title": "Glyph Search",
  "description": "Like the emoji picker, but for unicode glyphs",
  "icon": "icon.png",
  "author": "gbgk",
  "license": "MIT",
  "categories": ["Productivity"],
  "commands": [
    {
      "name": "index",
      "title": "Search Glyphs",
      "description": "Search Unicode glyphs",
      "mode": "view",
      "subtitle": "Glyphs"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.47.3"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}