{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-calendar",
  "title": "Google Calendar",
  "description": "View and interact with your Google Calendar events.",
  "icon": "google-calendar-logo.png",
  "author": "Wasson",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "My Schedule",
      "description": "View and interact with Google Calendar events for the upcoming week.",
      "icon": "google-calendar-logo.png",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.52.1",
    "@raycast/utils": "^1.7.0",
    "moment": "^2.29.4",
    "node-fetch": "^3.3.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
