{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-calendar",
  "title": "Google Calendar",
  "description": "Create & view Google Calendar events",
  "icon": "google-calendar.png",
  "author": "thebigsweatshirt",
  "categories": [
    "Productivity"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "createEvent",
      "title": "Create Event",
      "description": "Create a new event in Google Calendar",
      "mode": "view"
    },
    {
      "name": "viewTodayEvents",
      "title": "View Today's Events",
      "description": "See your events for today",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.65.1",
    "@raycast/utils": "^1.10.1",
    "colornames": "^1.1.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/node-fetch": "^2.6.11",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
