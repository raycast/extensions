{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "google-calendar",
  "title": "Google Calendar",
  "description": "Show Google calendar events in raycast",
  "icon": "command-icon.png",
  "author": "d4vz",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "daily",
      "title": "Daily Calendar Events",
      "description": "Show daily calendar events in Google Calendar",
      "mode": "view",
      "icon": "google-calendar.png"
    },
    {
      "name": "index",
      "title": "Calendar Events",
      "description": "Show calendar events in Google Calendar",
      "mode": "view",
      "icon": "google-calendar.png"
    },
    {
      "name": "logout",
      "title": "Logout",
      "description": "Logout in Google Calendar",
      "mode": "view",
      "icon": "google-calendar.png"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1",
    "@raycast/utils": "^1.15.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
