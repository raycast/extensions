{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "go-package-search",
  "title": "Go Package Search",
  "description": "Quickly search packages on https://pkg.go.dev",
  "icon": "command-icon.png",
  "author": "bstncartwright",
  "contributors": [
    "k8scat",
    "BalliAsghar"
  ],
  "categories": [
    "Developer Tools",
    "Productivity",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Packages",
      "subtitle": "Go",
      "description": "Search for packages on pkg.go.dev.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "node-fetch": "^3.0.0",
    "node-html-parser": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
