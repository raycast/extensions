{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "discord",
    "title": "Discord",
    "description": "Send message to a specific discord channel using webhook.",
    "icon": "command-icon.png",
    "author": "Aayush9029",
    "categories": [
        "Communication"
    ],
    "license": "MIT",
    "commands": [{
            "name": "sendMessage",
            "title": "Send Message",
            "subtitle": "Discord",
            "description": "Send a message to a specific channel using Discord webhook.",
            "mode": "view"
        },
        {
            "name": "addWebhook",
            "title": "Add Webhook",
            "subtitle": "Discord",
            "description": "Add discord webhooks.",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.32.1",
        "discord.js": "^13.6.0"
    },
    "devDependencies": {
        "@types/node": "~16.10.0",
        "@types/react": "^17.0.28",
        "@typescript-eslint/eslint-plugin": "^5.0.0",
        "@typescript-eslint/parser": "^5.0.0",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "prettier": "^2.5.1",
        "react-devtools": "^4.19.2",
        "typescript": "^4.4.3"
    },
    "scripts": {
        "build": "ray build -e dist",
        "dev": "ray develop",
        "fix-lint": "ray lint --fix",
        "lint": "ray lint"
    }
}