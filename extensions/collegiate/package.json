{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "collegiate",
  "title": "Collegiate",
  "description": "Access Collegiate Connect assignments.",
  "icon": "command-icon.png",
  "author": "LukeKosner",
  "license": "MIT",
  "commands": [
    {
      "name": "assignments",
      "title": "Assignments",
      "description": "View your assignments.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "calendarUrl",
      "description": "The URL of your Collegiate Connect calendar.",
      "type": "password",
      "required": true,
      "title": "Calendar URL"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.58.1",
    "@raycast/utils": "^1.10.0",
    "@types/he": "^1.2.1",
    "he": "^1.2.0",
    "node-ical": "^0.16.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
