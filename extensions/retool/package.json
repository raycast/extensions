{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "retool-documentation",
  "title": "Retool Documentation",
  "version": "1.0.0",
  "description": "Search the Retool documentation from Raycast",
  "icon": "icon.png",
  "author": "andre347",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "Retool",
      "description": "Search the Retool documentation from Raycast",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.40.1",
    "algoliasearch": "^4.14.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
