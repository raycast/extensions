{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "title",
  "title": "Title",
  "description": "Wrapper for Vercels title library",
  "icon": "command-icon.png",
  "author": "mrohmer",
  "categories": [
    "Productivity",
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Title",
      "description": "Capitalizes Your Titles",
      "mode": "no-view",
      "arguments": [
        {
          "name": "text",
          "type": "text",
          "required": false,
          "placeholder": "Text"
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.9",
    "title": "^3.5.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "1.0.5",
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@types/title": "^3.4.1",
    "eslint": "^7.32.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
