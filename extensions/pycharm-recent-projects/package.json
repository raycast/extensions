{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pycharm-recent-projects",
  "title": "PyCharm Recent Projects",
  "description": "Manipulate recent projects for standalone installation of PyCharm",
  "icon": "PyCharm_icon.png",
  "author": "remarkov",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "recent-projects",
      "title": "Recent projects",
      "description": "Search and manipulate recent projects in PyCharm",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.48.4",
    "fast-xml-parser": "^4.1.2",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
