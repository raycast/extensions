{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "pycharm-recent-projects",
  "title": "PyCharm Recent Projects",
  "description": "Manipulate recent projects for standalone installation of PyCharm",
  "icon": "PyCharm_icon.png",
  "author": "remarkov",
  "categories": [
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "recent-projects",
      "title": "Recent Projects",
      "description": "Search and manipulate recent projects in PyCharm",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.61.1",
    "fast-xml-parser": "^4.1.2",
    "run-applescript": "^6.1.0"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "18.8.4",
    "@types/react": "18.2.36",
    "eslint": "^8.53.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
