{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "cyberduck",
  "title": "Cyberduck",
  "description": "Quickly open your cyberduck connections",
  "icon": "icon.png",
  "author": "scottaglia",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "List Connections",
      "subtitle": "Cyberduck",
      "description": "List and open Cyberduck connections",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "fast-xml-parser": "^4.0.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
