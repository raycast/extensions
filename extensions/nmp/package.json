{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "nmp",
  "title": "Nordic Marketplaces - Raycast Extension",
  "description": "Helps you to navigate the internal tools of Nordic Marketplaces in all environments.",
  "icon": "schibsted.png",
  "author": "tom-fagerland_sch",
  "license": "MIT",
  "owners": ["nordic-marketplaces"],
  "commands": [
    {
      "name": "index",
      "title": "Find NMP tool",
      "description": "Find the NMP tool you need",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}