{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "go-links",
    "title": "Go Links",
    "version": "0.0.1",
    "description": "Create, manage, search, and quickly open your go links.",
    "icon": "goLinks.png",
    "author": "bradgarropy",
    "categories": [
        "Web"
    ],
    "license": "MIT",
    "commands": [
        {
            "name": "create",
            "title": "Create Link",
            "description": "Create a go link.",
            "mode": "view"
        },
        {
            "name": "search",
            "title": "Search Links",
            "description": "Search go links.",
            "mode": "view"
        }
    ],
    "dependencies": {
        "@raycast/api": "^1.83.0",
        "@raycast/utils": "^1.17.0"
    },
    "devDependencies": {
        "@bradgarropy/eslint-config": "^1.2.3",
        "@bradgarropy/eslint-config-react": "^1.0.2",
        "@bradgarropy/eslint-config-typescript": "^1.0.3",
        "@raycast/eslint-config": "^1.0.11",
        "@types/node": "20.8.10",
        "@types/react": "18.3.3",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-simple-import-sort": "^12.0.0",
        "prettier": "^3.3.3",
        "typescript": "^5.4.5"
    },
    "scripts": {
        "dev": "ray develop",
        "build": "ray build -e dist -o dist",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "lint:raycast": "ray lint",
        "lint:raycast:fix": "ray lint --fix",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
        "publish": "npx @raycast/api@latest publish"
    }
}
