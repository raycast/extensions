{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "github-cli-manual",
  "title": "GitHub CLI Manual",
  "description": "Raycast extension to search GitHub CLI Manual.",
  "icon": "icon.png",
  "author": "demartini",
  "categories": [
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Documentation",
      "subtitle": "GitHub CLI Manual",
      "description": "Search GitHub CLI Manual.",
      "icon": "icon.png",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.44.0"
  },
  "devDependencies": {
    "@types/node": "18.11.12",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
