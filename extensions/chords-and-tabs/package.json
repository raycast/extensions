{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "chords-and-tabs",
  "title": "Chords and Tabs",
  "description": "Find chords and tabs for your favorite songs and musical pieces!",
  "icon": "logo.png",
  "author": "BossElijah",
  "categories": [
    "Other",
    "Media",
    "Documentation"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "guitaretab",
      "title": "Search Music from Guitaretab",
      "description": "Search from www.guitaretab.com",
      "mode": "view"
    },
    {
      "name": "songsterr",
      "title": "Search Music from Songsterr",
      "description": "Search guitar tabs from www.songsterr.com",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.74.1",
    "@types/got": "^9.6.12",
    "cheerio": "^1.0.0-rc.12",
    "got": "^14.2.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
