{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "mousehunt-helper",
  "title": "MouseHunt Helper",
  "description": "Find the best attraction rates for different mice",
  "icon": "mousehunt-icon.png",
  "author": "chesterhow",
  "categories": [
    "Fun",
    "Data",
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Attraction Rates",
      "description": "Find the best attraction rates for different mice",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.45.3",
    "@raycast/utils": "^1.4.15"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
