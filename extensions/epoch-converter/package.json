{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "epoch-converter",
  "title": "Epoch Converter",
  "description": "Convert an epoch to a human-readable date.",
  "icon": "command-icon.png",
  "author": "nick_lansing",
  "categories": [
    "Productivity",
    "Data"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Convert Epoch",
      "description": "Convert an epoch to a human-readable date.",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}