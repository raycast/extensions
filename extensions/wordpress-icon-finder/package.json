{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "wordpress-icon-finder",
  "title": "WordPress Icon Finder",
  "description": "It helps you search icons from the WordPress Icons Library.",
  "icon": "icon.png",
  "author": "javierarce",
  "categories": [
    "Design Tools",
    "Web"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Icons",
      "description": "Search WordPress Icons",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.72.1"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
