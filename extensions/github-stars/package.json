{
  "name": "search-github-stars",
  "title": "GitHub Stars",
  "description": "Display and filter your recent GitHub stars",
  "icon": "icon.png",
  "author": "mrmartineau",
  "keywords": [
    "stars"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Starred Repositories",
      "subtitle": "GitHub",
      "description": "Display and filter your recent GitHub stars",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "githubUsername",
      "type": "textfield",
      "title": "GitHub username",
      "description": "Please enter the GitHub username",
      "required": true
    },
    {
      "name": "resultsCount",
      "type": "textfield",
      "title": "Results count",
      "description": "The number of results to display",
      "required": false,
      "default": "50"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.25.0",
    "fetch-readme": "^1.0.0",
    "node-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/parse-github-url": "^1.0.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
