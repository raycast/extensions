{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "instagram-media-downloader",
  "title": "Instagram Media Downloader",
  "description": "Download media from Instagram with ease",
  "icon": "extension-icon.png",
  "author": "ridemountainpig",
  "categories": [
    "Applications",
    "Media"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "download-instagram-media",
      "title": "Download Instagram Media",
      "description": "Download Instagram media with URL",
      "mode": "no-view",
      "arguments": [
        {
          "name": "instagramUrl",
          "description": "Instagram URL",
          "type": "text",
          "placeholder": "Instagram URL",
          "required": true
        }
      ],
      "preferences": [
        {
          "name": "mediaDownloadPath",
          "title": "Media Download Path",
          "description": "Path to save downloaded media",
          "type": "directory",
          "required": false
        }
      ]
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.94.3",
    "@raycast/utils": "^1.17.0",
    "axios": "^1.8.4"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^2.0.4",
    "@types/node": "22.13.10",
    "@types/react": "19.0.10",
    "eslint": "^9.22.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "scripts": {
    "build": "ray build",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "prepublishOnly": "echo \"\\n\\nIt seems like you are trying to publish the Raycast extension to npm.\\n\\nIf you did intend to publish it to npm, remove the \\`prepublishOnly\\` script and rerun \\`npm publish\\` again.\\nIf you wanted to publish it to the Raycast Store instead, use \\`npm run publish\\` instead.\\n\\n\" && exit 1",
    "publish": "npx @raycast/api@latest publish"
  }
}
