{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "save-to-cubox",
  "title": "Save to Cubox",
  "description": "Save url or memo to your cubox",
  "icon": "command-icon.png",
  "author": "xyxc0673",
  "categories": [
    "Other"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "save-to-cubox",
      "title": "Save to Cubox",
      "description": "Save url or memo to your cubox",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "api",
      "title": "Cubox API",
      "description": "Example: https://cubox.pro/c/api/save/xxxx",
      "type": "textfield",
      "required": true,
      "placeholder": "Example: https://cubox.pro/c/api/save/xxxx"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.62.2",
    "@raycast/utils": "^1.10.1",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
