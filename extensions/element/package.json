{
    "$schema": "https://www.raycast.com/schemas/extension.json",
    "name": "element",
    "title": "Element",
    "description": "Show information about an element of the periodic table.",
    "icon": "command-icon.png",
    "author": "Aayush9029",
    "categories": [
        "Other",
        "Data"
